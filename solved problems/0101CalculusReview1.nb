    \(\(Print[a2, "\<  <  \>", c2\ "\<  <  \>", b2];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 6.  (Extreme Value Theorem for a Continuous Function).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is a continuous function on the interval  ",
  Cell[BoxData[
      \(\([a, b]\)\)]],
  ".  Then there exists a lower bound  ",
  Cell[BoxData[
      \(M\_1\)]],
  "  and an upper bound  ",
  Cell[BoxData[
      \(M\_2\)]],
  "  and two numbers  ",
  Cell[BoxData[
      \(x\_1\)]],
  "  and  ",
  Cell[BoxData[
      \(x\_2\)]],
  "  in  ",
  Cell[BoxData[
      \(\([a, b]\)\)]],
  "  such that   ",
  Cell[BoxData[
      RowBox[{\(M\_1\), " ", "=", " ", 
        RowBox[{
          RowBox[{
            RowBox[{"f", 
              RowBox[{"(", 
                StyleBox[
                  RowBox[{"x", 
                    StyleBox["1",
                      FontSize->10,
                      FontVariations->{"CompatibilityType"->"Subscript"}]}]], 
                ")"}]}], " ", "\[LessEqual]", " ", \(f \((x)\)\), " ", 
            "\[LessEqual]", " ", 
            RowBox[{"f", 
              RowBox[{"(", 
                StyleBox[
                  RowBox[{"x", 
                    StyleBox["2",
                      FontSize->10,
                      FontVariations->{"CompatibilityType"->"Subscript"}]}]], 
                ")"}]}]}], " ", "=", " ", \(M\_2\)}]}]]],
  ",    whenever  ",
  Cell[BoxData[
      StyleBox[\(a \[LessEqual] x \[LessEqual] b\),
        FontWeight->"Plain",
        FontColor->GrayLevel[0]]], "Input"],
  "."
}], "Text",
  CellTags->"Extreme Value Theorem for a Continuous Function"],

Cell[TextData[{
  StyleBox["Example 14.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      \(f \((x)\) = 35 + 59.5\ x - 66.5\ x\^2 + 15\ x\^3\)]],
  ".  Find the minimum and maximum of  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  over the interval  ",
  Cell[BoxData[
      \(\([0, 3]\)\)]],
  ".  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 14.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  StyleBox["Method i.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 1]],
  "  Use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s built in function for finding a minimum value.  Compute approximations \
for the minimum and maximum of  f(x)  over the interval  [0, 3]."
}], "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(Clear[dot, f, x];\)\ \ \), "\n", 
    \(\(f[x_]\  = \ 35 + 59.5\ x - 66.5\ x\^2 + 15\ x\^3;\)\ \ \), "\n", 
    \(\(minset\  = \ FindMinimum[f[x], {x, 2.5}];\)\ \ \), "\n", 
    \(\(maxset\  = \ FindMinimum[\(-f[x]\), {x, 0.5}];\)\ \ \), "\n", 
    \(\(maxset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\  \
= \ \(-\ maxset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\);\
\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(dot1\  = \ 
        Graphics[{Green, PointSize[0.025], 
            Point[{minset\_\(\(\[LeftDoubleBracket]\)\(2, 1, 2\)\(\
\[RightDoubleBracket]\)\), 
                minset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)}]}];\)\ \ \), "\n", 
    \(\(dot2\  = \ 
        Graphics[{Blue, PointSize[0.025], 
            Point[{maxset\_\(\(\[LeftDoubleBracket]\)\(2, 1, 2\)\(\
\[RightDoubleBracket]\)\), 
                maxset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)}]}];\)\ \ \), "\n", 
    \(\(grf = 
        Plot[f[x], {x, 0, 3}, PlotStyle \[Rule] Magenta, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(Show[grf, dot1, dot2, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] \*"\"\<     y = 35+59.5 x-66.5 \!\(x\^2\)+15 \
\!\(x\^3\)\>\"", DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<For the minimum we have:  \>", minset];\)\ \ \), "\n", 
    \(\(Print["\<For the maximum we have:  \>", maxset];\)\ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["Method ii.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 1]],
  "  Since the given function  f(x)  is also differentiable,  locate the \
critical values where  f '(x) = 0."
}], "Text",
  Evaluatable->False],

Cell[BoxData[{
    RowBox[{
      RowBox[{
        RowBox[{"solset", "=", 
          RowBox[{"Solve", "[", 
            RowBox[{
              RowBox[{
                RowBox[{
                  SuperscriptBox["f", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}], "\[Equal]", 
                "0"}], ",", "x"}], "]"}]}], ";"}], "  "}], "\n", 
    RowBox[{\(xmin\  = \ 
          solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\);\), "  "}], "\n", 
    RowBox[{\(ymin\  = \ f[xmin];\), "  "}], "\n", 
    RowBox[{\(xmax\  = \ 
          solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\);\), "  "}], "\n", 
    RowBox[{\(ymax\  = \ f[xmax];\), "  "}], "\[IndentingNewLine]", 
    RowBox[{\(Needs["\<Graphics`Colors`\>"];\), "  "}], "\[IndentingNewLine]",
   
    RowBox[{\(dot1\  = \ 
          Graphics[{Green, PointSize[0.025], 
              Point[{minset\_\(\(\[LeftDoubleBracket]\)\(2, 1, 2\)\(\
\[RightDoubleBracket]\)\), 
                  minset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\
\[RightDoubleBracket]\)\)}]}];\), "  "}], "\n", 
    RowBox[{\(dot2\  = \ 
          Graphics[{Blue, PointSize[0.025], 
              Point[{maxset\_\(\(\[LeftDoubleBracket]\)\(2, 1, 2\)\(\
\[RightDoubleBracket]\)\), 
                  maxset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\
\[RightDoubleBracket]\)\)}]}];\), "  "}], "\[IndentingNewLine]", 
    RowBox[{\(lmin = 
          Graphics[{Green, 
              Line[{{xmin + 0.5, ymin}, {xmin - 0.5, ymin}}]}];\), 
      "  "}], "\n", 
    RowBox[{\(lmax = 
          Graphics[{Blue, Line[{{xmax + 0.5, ymax}, {xmax - 0.5, ymax}}]}];\),
       "  "}], "\n", 
    RowBox[{\(Show[grf, dot1, dot2, lmin, lmax, 
          Prolog \[Rule] {PointSize[0.02]}, 
          Ticks \[Rule] {Range[0, 3, 0.5], Range[0, 60, 10]}, 
          AxesLabel \[Rule] {x, y}, \n\t
          PlotLabel \[Rule] \*"\"\<     y = 35+59.5 x-66.5 \!\(x\^2\)+15 \
\!\(x\^3\)\>\"", DisplayFunction \[Rule] $DisplayFunction];\), "  "}], "\n", 
    RowBox[{\(Print["\< f[x] = \>", f[x]\ ];\), "  "}], "\n", 
    RowBox[{\(Print["\<f'[x] = \>", \(f'\)[x]\ ];\), "  "}], "\n", 
    RowBox[{\(Print["\<Solve   f'[x] = 0\>"];\), "  "}], "\n", 
    RowBox[{\(Print[solset];\), "  "}], "\n", 
    RowBox[{\(Print["\<\n\>", "\<The maximum value for the function \
f(x):\>"];\), "  "}], "\n", 
    RowBox[{\(Print["\<f(x) = \>", f[x]];\), "  "}], "\n", 
    RowBox[{\(Print[\*"\"\<\!\(x\_1\) = \>\"", xmax];\), "  "}], "\n", 
    RowBox[{\(Print[\*"\"\<\!\(max\+\(0 \[LessEqual] x \[LessEqual] 3\)\) \
\>\"", "\<f(x) = f(\>", xmax, \*"\"\<) = \!\(y\_1\) = \>\"", ymax];\), 
      "   "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<\n\>", "\<The minimum value for the function \
f(x):\>"];\), "  "}], "\n", 
    RowBox[{\(Print["\<f(x) = \>", f[x]];\), "  "}], "\n", 
    RowBox[{\(Print[\*"\"\<\!\(x\_2\) = \>\"", xmin];\), "  "}], "\n", 
    RowBox[{\(Print[\*"\"\<\!\(min\+\(0 \[LessEqual] x \[LessEqual] 3\)\) \
\>\"", "\<f(x) = f(\>", xmin, \*"\"\<) = \!\(y\_2\) = \>\"", ymin];\), 
      "  "}]}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 7.  (Differentiable Function Implies Continuous \
Function).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\nIf  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is differentiable at  ",
  Cell[BoxData[
      \(x = x\_0\)]],
  ",  then  ",
  Cell[BoxData[
