  Cell[BoxData[
      \(f' \((x\_0)\)\  = \ \ \(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\
\ \(f \((x)\)\  - \ f \((x\_0)\)\)\/\(x\  - \ x\_0\)\)]],
  ",  \nor\n\t",
  Cell[BoxData[
      \(f' \((x\_0)\) = \ \(\(\ \)\(lim\)\(\ \)\)\+\(\[CapitalDelta]x\  \
\[Rule] \ 0\)\ \(f \((x\_0\  + \ \[CapitalDelta]x)\)\  - \ f \((x\_0)\)\)\/\
\[CapitalDelta]x\)]],
