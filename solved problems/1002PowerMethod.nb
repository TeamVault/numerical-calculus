Cell[BoxData[
    \(\(\(InvPowerMeth[A0_, \[Alpha]_, X0_, \[Epsilon]_, 
          max1_] := \[IndentingNewLine]Module[{A, X\  = \ N[X0], Y, c1, 
            count\  = \ 0, \[CapitalDelta]\[Lambda], \[CapitalDelta]V, 
            err\  = \ 1, \[Lambda]\  = \ 0, n\  = \ Length[A0], state, done, 
            iterating}, \[IndentingNewLine]maxsize[W_] := 
            Module[{w\  = \ Sort[W], msize}, \[IndentingNewLine]If[\ 
                Abs[w\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\[RightDoubleBracket]\
\)\)] \[GreaterEqual] 
                  Abs[w\_\(\(\[LeftDoubleBracket]\)\(1\)\(\
\[RightDoubleBracket]\)\)], \[IndentingNewLine]msize\  = \ 
                  w\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\[RightDoubleBracket]\)\
\), \[IndentingNewLine]msize\  = \ 
                  w\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\
\)]; \ \[IndentingNewLine]Return[msize];\ ]; \ \[IndentingNewLine]norm[
              V_] := \@\(V . V\); \ \[IndentingNewLine]A\  = \ 
            A0\  - \ \[Alpha]\ \ IdentityMatrix[
                  n]; \ \[IndentingNewLine]lud = 
            LUDecomposition[A]; \ \[IndentingNewLine]state\  = \ 
            iterating; \[IndentingNewLine]While[\ 
            And[\ count \[LessEqual] max1, \ 
              state \[Equal] iterating\ ], \[IndentingNewLine]Y = 
              LUBackSubstitution[lud, X]; \ \[IndentingNewLine]c1\  = \ 
              maxsize[Y]; \ \[IndentingNewLine]\[CapitalDelta]\[Lambda]\  = \ 
              Abs[\[Lambda] - c1]; \ \[IndentingNewLine]Y\  = \ 
              Y\/c1; \ \[IndentingNewLine]\[CapitalDelta]V\  = \ 
              norm[X - Y]; \ \[IndentingNewLine]err\  = \ 
              Max[{\[CapitalDelta]\[Lambda], \[CapitalDelta]V}]; \ \
\[IndentingNewLine]X\  = \ Y; \ \[IndentingNewLine]If[\ err > \[Epsilon], \ 
              state = iterating, \ 
              state\  = \ done]; \ \[IndentingNewLine]Print[
              NumberForm[\ \[Alpha] + 1\/c1, 12], "\<,  \>", 
              PaddedForm[
                X, {12, 12}]]; \ \ \[IndentingNewLine]\[Lambda]\  = \ 
              c1; \ \[IndentingNewLine]count\  = \ 
              count + 1;\ ]; \ \[IndentingNewLine]\[Lambda]\  = \ \[Alpha] + 
