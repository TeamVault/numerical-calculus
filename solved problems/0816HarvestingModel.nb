  Cell[BoxData[
      \(x \((0)\) = 3\/5, 2\/5, 1\/5\)]],
  "   must be computed over smaller intervals, \nfor instance the intervals   \
",
  Cell[BoxData[
      \(\([a, b]\) = \([0, 1.5]\), \([0, 1.0]\), \ \([0, 0.5]\)\)]],
  "."
}], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, t, y];\)\), "\[IndentingNewLine]", 
    \(\(f[t_, y_]\  = \ 2\ y - y\^2 - 1;\)\ \), "\[IndentingNewLine]", 
    \(\(p0 = {6, 5, 4, 3, 2, 1, 4\/5};\)\ \), "\[IndentingNewLine]", 
    \(\(a = 0.0;\)\), "\[IndentingNewLine]", 
    \(\(b = 4.0;\)\ \), "\[IndentingNewLine]", 
    \(\(MEuler[\ a\ , b, p0\ , 110\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Z = 
        Table[0, {i, 
            Length[Y\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\
\)]}];\)\), "\[IndentingNewLine]", 
    \(\(graphs = Z;\)\ \), "\[IndentingNewLine]", 
