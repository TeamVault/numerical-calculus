Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 16.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, k, x, t];\)\ \), "\[IndentingNewLine]", 
    \(\(de = \(x'\)[t] \[Equal] 
          a\ x[t] - b\ x[t]\^2 - k;\)\ \), "\[IndentingNewLine]", 
