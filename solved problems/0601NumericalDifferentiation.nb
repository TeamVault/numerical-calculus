Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    RowBox[{\(f[x_]\  = \ \[ExponentialE]\^\(-x\)\ \ Sin[x];\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"Print", "[", 
        RowBox[{
        "\"\<  f[x] = \>\"", ",", \(f[x]\), " ", ",", "\"\<\\n\>\"", ",", 
          "\"\< f'[x] = \>\"", ",", \(\(f'\)[x]\), " ", ",", "\"\<\\n\>\"", 
          ",", "\"\<f''[x] = \>\"", ",", \(\(f''\)[x]\), " ", ",", 
          "\"\<\\n\\n\>\"", ",", "\"\<\!\(f\^\((3)\)\)[x] = \>\"", ",", 
          RowBox[{
            SuperscriptBox["f", 
              TagBox[\((3)\),
                Derivative],
              MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
      ";"}]}], "Input"],

Cell[BoxData[{
    RowBox[{\(Needs["\<Graphics`Colors`\>"];\), " "}], "\[IndentingNewLine]", 
  
    RowBox[{
      RowBox[{
        RowBox[{"Plot", "[", " ", 
          RowBox[{
            RowBox[{"{", " ", 
              RowBox[{
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((3)\),
                      Derivative],
                    MultilineFunction->None], "[", "x", "]"}], ",", 
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((3)\),
                      Derivative],
                    MultilineFunction->None], "[", "0", "]"}], ",", 
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((3)\),
                      Derivative],
                    MultilineFunction->None], "[", "\[Pi]", "]"}]}], " ", 
              "}"}], ",", \({x, 0, \[Pi]}\), 
            ",", \(PlotStyle \[Rule] {Orange, Blue, Cyan}\), 
            ",", \(Ticks \[Rule] {Range[0, \[Pi], \[Pi]\/4], 
                Range[0, 2, 0.5]}\)}], "]"}], ";"}], " "}], "\n", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<y = \!\(f\^\((3)\)\)[x] = \>\"", ",", 
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((3)\),
                  Derivative],
                MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
        ";"}], " "}], "\[IndentingNewLine]", \(Print["\<The maximum occurs at \
x = 0 and the minimum occurs at x = \[Pi].\>"];\), "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"extrema", "=", 
          RowBox[{"{", " ", 
            RowBox[{
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((3)\),
                    Derivative],
                  MultilineFunction->None], "[", "0", "]"}], ",", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((3)\),
                    Derivative],
                  MultilineFunction->None], "[", "\[Pi]", "]"}]}], " ", 
            "}"}]}], ";"}], " "}], "\n", 
    RowBox[{\(M\_3\  = \ Max[Abs[extrema]];\), " "}], "\n", 
    RowBox[{\(EB1[h_]\  = \ \(\(M\_3\) h\^2\)\/6;\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print[\*"\"\<Extrema of \!\(f\^\((3)\)\)[x]: \>\"", 
