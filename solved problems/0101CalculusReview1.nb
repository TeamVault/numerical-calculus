Cell[TextData[{
  StyleBox["Theorem 4.  (The Chain Rule).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Let   ",
  Cell[BoxData[
      \(z = f \((y)\)\)]],
  "  and  ",
  Cell[BoxData[
      \(y = g \((x)\)\)]],
  "  be differentiable functions,  and define  ",
  Cell[BoxData[
      \(h \((x)\)\)]],
  "  as their composition  \n\n\t\t",
  Cell[BoxData[
      \(h \((x)\) = f \((g \((x)\))\)\)]],
  ". \n\t\t\nThen  ",
  Cell[BoxData[
      \(h \((x)\)\)]],
  "  is differentiable and its derivative is  \n\n\t\t",
  Cell[BoxData[
      \(h' \((x)\) = \ f' \((g \((x)\))\)\ g' \((x)\)\)]],
  ".  "
}], "Text",
  CellTags->"The Chain Rule"],

Cell[BoxData[{
    \(\(Clear[e, f, g, h, p, q, x];\)\ \ \ \), "\n", 
    \(\(Print["\< h[x] = \>", f[g[x]]\ ];\)\ \ \), "\n", 
    \(\(Print["\<h'[x] = \>", \[PartialD]\_x\ f[g[x]]\ ];\)\ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["\n",
    FontWeight->"Bold"],
  StyleBox["Example 12.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      \(f \((y)\) = 1\/\(1\  - \ y\^2\)\)]],
  "  and  ",
  Cell[BoxData[
      \(g \((x)\) = \((Sin[x])\)\^2\)]],
  ".  Find the derivative of  ",
  Cell[BoxData[
      \(h \((x)\) = f \((g \((x)\))\)\)]],
  ".   "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 12.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(f[y_] = 1\/\(1\  - \ y\^2\);\)\ \ \), "\n", 
    \(\(g[x_] = \((Sin[x])\)\^2;\)\ \ \), "\n", 
    \(\(h[x_] = f[g[x]];\)\ \ \), "\n", 
    \(\(f1[y_] = \[PartialD]\_y\ f[y];\)\ \ \), "\n", 
    \(\(g1[x_] = \[PartialD]\_x\ g[x];\)\ \ \), "\n", 
    \(\(h1[x_] = \[PartialD]\_x\ h[x];\)\ \ \), "\n", 
    \(\(Print["\<f[y] = \>", f[y]\ ];\)\ \ \), "\n", 
    \(\(Print["\<g[x] = \>", g[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<h[x] = f[g[x]] = \>", f[g[x]]\ ];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\< f'[y] = \>", f1[y]\ ];\)\ \ \), "\n", 
    \(\(Print["\< f'[g[x]] = \>", f1[g[x]]\ ];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\< g'[x] = \>", 
        g1[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\< f'[g[x]] g'[x] = \>", 
        f1[g[x]]\ g1[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<          h'[x] = \>", h1[x]\ ];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 5.  (Intermediate Value Theorem for Continuous \
Functions).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is a continuous on the interval  ",
  Cell[BoxData[
      \(\([a, b]\)\)]],
  ",  and  ",
  Cell[BoxData[
      \(L\)]],
  "  is  any number between  ",
  Cell[BoxData[
      \(f \((a)\)\)]],
  "  and  ",
  Cell[BoxData[
      \(f \((b)\)\)]],
  ".  Then there exists a value  ",
  Cell[BoxData[
      \(c\)]],
  "  with  ",
  Cell[BoxData[
      \(a < c < b\)]],
  "  such that  ",
  Cell[BoxData[
      \(f \((c)\) = L\)]],
  ".  "
}], "Text",
  CellTags->"Intermediate Value Theorem for Continuous Functions"],

Cell[TextData[{
  StyleBox["Example 13.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Consider the function  ",
  Cell[BoxData[
      \(f \((x)\) = cos \((x - 1)\)\)]],
  ",  which is continuous on the interval  ",
  Cell[BoxData[
      \(\([0, 2.5]\)\)]],
  "  and suppose that  ",
  Cell[BoxData[
      \(L = 0.8\)]],
  "   Since  ",
  Cell[BoxData[
      \(f \((0)\) = \(0.540302 < L < 1 = \ f \((1)\)\)\)]],
  ",  there exists a value  ",
  Cell[BoxData[
      \(c\_1\)]],
  "  with  ",
  Cell[BoxData[
      \(0 < c\_1 < 1\)]],
  " so that  ",
  Cell[BoxData[
      \(f \((c\_1)\) = L\)]],
  ".   Also, since  ",
  Cell[BoxData[
      \(f \((2.5)\) = \(0.0707372 < L < 1 = f \((1)\)\)\)]],
  ",  there exists a value  ",
  Cell[BoxData[
      \(c\_2\)]],
  "  with  ",
  Cell[BoxData[
      \(1 < c\_2 < 2.5\)]],
  "  so that  ",
  Cell[BoxData[
      \(f \((c\_2)\) = L\)]],
  ".  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 13.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a1, a2, b1, b2, c, c1, c2, f, grf, ln0, ln1, ln2, L, x, y1, 
        y2];\)\ \ \), "\n", 
    \(\(f[x_] = Cos[x - 1];\)\ \ \), "\n", 
    \(\(a1 = 0;\)\ \ \), "\n", 
    \(\(b1 = 1;\)\ \ \), "\n", 
    \(\(a2 = 1;\)\ \ \), "\n", 
    \(\(b2 = 2.5;\)\ \ \), "\n", 
    \(\(L = 0.8;\)\ \ \), "\n", 
    \(\(solset\  = \ Solve[\ f[c]\  \[Equal] \ L, c\ ];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<   L = \>", L];\)\ \ \), "\n", 
    \(\(Print["\<f[c] = \>", L];\)\ \ \), "\n", 
    \(\(Print["\<Solve  f[c] \[Equal] L  for c.\>"];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(c\_1\) = \>\"", 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\)];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(c\_2\) = \>\"", 
        solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\)];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(c1 = 1 - ArcCos[0.8];\)\ \ \), "\n", 
    \(\(y1 = Cos[c1 - 1];\)\ \ \), "\n", 
    \(\(c2 = 1 + ArcCos[0.8];\)\ \ \), "\n", 
    \(\(y2 = Cos[c2 - 1];\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(grf = 
        Plot[f[x], {x, 0, 1 + \[Pi]\/2}, PlotStyle \[Rule] Red, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(ln0 = 
        Graphics[{Cyan, Line[{{0, L}, {1 + \[Pi]\/2, L}}]}];\)\ \ \), "\n", 
    \(\(ln1 = Graphics[{Green, Line[{{c1, 0}, {c1, y1}}]}];\)\ \ \), "\n", 
    \(\(ln2 = Graphics[{Green, Line[{{c2, 0}, {c2, y1}}]}];\)\ \ \), "\n", 
    \(\(Show[grf, ln0, ln1, ln2, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] "\<y = Cos[x-1]\>", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<f(x) = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<[a,b] = [\>", a1, "\<, \>", b1, "\<]\>"];\)\ \ \), "\n", 
