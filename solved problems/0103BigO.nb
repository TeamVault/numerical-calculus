Cell[BoxData[{
    \(\(Clear[f, g, h, m, n, P, Q];\)\ \ \), "\n", 
    \(\(n = 3;\)\ \ \), "\n", 
    \(\(P[h_]\  = \ Series[f[h], {h, 0, n - 1}];\)\ \ \), "\n", 
    \(\(m = 4;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Q[h_]\  = \ Series[g[h], {h, 0, m - 1}];\)\ \ \), "\n", 
    \(\(r\  = \ Min[n, m];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<f[h] = \>", P[h]];\)\ \ \), "\n", 
    \(\(Print["\<       has order of approximation \>", 
        O[h]\^n];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g[h] = \>", Q[h]];\)\ \ \), "\n", 
    \(\(Print["\<       has order of approximation \>", 
        O[h]\^m];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<r = min{\>", n, "\<,\>", m, "\<} = \>", r];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<f[h]+g[h] = \>", P[h] + Q[h]];\)\ \ \), "\n", 
    \(\(Print["\<            has order of approximation \>", 
        O[h]\^r];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[h]g[h] = \>", P[h]*Q[h]];\)\ \ \), "\n", 
    \(\(Print["\<           has order of approximation \>", 
        O[h]\^r];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(f[h]\/g[h]\) = \>\"", 
        P[h]\/Q[h]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<        has order of approximation \>", 
        O[h]\^r];\)\)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["\tIt is instructive to consider",
    FontFamily->"Times-Roman"],
  "  ",
  Cell[BoxData[
      \(p \((x)\)\)]],
  "  ",
  StyleBox["to be the ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(n\^th\)]],
  StyleBox[" degree Taylor polynomial approximation of",
    FontFamily->"Times-Roman"],
  "  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  StyleBox[";  then the remainder term is simply designated  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{
          StyleBox["O",
            FontWeight->"Bold"], \((h\^\(n + 1\))\)}]}]]],
  StyleBox[",  which stands for the presence of omitted terms starting with \
the power  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(h\^\(n + 1\)\)]],
  StyleBox[".  The remainder term converges to zero with the same rapidity \
that",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(\ \ \)\(h\^\(n + 1\)\)\)\)]],
  StyleBox["  converges to zero as ",
    FontFamily->"Times-Roman"],
  StyleBox["h",
    FontFamily->"Courier New"],
  StyleBox[" approaches zero, as expressed in the relationship\n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{
        RowBox[{
          RowBox[{
            StyleBox["O",
              FontWeight->"Bold"], \((h\^\(n + 1\))\)}], " ", "\[TildeTilde]",
           " ", \(M\ h\^\(n + 1\)\)}], " ", "=", " ", 
        RowBox[{
          FractionBox[
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((n + 1)\),
                  Derivative],
                
                MultilineFunction->None], \((c)\)}], \(\((n + 
                  1)\)!\)], \(h\^\(n + 1\)\)}]}]]],
  " ",
  StyleBox["\n\nfor sufficiently small  ",
    FontFamily->"Times-Roman"],
  StyleBox["h",
    FontFamily->"Courier New"],
  StyleBox[".  Hence the notation  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{
          StyleBox["O",
            FontWeight->"Bold"], \((h\^\(n + 1\))\)}]}]]],
  StyleBox["  stands in place of the quantity  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(M\ h\^\(n + 1\)\)]],
  StyleBox[", where  ",
    FontFamily->"Times-Roman"],
  StyleBox["M",
    FontFamily->"Courier New"],
  StyleBox["  is a constant or \[OpenCurlyDoubleQuote]behaves like a \
constant.\[CloseCurlyDoubleQuote]",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[TextData[{
  StyleBox["Theorem (",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  " ",
  StyleBox[ButtonBox["Taylor polynomial",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/TaylorSeries.html"], None},
    ButtonStyle->"Hyperlink"],
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  " ",
  StyleBox[").",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that the function  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  and its derivatives  ",
  Cell[BoxData[
      RowBox[{\(f' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f'' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], "...", " ", 
        StyleBox[",",
          FontFamily->"Times New Roman"], 
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((n + 1)\),
              Derivative],
            MultilineFunction->None], \((x)\)}]}]]],
  "  are all continuous on  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ".  If  both  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  and  ",
  Cell[BoxData[
      \(x = x\_0 + h\)]],
  "  lie in the interval  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  and  ",
  Cell[BoxData[
      \(h = x - x\_0\)]],
  "  then  \n\n\t",
  Cell[BoxData[
      RowBox[{\(f \((x\_0 + h)\)\), " ", "=", " ", 
        RowBox[{
          RowBox[{
            StyleBox[
              UnderoverscriptBox["\[Sum]", 
                StyleBox[\(k = 0\),
                  FontSize->10], 
                StyleBox["n",
                  FontSize->10]],
              FontSize->18], " ", 
            RowBox[{
              FractionBox[
                StyleBox[
                  RowBox[{
                    SuperscriptBox["f", 
                      TagBox[\((k)\),
                        Derivative],
                      MultilineFunction->None], \((x\_0)\)}],
                  FontSize->12], 
                StyleBox[\(k!\),
                  FontSize->12]], \(h\^k\)}]}], " ", "+", " ", 
          RowBox[{
            StyleBox["O",
              FontWeight->"Bold"], \((h\^\(n + 1\))\)}]}]}]]],
  ",  \n\t\nis the n-th degree ",
  ButtonBox["Taylor polynomial",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/TaylorSeries.html"], None},
    ButtonStyle->"Hyperlink"],
  " expansion of  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  about  ",
  Cell[BoxData[
      \(x\_0\)]],
  ".  The Taylor polynomial of degree ",
  StyleBox["n",
    FontFamily->"Courier New"],
  "  is \n\n\t",
  Cell[BoxData[
      RowBox[{\(\(P\_n\) \((x)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox[
            UnderoverscriptBox["\[Sum]", 
              StyleBox[\(k = 0\),
                FontSize->10], 
              StyleBox["n",
                FontSize->10]],
            FontSize->18], " ", 
          RowBox[{
            FractionBox[
              StyleBox[
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((k)\),
                      Derivative],
                    MultilineFunction->None], \((x\_0)\)}],
                FontSize->12], 
              StyleBox[\(k!\),
                FontSize->12]], \(\((x - x\_0)\)\^k\)}]}]}]]],
  "   \nand  \n\t",
  Cell[BoxData[
      RowBox[{\(f \((x)\)\), " ", "=", " ", 
        RowBox[{\(\(P\_n\) \((x)\)\  + \ \(R\_n\) \((x)\)\), " ", "=", " ", 
          RowBox[{\(\(P\_n\) \((x)\)\), " ", "+", " ", 
            RowBox[{
              StyleBox["O",
                FontWeight->"Bold"], \((h\^\(n + 1\))\)}]}]}]}]]],
  ".\n\nThe integral form of the remainder is  \n\n\t",
  Cell[BoxData[
      RowBox[{\(\(R\_n\) \((x)\)\), " ", "=", " ", 
        RowBox[{\(1\/\(n!\)\), 
          RowBox[{\(\[Integral]\_\(x\_0\)\%x\), 
            RowBox[{\(\((x - t)\)\^n\), 
              SuperscriptBox["f", 
                TagBox[\((n + 1)\),
                  Derivative],
                
                MultilineFunction->
                  None], \((t)\), \(\[DifferentialD]t\)}]}]}]}]]],
  ",\n\n and Lagrange's formula for the remainder is\n\n\t",
  Cell[BoxData[
      RowBox[{\(\(R\_n\) \((x)\)\), "=", 
        RowBox[{
          FractionBox[
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((n + 1)\),
                  Derivative],
                
                MultilineFunction->None], \((c)\)}], \(\((n + 
                  1)\)!\)], \(\((x - x\_0)\)\^\(n + 1\)\)}]}]]],
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{"=", 
          RowBox[{
            FractionBox[
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((n + 1)\),
                    Derivative],
                  
                  MultilineFunction->None], \((c)\)}], \(\((n + 
                    1)\)!\)], \(h\^\(n + 1\)\)}]}]}]]],
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{"=", " ", 
          RowBox[{
            StyleBox["O",
              FontWeight->"Bold"], \((h\^\(n + 1\))\)}]}]}]]],
  "\n\nwhere ",
  Cell[BoxData[
      \(c\)]],
  " depends on ",
  Cell[BoxData[
      \(x\)]],
  " and lies somewhere between  ",
  Cell[BoxData[
      RowBox[{\(x\_0\), "  ", 
        StyleBox["and",
          FontFamily->"Times New Roman"], "  ", "x"}]]],
  ".  "
}], "Text",
  CellTags->"Taylor's Polynomial with Integral Remainder"],

