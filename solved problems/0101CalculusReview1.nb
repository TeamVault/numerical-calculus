          SequenceForm[ "\n", "Derivative of a scalar multiple."],
          Editable->False], "\n", 
        InterpretationBox[
          
          RowBox[{"\<\" F[x] = \"\>", "\[InvisibleSpace]", \(c\ f[x]\), 
            "\[InvisibleSpace]", "\<\"\\n\"\>", 
            "\[InvisibleSpace]", "\<\"F'[x] = \"\>", "\[InvisibleSpace]", 
            RowBox[{"c", " ", 
              RowBox[{
                SuperscriptBox["f", "\[Prime]",
                  MultilineFunction->None], "[", "x", "]"}]}]}],
          SequenceForm[ " F[x] = ", 
            Times[ c, 
              f[ x]], "\n", "F'[x] = ", 
            Times[ c, 
              Derivative[ 1][ f][ x]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Derivative of a linear \
combination."\),
          SequenceForm[ "\n", "Derivative of a linear combination."],
          Editable->False], "\n", 
        InterpretationBox[
          
          RowBox[{"\<\" F[x] = \"\>", 
            "\[InvisibleSpace]", \(a\ f[x] + b\ g[x]\), 
            "\[InvisibleSpace]", "\<\"\\n\"\>", 
            "\[InvisibleSpace]", "\<\"F'[x] = \"\>", "\[InvisibleSpace]", 
            RowBox[{
              RowBox[{"a", " ", 
                RowBox[{
                  SuperscriptBox["f", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}]}], "+", 
              RowBox[{"b", " ", 
                RowBox[{
                  SuperscriptBox["g", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}]}]}]}],
          SequenceForm[ " F[x] = ", 
            Plus[ 
              Times[ a, 
                f[ x]], 
              Times[ b, 
                g[ x]]], "\n", "F'[x] = ", 
            Plus[ 
              Times[ a, 
                Derivative[ 1][ f][ x]], 
              Times[ b, 
                Derivative[ 1][ g][ x]]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Derivative of the product \
of two functions."\),
          SequenceForm[ "\n", "Derivative of the product of two functions."],
          Editable->False], "\n", 
        InterpretationBox[
          
          RowBox[{"\<\" F[x] = \"\>", "\[InvisibleSpace]", \(f[x]\ g[x]\), 
            "\[InvisibleSpace]", "\<\"\\n\"\>", 
            "\[InvisibleSpace]", "\<\"F'[x] = \"\>", "\[InvisibleSpace]", 
            RowBox[{
              RowBox[{\(g[x]\), " ", 
                RowBox[{
                  SuperscriptBox["f", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}]}], "+", 
              RowBox[{\(f[x]\), " ", 
                RowBox[{
                  SuperscriptBox["g", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}]}]}]}],
          SequenceForm[ " F[x] = ", 
            Times[ 
              f[ x], 
              g[ x]], "\n", "F'[x] = ", 
            Plus[ 
              Times[ 
                g[ x], 
                Derivative[ 1][ f][ x]], 
              Times[ 
                f[ x], 
                Derivative[ 1][ g][ x]]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Derivative of the quotient \
of two functions."\),
          SequenceForm[ "\n", "Derivative of the quotient of two functions."],
          
          Editable->False], "\n", 
        InterpretationBox[
          
          RowBox[{"\<\" F[x] = \"\>", "\[InvisibleSpace]", \(f[x]\/g[x]\), 
            "\[InvisibleSpace]", "\<\"\\n\"\>", 
            "\[InvisibleSpace]", "\<\"F'[x] = \"\>", "\[InvisibleSpace]", 
            FractionBox[
              RowBox[{
                RowBox[{\(g[x]\), " ", 
                  RowBox[{
                    SuperscriptBox["f", "\[Prime]",
                      MultilineFunction->None], "[", "x", "]"}]}], "-", 
                RowBox[{\(f[x]\), " ", 
                  RowBox[{
                    SuperscriptBox["g", "\[Prime]",
                      MultilineFunction->None], "[", "x", "]"}]}]}], \(g[
                    x]\^2\)]}],
          SequenceForm[ " F[x] = ", 
            Times[ 
              f[ x], 
              Power[ 
                g[ x], -1]], "\n", "F'[x] = ", 
            Times[ 
              Power[ 
                g[ x], -2], 
              Plus[ 
                Times[ 
                  g[ x], 
                  Derivative[ 1][ f][ x]], 
                Times[ -1, 
                  f[ x], 
                  Derivative[ 1][ g][ x]]]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Derivative of the \
composition of two functions."\),
          SequenceForm[ 
          "\n", "Derivative of the composition of two functions."],
          Editable->False], "\n", 
        InterpretationBox[
          
          RowBox[{"\<\" F[x] = \"\>", "\[InvisibleSpace]", \(f[g[x]]\), 
            "\[InvisibleSpace]", "\<\"\\n\"\>", 
            "\[InvisibleSpace]", "\<\"F'[x] = \"\>", "\[InvisibleSpace]", 
            RowBox[{
              RowBox[{
