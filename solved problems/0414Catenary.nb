  Cell[BoxData[
      RowBox[{"y", "  ", "=", " ", 
        RowBox[{\(f \((x)\)\), " ", "=", "  ", 
          RowBox[{
            RowBox[{\(c\ cosh \((x\/c)\)\), " ", "-", " ", 
              RowBox[{"c", "    ", 
                StyleBox["where",
