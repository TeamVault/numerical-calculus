Cell[BoxData[{
    RowBox[{\(Needs["\<Graphics`Colors`\>"];\), "  "}], "\[IndentingNewLine]",
   
    RowBox[{
      RowBox[{
        RowBox[{\(CubicSearch[a0_, b0_, max_]\), ":=", "\[IndentingNewLine]", 
          
          RowBox[{"Module", "[", 
            
            RowBox[{\({a\  = \ N[a0], b\  = \ N[b0], c, d, h, k, m, r, Ya, 
                Yb, Yc, Yd}\), ",", " ", "\[IndentingNewLine]", 
              
              RowBox[{\(p\_0 = a\), ";", \(Clear[h]\), 
                ";", \(h\_0 = \(2 \((f[b] - f[a])\)\)\/\(f'\)[a]\), 
                ";", \(p\_1 = p\_0 + h\_0\), ";", 
                "\[IndentingNewLine]", \(pointsab = {{Red, PointSize[0.02], 
                      Point[{p\_0, f[p\_0]}], Point[{p\_1, f[p\_1]}]}}\), ";",
                 "  ", "\[IndentingNewLine]", \(dotsab = Graphics[pointsab]\),
                 ";", "  ", 
                "\[IndentingNewLine]", \(gr0 = 
                  Plot[f[x], {x, a0, b0}, 
                    PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 0.75}}, 
                    PlotStyle \[Rule] Magenta, 
                    DisplayFunction \[Rule] Identity]\), ";", " ", 
                "\[IndentingNewLine]", \(Show[gr0, dotsab, 
                  PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 0.72}}, 
                  Ticks \[Rule] {Range[0, 2, 0.1], Range[0, 2, 0.1]}, 
                  DisplayFunction \[Rule] $DisplayFunction]\), ";", " ", 
                "\[IndentingNewLine]", \(Print["\<p\>"\_0, "\< = \>", 
                  PaddedForm[p\_0, {9, 8}\ ], "\<,  f[\>", 
                  PaddedForm[p\_0, {9, 8}\ ], "\< ] = \>", 
                  PaddedForm[f[p\_0], {11, 10}]\ ]\), ";", 
                "\[IndentingNewLine]", \(Print["\<p\>"\_1, "\< = \>", 
                  PaddedForm[p\_1, {9, 8}\ ], "\<,  f[\>", 
                  PaddedForm[p\_1, {9, 8}\ ], "\< ] = \>", 
                  PaddedForm[f[p\_1], {11, 10}]\ ]\), ";", 
                "\[IndentingNewLine]", \(m = 1\), ";", "\[IndentingNewLine]", 
                
                RowBox[{"While", "[", " ", 
                  RowBox[{\(m \[LessEqual] max\), ",", 
                    
                    RowBox[{\(F = f[p\_1] - f[p\_0]\), 
                      ";", \(G = \(h\_0\) \((\(f'\)[p\_1] - \(f'\)[p\_0])\)\),
                       ";", \(\[Alpha] = G - 2 \((F - h\_0\ \(f'\)[p\_0])\)\),
                       ";", \(\[Gamma] = \(\(-2\)\ h\_0\ \
\(f'\)[p\_0]\)\/\(\((G - 3\ \[Alpha])\) + \@\(\((G - 3\ \[Alpha])\)\^2 - 12\ \
h\_0\ \[Alpha]\ \(f'\)[p\_0]\)\)\), ";", " ", 
                      RowBox[{\(p\_2\), "=", 
                        StyleBox[" ",
                          
                          FontFamily->
                            "Times-Roman"], \(p\_0 + \[Gamma]\ h\_0\)}], 
                      ";", \(m = m + 1\), ";", 
                      "\[IndentingNewLine]", \(pointsab = {{Red, 
                            PointSize[0.02], Point[{p\_0, f[p\_0]}], 
                            Point[{p\_1, f[p\_1]}]}, {Blue, PointSize[0.02], 
                            Point[{p\_2, f[p\_2]}]}}\), ";", "  ", 
                      "\[IndentingNewLine]", \(dotsab = Graphics[pointsab]\), 
                      ";", "  ", 
                      "\[IndentingNewLine]", \(Show[gr0, dotsab, 
                        PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 
                              0.72}}, 
                        Ticks \[Rule] {Range[0, 2, 0.1], Range[0, 2, 0.1]}, 
                        DisplayFunction \[Rule] $DisplayFunction]\), ";", " ",
                       "\[IndentingNewLine]", \(Print["\<p\>"\_m, "\< = \>", 
                        PaddedForm[p\_2, {9, 8}\ ], "\<,  f[\>", 
                        PaddedForm[p\_2, {9, 8}\ ], "\< ] = \>", 
                        PaddedForm[f[p\_2], {11, 10}]\ ]\), ";", 
                      "\[IndentingNewLine]", \(p\_0 = p\_1\), 
                      ";", \(p\_1 = p\_2\), ";", \(h\_0 = p\_1 - p\_0\), 
                      ";"}]}], " ", "]"}], ";", " ", 
                "\[IndentingNewLine]", \(Print["\<After m = \>", 
                  m - 1, "\< iterations\>"]\), ";", " ", 
                "\[IndentingNewLine]", \(Print["\<f[x] = \>", f[x]]\), ";", 
                "\[IndentingNewLine]", \(Print["\<f[\>", 
                  PaddedForm[p\_2, {9, 8}\ ], "\< ] = \>", 
                  PaddedForm[f[p\_2], {11, 10}]\ ]\), ";", 
                "\[IndentingNewLine]", \(Return[{f[p\_2], p\_2}\ ]\)}]}], 
            "]"}]}], ";"}], " "}]}], "Input",
  AspectRatioFixed->False],

