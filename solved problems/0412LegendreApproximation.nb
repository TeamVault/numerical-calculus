Cell[TextData[{
  StyleBox["Example 3.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Compare the \"discrete least squares approximation\" and \"continuous \
least squares approximation,\" on the interval  ",
  Cell[BoxData[
      \(\([\(-1\), 1]\)\)]],
  ".  \n",
  StyleBox["3 (a).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Use the function  ",
  Cell[BoxData[
      \(f[x] = \[ExponentialE]\^x\)]],
  ".  \n",
  StyleBox["3 (b).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Use the function  ",
  Cell[BoxData[
      \(f[x] = \(\[ExponentialE]\^x\) Cos[\[Pi]\/2\ x]\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 3 (a).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(f[x_] = \[ExponentialE]\^x;\)\ \ \), "\[IndentingNewLine]", 
    \(\(n = 4;\)\ \), "\[IndentingNewLine]", 
    \(\(a = \(-1\);\)\ \), "\[IndentingNewLine]", 
    \(\(b = \ 1;\)\ \), "\[IndentingNewLine]", 
    \(Clear[k]; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(eqns\  = \ 
        Table[\ \[Sum]\+\(k = 1\)\%\(n + 1\)\(c\_k\) \(\[Integral]\_a\%b\ \(x\
\^\(j + k - 2\)\) \[DifferentialD]x\)\ \  \[Equal] \ \ \[Integral]\_a\%b\ 
              f[x] \(x\^\(j - 1\)\) \[DifferentialD]x, {j, 1, 
            n + 1}];\)\), "\[IndentingNewLine]", 
    \(\(vars\  = \ \ Table[\ 
          c\_k, {k, 1, n + 1}];\)\ \), "\[IndentingNewLine]", 
    \(\(solset = Solve[eqns, vars];\)\ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Solve the normal equations\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[TableForm[eqns]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<For \>", \*"\"\<{\!\(c\_k\)}\>\""\_\("\<k=1\>"\)\%\(n + 1\), \
"\< = \>", vars];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[
        TableForm[
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)]]\
;\)\), "\[IndentingNewLine]", 
    \(\(nsolset = solset;\)\), "\[IndentingNewLine]", 
    \(\(nsolset\_\(\(\[LeftDoubleBracket]\)\(1, All, 2\)\(\
\[RightDoubleBracket]\)\) = 
        N[solset\_\(\(\[LeftDoubleBracket]\)\(1, All, 2\)\(\
\[RightDoubleBracket]\)\)];\)\), "\[IndentingNewLine]", 
    \(\(Print[\ TableForm[Partition[nsolset, 1]]\ ];\)\ \)}], "Input"],

Cell[BoxData[{
    \(\(p\_n[x_] = 
