                    Cos[j  \[Pi]\/L\ X\_k] Y\_k\)}], "]"}]}]}], " ", "]"}], 
        ";"}], "  "}], "\n", 
    RowBox[{
      RowBox[{
        RowBox[{"For", "[", 
          
          RowBox[{\(j = 1\), ",", \(j \[LessEqual] m\), ",", \(j++\), ",", 
            "\[IndentingNewLine]", 
            RowBox[{\(B\_j\), "=", 
              RowBox[{"Chop", "[", " ", 
                RowBox[{
                  FractionBox[
                    StyleBox["2",
                      FontSize->12], 
                    StyleBox["n",
                      FontSize->12]], \(\[Sum]\+\(k = 0\)\%\(n - 1\)\ 
                    Sin[j  \[Pi]\/L\ X\_k] Y\_k\)}], "]"}]}]}], " ", "]"}], 
        ";"}], "  "}], "\[IndentingNewLine]", 
