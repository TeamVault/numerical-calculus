    \(\(Print[MatrixForm[L . Y], "\< = \>", 
        MatrixForm[B]];\)\), "\[IndentingNewLine]", 
    \(\(Print[L . Y \[Equal] B];\)\)}], "Input"],

Cell[TextData[{
  "Second, s",
  "olve the upper-triangular system    ",
  StyleBox["UX",
    FontWeight->"Bold"],
  " = ",
  StyleBox["Y",
    FontWeight->"Bold"],
  "  for  ",
  StyleBox["X",
    FontWeight->"Bold"],
  "."
}], "Text"],

Cell[BoxData[{
    \(BackSub[5]\), "\[IndentingNewLine]", 
    \(\(Print["\<X = \>", MatrixForm[X]\ ];\)\)}], "Input"],

Cell[TextData[{
  "Verify that  ",
  StyleBox["UX",
    FontWeight->"Bold"],
  " = ",
  StyleBox["Y",
    FontWeight->"Bold"],
  ".  "
}], "Text"],

