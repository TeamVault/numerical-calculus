  Cell[BoxData[
      RowBox[{
        RowBox[{"|", 
          RowBox[{
            SuperscriptBox["f", 
              TagBox[\((4)\),
                Derivative],
              MultilineFunction->None], "[", "c", "]"}], "|", 
          RowBox[{"\[LessEqual]", 
