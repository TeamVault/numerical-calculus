Cell[TextData[{
  StyleBox["Corollary (Newton-Cotes Precision)",
    FontFamily->"Times-Bold",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  Assume that  ",
    FontFamily->"Times-Roman"],
  StyleBox["f(x)",
    FontFamily->"Courier New"],
  StyleBox["  is sufficiently differentiable; then  ",
    FontFamily->"Times-Roman"],
  StyleBox["E[f]",
    FontFamily->"Courier New"],
  StyleBox["  for Newton-Cotes quadrature involves an appropriate higher \
derivative.",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[TextData[{
  StyleBox["(8) The trapezoidal rule has degree of precision  ",
    FontFamily->"Times-Roman"],
  StyleBox["n=1",
    FontFamily->"Courier New"],
  StyleBox[".  If  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \[Element] \(C\^2\)[a, b]\)]],
  StyleBox[", then \n\n\t",
    FontFamily->"Times-Roman"],
  "\t",
  Cell[BoxData[
      RowBox[{\(\[Integral]\_\(x\_0\)\%\(x\_1\)\ 
          f \((x)\) \[DifferentialD]x\), " ", "=", " ", 
        RowBox[{\(\(h\/2\) \((f\_0\  + \ f\_1)\)\), " ", "-", 
          RowBox[{
            InterpretationBox[
              RowBox[{\(1\/12\), 
                SuperscriptBox["f", 
                  TagBox[\((2)\),
                    Derivative],
                  MultilineFunction->None], \((c)\)}],
              SeriesData[ h, 0, {
                Times[ 
                  Rational[ 1, 24], 
                  Derivative[ 2][ f][ x]]}, 3, 4, 1]], \(h\^3\)}]}]}]]],
  ". "
}], "Text"],

Cell[TextData[{
  StyleBox["(9) Simpson\[CloseCurlyQuote]s rule has degree of precision  ",
    FontFamily->"Times-Roman"],
  StyleBox["n=3",
    FontFamily->"Courier New"],
  StyleBox[".  If  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \[Element] \(C\^4\)[a, b]\)]],
  StyleBox[", then \n\n\t",
    FontFamily->"Times-Roman"],
  "\t",
  Cell[BoxData[
      RowBox[{\(\[Integral]\_\(x\_0\)\%\(x\_2\)\ 
          f \((x)\) \[DifferentialD]x\), " ", "=", " ", 
        RowBox[{\(\(h\/3\) \((f\_0\  + \ 4  f\_1\  + \ f\_2)\)\), " ", "-", 
          RowBox[{
            InterpretationBox[
              RowBox[{\(1\/90\), " ", 
                SuperscriptBox["f", 
                  TagBox[\((4)\),
                    Derivative],
                  MultilineFunction->None], \((c)\)}],
              SeriesData[ h, 0, {
                Times[ 
                  Rational[ 1, 24], 
                  Derivative[ 2][ f][ x]]}, 3, 4, 1]], \(h\^5\)}]}]}]]],
  ". "
}], "Text"],

Cell[TextData[{
  StyleBox["(10) Simpson\[CloseCurlyQuote]s ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(3\/8\)]],
  StyleBox["rule has degree of precision  ",
    FontFamily->"Times-Roman"],
  StyleBox["n=3",
    FontFamily->"Courier New"],
  StyleBox[".  If  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \[Element] \(C\^4\)[a, b]\)]],
  StyleBox[", then \n\n\t",
    FontFamily->"Times-Roman"],
  "\t",
  Cell[BoxData[
      RowBox[{\(\[Integral]\_\(x\_0\)\%\(x\_3\)\ 
          f \((x)\) \[DifferentialD]x\), " ", "=", " ", 
        RowBox[{\(\(\(3  h\)\/8\) \((f\_0\  + \ 3  f\_1\  + \ 3  f\_2 + \ 
                f\_3)\)\), " ", "-", 
          RowBox[{
            InterpretationBox[
              RowBox[{\(3\/80\), " ", 
                SuperscriptBox["f", 
                  TagBox[\((4)\),
                    Derivative],
                  MultilineFunction->None], \((c)\)}],
              SeriesData[ h, 0, {
                Times[ 
                  Rational[ 1, 24], 
                  Derivative[ 2][ f][ x]]}, 3, 4, 1]], \(h\^5\)}]}]}]]],
  ". "
}], "Text"],

Cell[TextData[{
  StyleBox["(11) Boole\[CloseCurlyQuote]s rule has degree of precision  ",
    FontFamily->"Times-Roman"],
  StyleBox["n=5",
    FontFamily->"Courier New"],
  StyleBox[".  If  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \[Element] \(C\^6\)[a, b]\)]],
