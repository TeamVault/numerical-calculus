Cell[TextData[{
  StyleBox["Program ",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox["(Neumann",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox[" Method for Laplace's equation",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox[")",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox["  To approximate the solution of the Laplace's equation  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(u\_xx\) \((x, y)\) + \(u\_yy\) \((x, y)\) = 0\)]],
  "  ",
  StyleBox["over the rectangle  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Italic"], 
        "=", \({\((x, y)\) : 0 \[LessEqual] x \[LessEqual] a, 
          0 \[LessEqual] y \[LessEqual] b}\)}]]],
  "  with  ",
  Cell[BoxData[
      \(\(u\_y\) \((x, 0)\) = 0\)]],
  ",  ",
  Cell[BoxData[
      \(u \((x, b)\) = \(f\_2\) \((x)\)\)]],
  ",  for  ",
  Cell[BoxData[
      \(0 \[LessEqual] x \[LessEqual] a\)]],
  ",  and  ",
  Cell[BoxData[
      RowBox[{\(u \((0, y)\)\), "=", 
        RowBox[{
          RowBox[{\(f\_3\), \((y)\), "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "  ", "u", \((a, y)\)}], 
          "=", \(\(f\_4\) \((y)\)\)}]}]]],
  ",  for  ",
  Cell[BoxData[
      \(0 \[LessEqual] t \[LessEqual] b\)]],
  ".  It is assumed that  ",
  Cell[BoxData[
      \(\[CapitalDelta]x = \(\[CapitalDelta]y = h\)\)]],
  "  and integers  n  and  m  exist so that  ",
  Cell[BoxData[
      RowBox[{"a", " ", "=", " ", 
        RowBox[{
          RowBox[{"n", " ", "h", "   ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "   ", "b"}], " ", "=", 
          " ", \(m\ h\)}]}]]],
  ".  "
}], "Text"],

Cell[BoxData[
    \(\(\(Neumann[n_, 
          m_] := \[IndentingNewLine]Module[{Err = 1.0, i, j, 
            k = 0, \[Omega]}, \[IndentingNewLine]\[Omega]\  = \ 
            4\/\(2 + \@\(4 - \((Cos[\[Pi]\/\(n - 1\)] + Cos[\[Pi]\/\(m - \
1\)])\)\^2\)\); \ \ \[IndentingNewLine]While[\ 
            And[Err > 0.001\ , \ 
              k \[LessEqual] 70\ ], \[IndentingNewLine]Err\  = \ 
              0.0; \ \[IndentingNewLine]For[\ j = 2, \ 
              j \[LessEqual] 
                m - 1, \ \(j++\), \[IndentingNewLine]\(For[\ i = 2, \ 
                  i \[LessEqual] 
                    n - 1, \ \(i++\), \[IndentingNewLine]Relax = \(1\/4\) \((\
\[Omega]\ \((u\_\(\(\[LeftDoubleBracket]\)\(i, j + 1\)\(\[RightDoubleBracket]\
\)\) + u\_\(\(\[LeftDoubleBracket]\)\(i, j - 1\)\(\[RightDoubleBracket]\)\) + 
                              u\_\(\(\[LeftDoubleBracket]\)\(i + 1, j\)\(\
\[RightDoubleBracket]\)\) + 
                              u\_\(\(\[LeftDoubleBracket]\)\(i - 1, j\)\(\
\[RightDoubleBracket]\)\) - 
                              4\ u\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\))\))\); \ \[IndentingNewLine]u\_\(\(\
\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\  = \ 
                    u\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\)\  + \ Relax; \ \[IndentingNewLine]Err\  = \ 
                    Max[Err, 
                      Abs[Relax]];\ \ ];\)\ ]; \ \[IndentingNewLine]For[\ 
              j = 2, \ j \[LessEqual] 
                m - 1, \ \(j++\), \[IndentingNewLine]Relax = \(1\/4\) \((\
\[Omega]\ \((2\ u\_\(\(\[LeftDoubleBracket]\)\(n - 1, j\)\(\
\[RightDoubleBracket]\)\) + 
                          u\_\(\(\[LeftDoubleBracket]\)\(n, j - 1\)\(\
\[RightDoubleBracket]\)\) + 
                          u\_\(\(\[LeftDoubleBracket]\)\(n, j + 1\)\(\
\[RightDoubleBracket]\)\) - 
                          4\ u\_\(\(\[LeftDoubleBracket]\)\(n, j\)\(\
\[RightDoubleBracket]\)\))\))\); \ \[IndentingNewLine]u\_\(\(\
\[LeftDoubleBracket]\)\(n, j\)\(\[RightDoubleBracket]\)\)\  = \ 
                u\_\(\(\[LeftDoubleBracket]\)\(n, \
j\)\(\[RightDoubleBracket]\)\)\  + \ Relax; \ \[IndentingNewLine]Err\  = \ 
                Max[Err, 
                  Abs[Relax]];\ ]; \ \[IndentingNewLine]Print["\<Max grid \
change = \>", Err]; \ \[IndentingNewLine]k\  = \ 
              k + 1;\ ];\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->False],

