    \(\(Print["\<       g[h] = \>", Q[h]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[h] + g[h] = (\>", Normal[P[h]], "\<) + (\>", 
        Normal[Q[
            h]], "\<) + \>", "\<O[h]\>", "\<\>"\^"\<min{\>", "\<\>"\^n, \
"\<\>"\^"\<,\>", "\<\>"\^m, "\<\>"\^"\<}\>"\ ];\)\ \ \), "\n", 
