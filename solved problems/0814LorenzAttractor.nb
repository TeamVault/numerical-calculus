Cell[BoxData[{
    \(\(Clear[c, f, p, r, t, x, y, z];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f\_1[t_, x_, y_, z_]\  = \ 
        p \((y - x)\);\)\ \ \), "\[IndentingNewLine]", 
    \(\(f\_2[t_, x_, y_, z_]\  = \ 
        r\ x - y - x\ z;\)\ \ \), "\[IndentingNewLine]", 
    \(\(f\_3[t_, x_, y_, z_]\  = \ x\ y\  - \ c\ z;\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(p = 10;\)\), "\[IndentingNewLine]", 
    \(\(r = 28;\)\), "\[IndentingNewLine]", 
    \(\(c = 8\/3;\)\), "\[IndentingNewLine]", 
    \(\(F[t_, {x_, y_, z_}]\  = \ {f\_1[t, x, y, z], f\_2[t, x, y, z], 
          f\_3[t, x, y, z]\ };\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Find a numerical solution to Lorenz equations\>"];\)\ \ \), \
"\[IndentingNewLine]", 
    \(\(Print["\<x' = \>", 
        f\_1[t, x, y, z]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<y' = \>", 
        f\_2[t, x, y, z]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<z' = \>", 
        f\_3[t, x, y, z]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<F[t,{x,y,z}] = \>", F[t, {x, y, z}]\ ];\)\ \ \)}], "Input"],

Cell["\<\
Compute the Runge-Kutta solution. Since this curve is known to \
twist a lot in 3D space, it will require about 1000 subintervals to get good \
resolution. \
\>", "Text"],

Cell[BoxData[{
    \(\(a = 0.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(b = 10.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(x0\  = \ 10;\)\), "\[IndentingNewLine]", 
    \(\(y0\  = \ \(-10\);\)\), "\[IndentingNewLine]", 
    \(\(z0\  = \ 20;\)\), "\[IndentingNewLine]", 
    \(\(n = 1000;\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(eqns = {f\_1[t, x, y, z] \[Equal] 0, f\_2[t, x, y, z] \[Equal] 0, 
          f\_3[t, x, y, z] \[Equal] 0};\)\), "\[IndentingNewLine]", 
    \(\(solset = Solve[eqns, {x, y, z}];\)\), "\[IndentingNewLine]", 
    \(\(pts3 = Runge3D[a, b, x0, y0, z0, n];\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(pts = 
        Table[ReplaceAll[{x, y, z}, 
            solset\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\)\
], {i, 2, 3}];\)\), "\[IndentingNewLine]", 
    \(\(dots = 
        Table[Point[
            ReplaceAll[{x, y, z}, 
              solset\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\
\)]], {i, 2, 3}];\)\), "\[IndentingNewLine]", 
    \(\(dots = 
