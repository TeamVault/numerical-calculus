Cell[TextData[{
  StyleBox["Example 3.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  StyleBox[" ",
    FontWeight->"Bold"],
  " Solve the population model with harvesting  ",
  Cell[BoxData[
      \(dx\/dt\  = \ a\ x\  - \ b\ x\^2\  - \ k\)]],
  ", \nusing the constants  ",
  StyleBox["a = 2",
    FontFamily->"Courier New"],
  ",  ",
  StyleBox["b = 1",
    FontFamily->"Courier New"],
  ",  and  ",
  StyleBox["k = 1",
    FontFamily->"Courier New"],
  ",  and explore this situation."
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 3.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, k, x, t];\)\ \), "\[IndentingNewLine]", 
    \(\(de = \(x'\)[t] \[Equal] 
          a\ x[t] - b\ x[t]\^2 - k;\)\ \), "\[IndentingNewLine]", 
    \(\(de1 = 
        ReplaceAll[\ 
          de, \ {a \[Rule] 2, b \[Rule] 1, 
            k \[Rule] 1}\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Replace  {a\[Rule]2, b\[Rule]1, k\[Rule]1}\>"];\)\ \ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<Get\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de1];\)\ \ \)}], "Input"],

Cell["Construct the general solution. ", "Text"],

Cell[BoxData[{
    \(\(Print["\<Solve\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de1];\)\ \), "\[IndentingNewLine]", 
    \(\(sol1 = DSolve[de1\ , x[t], t];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[sol1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Get\>"];\)\ \), "\[IndentingNewLine]", 
    \(x[t_] = \ 
      sol1\_\(\(\[LeftDoubleBracket]\)\(1, 1, 2\)\(\[RightDoubleBracket]\)\); \
\ Print["\<x[t] = \>", x[t]\ ];\ \)}], "Input"],

Cell["Construct the constant solution.  ", "Text"],

Cell[BoxData[{
    \(\(ceqn = 
        a\ X[t] - b\ X[t]\^2 - k\  \[Equal] \ 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(ceqn1 = 
        ReplaceAll[
          ceqn, \ {a \[Rule] 2, b \[Rule] 1, 
            k \[Rule] 1}\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(csolset = Solve[\ ceqn1, \ X[t]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(x\_1[t_] = \ 
        csolset\_\(\(\[LeftDoubleBracket]\)\(1, 1, 2\)\(\[RightDoubleBracket]\
\)\);\)\ \), "\[IndentingNewLine]", 
    \(\(Print[ceqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Replace {a\[Rule]2, b\[Rule]1, k\[Rule]1}\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<Solve\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[ceqn1];\)\ \), "\[IndentingNewLine]", 
    \(Print["\<Get\>"]; \ 
    Print[\*"\"\<\!\(x\_1\)[t] = \>\"", x\_1[t]\ ];\ \)}], "Input"],

Cell[TextData[{
