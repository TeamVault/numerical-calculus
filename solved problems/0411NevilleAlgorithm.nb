    RowBox[{
      RowBox[{\(Neville[X_, z_]\), ":=", 
        RowBox[{"Module", "[", 
          RowBox[{\({i, j, k = Length[X]}\), ",", "\[IndentingNewLine]", 
            
            RowBox[{\(P = Table["\<\>", {k}, {k}]\), ";", 
              "\[IndentingNewLine]", \(For[i = 1, 
                i \[LessEqual] 
                  k, \(i++\), \ \ \(P\_\(\(\[LeftDoubleBracket]\)\(i, 1\)\(\
\[RightDoubleBracket]\)\) = 
                    f[X\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)];\)\ ]\), ";", "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
                
                RowBox[{\(i = 1\), ",", \(i \[LessEqual] \ k - 1\), 
                  ",", \(i++\), ",", "\[IndentingNewLine]", 
                  RowBox[{
                    RowBox[{"For", "[", 
                      
                      RowBox[{\(j = 1\), ",", \(j \[LessEqual] \ i\), 
                        ",", \(j++\), ",", "\[IndentingNewLine]", 
                        RowBox[{
                          
                          RowBox[{\(P\_\(\(\[LeftDoubleBracket]\)\(i + 1, 
                                j + 1\)\(\[RightDoubleBracket]\)\)\), "=", 
                            " ", 
                            
                            RowBox[{\(1\/\(X\_\(\(\[LeftDoubleBracket]\)\(i + \
1\)\(\[RightDoubleBracket]\)\) - 
                                    X\_\(\(\[LeftDoubleBracket]\)\(i - j + \
1\)\(\[RightDoubleBracket]\)\)\)\), 
                              RowBox[{"Det", "[", 
                                RowBox[{"(", GridBox[{
                                      {\(P\_\(\(\[LeftDoubleBracket]\)\(i, 
                                        j\)\(\[RightDoubleBracket]\)\)\), \(X\
\_\(\(\[LeftDoubleBracket]\)\(i - j + 1\)\(\[RightDoubleBracket]\)\) - z\)},
                                      {\(P\_\(\(\[LeftDoubleBracket]\)\(i + 
                                        1, j\)\(\[RightDoubleBracket]\)\)\), \
\(X\_\(\(\[LeftDoubleBracket]\)\(i + 1\)\(\[RightDoubleBracket]\)\) - z\)}
                                      }], ")"}], "]"}]}]}], ";"}]}], " ", 
                      "]"}], ";"}]}], "  ", "]"}], ";"}]}], "  ", "]"}]}], 
      ";"}]], "Input"],

