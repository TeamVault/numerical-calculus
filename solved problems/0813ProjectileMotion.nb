    \(\(Print["\<The Runge-Kutta solution to the D. E.\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<   y'' = \>", 
        f\_2[t, y, v]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<With the initial conditions\>"];\)\ \), \
"\[IndentingNewLine]", 
    \(\(Print["\< y(\>", a, "\<) = \>", Y\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<y'(\>", a, "\<) = \>", V\_0];\)\), "\[IndentingNewLine]", 
    \(\)}], "Input"],

Cell[BoxData[{
    RowBox[{\(g = 32;\), " "}], "\n", 
    RowBox[{\(k = \(4\/100\) m;\), " "}], "\n", 
    RowBox[{\(V\_0 = 300;\), " "}], "\n", 
    RowBox[{\(Y\_0 = 0;\), " "}], "\n", 
    RowBox[{\(Clear[v, t]\), " "}], "\n", 
    RowBox[{
      RowBox[{"de", "=", 
        RowBox[{
          RowBox[{"m", " ", 
            RowBox[{
              SuperscriptBox["v", "\[Prime]",
                MultilineFunction->None], "[", "t", "]"}]}], 
