      RowBox[{
        StyleBox["M",
          FontWeight->"Bold"], "=", 
        RowBox[{"[", 
          RowBox[{
            StyleBox["A",
              FontWeight->"Bold"], ",", 
            StyleBox["0",
              FontWeight->"Bold"]}], "]"}]}]]],
  " is the augmented matrix then  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"rank", 
          RowBox[{"(", 
            StyleBox["A",
              FontWeight->"Bold"], ")"}]}], " ", "=", 
        RowBox[{"rank", 
