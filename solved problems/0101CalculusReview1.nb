        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox[\(x\_0\),
          FontSize->12]}]]],
  " ,  then  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox[\(x\_n\),
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
