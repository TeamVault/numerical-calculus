}], "Text"],

Cell[TextData[{
  StyleBox["Example 1.",
    FontFamily->"Times-Bold",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  StyleBox["  Consider the functions  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((x)\)\  = \ x\^4 + 2  x\^5\)]],
  "  ",
  StyleBox["and  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(g \((x)\)\  = \ x\^3\)]],
  StyleBox[".   Show that ",
    FontFamily->"Times-Roman"],
  "  ",
  Cell[BoxData[
      RowBox[{\(f \((h)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox["O",
            FontWeight->"Bold"], \((g \((h)\))\)}]}]]],
  StyleBox[",  over the interval  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\)\(|\)\(\ \)\(\(\[LessEqual]\)\(\ \)\(1\)\)\)\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  StyleBox["Since  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\^4\)\(|\)\(\ \)\(\(\[LessEqual]\)\(\ \
\)\(\(|\)\(h\^3\)\(|\)\)\)\)\)]],
  "  ",
  StyleBox["and  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\^5\)\(|\)\(\(\[LessEqual]\)\(\(|\)\(h\^3\)\(|\)\)\)\)\)]],
  StyleBox["  for  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\)\(|\)\(\(\[LessEqual]\)\(1\)\)\)\)]],
  " ",
  StyleBox[" it follows that  \n\n\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\^4 + 
          2  h\^5\)\(|\)\(\ \)\(\(\[LessEqual]\)\(\ \ \
\)\(\(|\)\(h\^4\)\(|\)\(+\(\(|\)\(2 
                   h\^5\)\(|\)\(\ \)\(\(\[LessEqual]\)\(\ \
\)\(3\)\)\(|\)\(h\^3\)\(|\)\)\)\)\)\)\)]],
  ".  \n\nIf we set  ",
  Cell[BoxData[
      \(\(\(\ \)\(C\&^\  = 3\)\)\)]],
  "  and  ",
  Cell[BoxData[
      \(\[Delta] = 1\)]],
  "  then we see that",
  StyleBox[" ",
    FontFamily->"Times-Roman"],
  "  ",
  Cell[BoxData[
      RowBox[{\(f \((h)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox["O",
            FontWeight->"Bold"], \((g \((h)\))\)}]}]]],
  StyleBox[",  over the interval  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(|\)\(h\)\(|\)\(\ \)\(\(\[LessEqual]\)\(\ \)\(1\)\)\)\)]],
  ".     "
}], "Text"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Plot[{Abs[h\^4], Abs[h\^3]}, {h, \(-1\), 1}, 
        PlotStyle \[Rule] {Red, Blue}, 
        PlotRange \[Rule] All];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<|\!\(h\^4\)| \[LessEqual] |\!\(h\^3\)|   for   0 \
\[LessEqual] |h| \[LessEqual] 1\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<y = |\!\(h\^3\)|  is the blue curve\>\""];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print[\*"\"\<y = |\!\(h\^4\)|  is the red  curve\>\""];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Plot[{Abs[2  h\^5], 2  Abs[h\^3]}, {h, \(-1\), 1}, 
        PlotStyle \[Rule] {Red, Blue}, 
        PlotRange \[Rule] All];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<|2 \!\(h\^5\)| \[LessEqual] 2|\!\(h\^3\)|   for   0 \
\[LessEqual] |h| \[LessEqual] 1\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<y = 2 |\!\(h\^3\)|  is the blue curve\>\""];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print[\*"\"\<y = |2 \!\(h\^5\)|  is the red  curve\>\""];\)\ \), "\
\[IndentingNewLine]", 
    \(\)}], "Input"],

Cell["Combine the above to obtain the desired result.  ", "Text"],

Cell[BoxData[{
    \(\(f[x_]\  = \ x\^4 + 2  x\^5;\)\ \), "\[IndentingNewLine]", 
    \(\(g[x_]\  = \ x\^3;\)\ \), "\[IndentingNewLine]", 
    \(\(C\&^\  = \ 3;\)\ \), "\[IndentingNewLine]", 
    \(\(\[Delta]\  = \ 1;\)\), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Plot[{Abs[f[h]], C\&^\ Abs[g[h]]}, {h, \(-\[Delta]\), \[Delta]}, 
        PlotStyle \[Rule] {Red, Blue}, 
        PlotRange \[Rule] All];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = f[x] = \>", 
        f[h], "\<  is the red curve\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = g[x] = \>", 
        g[h], "\<       is the blue curve\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\< f[h]  =  O(g[h])   for   0 \[LessEqual] |h| \[LessEqual] \
\>", \[Delta]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Because\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<|f[h]| \[LessEqual] \!\(C\&^\) |g[h]|   for   0 \
\[LessEqual] |h| \[LessEqual] \[Delta]\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<choose  \!\(C\&^\) = \>\"", 
        C\&^, "\<  and  \[Delta] = \>", \[Delta], "\<  and get\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<|\>", f[h], "\<| \[LessEqual] \>", C\&^\ , "\<|\>", 
        g[h], "\<|   for   0 \[LessEqual] |h| \[LessEqual] \>", \[Delta]];\)\ \
\), "\[IndentingNewLine]", 
    \(\(Print["\<Therefore\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\< \>", f[h], "\< = O(\>", g[h], "\<)\>"];\)\ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["\tThe ",
    FontFamily->"Times-Roman"],
  StyleBox["big Oh",
    FontFamily->"Times-Roman",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" notation provides a useful way of describing the rate of growth \
of a function in terms of well-known elementary functions (",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{\(x\^n\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
