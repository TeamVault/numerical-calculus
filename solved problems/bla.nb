Cell[BoxData[
    \(l[X_, \ i_, \ x_] := 
      Product[If[j \[Equal] i, \ 
          1, \ \((x - X[\([j]\)])\)/\((X[\([i]\)] - 
                X[\([j]\)])\)], \ {j, \ \(Dimensions[
              x]\)[\([1]\)]}]\)], "Input"],

Cell[BoxData[
    \(L[X_, \ x_, \ f_] := 
      Sum[l[X, \ i, \ x]*
          f[x[\([i]\)]], \ {i, \ \(Dimensions[x]\)[\([1]\)]}]\)], "Input"],

Cell[BoxData[
    \(f[x_] := Abs[x]\)], "Input"],

Cell[BoxData[
    \(Y[n_] := Table[\(-1\) + 2  i/n, \ {i, 0, n}]\)], "Input"],

Cell[BoxData[
    \(Ce[n_] := 
      Table[N[Cos[\((2  i + 1)\)*\(Pi/2\)/n]], \ {i, 0, \ n - 1}]\)], "Input"],

