Cell[TextData[{
  StyleBox["Background. Lagrange form of the Remainder.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  The Lagrange form of the error is  ",
  Cell[BoxData[
      RowBox[{\(R[x, c]\), " ", "=", " ", 
        RowBox[{
          FractionBox[
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((21)\),
                  Derivative],
                MultilineFunction->None], "[", "c", 
              "]"}], \(21!\)], \(x\^21\)}]}]]],
  "  where  c  is known to exist and lies somewhere between  0  and  x."
}], "Text"],

Cell[BoxData[{\(Clear[c];\), "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{\(R[x_, c_]\), " ", "=", " ", 
          RowBox[{
            FractionBox[
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((11)\),
                    Derivative],
                  MultilineFunction->None], "[", "c", 
                "]"}], \(11!\)], \(x\^11\)}]}], ";"}], 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<The Lagrange form of the remainder is\>", "\<\n\>"];\),
       " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<R[x,c] = \!\(
\(\*SuperscriptBox[\(f\), 
TagBox[\((11)\),
Derivative],
MultilineFunction->None]\)[c]\/\(11!\)\)\!\(x\^11\) where c lies somewhere \
between 0 and x.\>\"", ",", "\"\<\\n\>\""}], "]"}], ";"}], 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<R[x,c] = \>", Together[R[x, c]]\ ];\), " "}]}], "Input"],

Cell[TextData[{
  "First we need to bound the size of the term  ",
  Cell[BoxData[
