    \(\(Print["\<S = \>", 
        Sign[X\_\(\(\[LeftDoubleBracket]\)\(k + \
1\)\(\[RightDoubleBracket]\)\)], \@\(\[Sum]\+\(j = k + 1\)\%n\(("\<x\>"\_j)\)\
\^2\), "\< = \>", S, "\< = \>", 
        NumberForm[N[S], 10]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<R = \>", \@\(\(("\<x\>"\_\(k + 1\) + "\<S\>")\)\ "\<2 \
S\>"\), "\< = \>", R, "\< = \>", NumberForm[N[R], 10]\ ];\)\ \)}], "Input"],

Cell[TextData[{
  StyleBox["The constants  ",
    FontFamily->"Times New Roman"],
