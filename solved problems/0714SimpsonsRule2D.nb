Cell[BoxData[{
    \(\(Clear[c, d];\)\), "\[IndentingNewLine]", 
    \(\(c[x_] = x\^2;\)\), "\n", 
    \(\(d[x_] = \@\(8  x\);\)\), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`FilledPlot`\>"];\)\), "\[IndentingNewLine]", 
