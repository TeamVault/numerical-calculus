Cell[TextData[{
  StyleBox["Then the vector  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        StyleBox["Q",
          FontWeight->"Bold"], " ", "=", " ", 
        RowBox[{
          StyleBox["V",
            FontWeight->"Bold"], " ", "-", " ", 
          RowBox[{"c", " ", 
            StyleBox["W",
              FontWeight->"Bold"]}]}]}]]],
  "  ",
  StyleBox[" is formed",
    FontFamily->"Times New Roman"]
}], "Text"],

Cell[BoxData[{
    \(\(Q\  = \ V - c\ W;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q = V - c W\>"\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q = \>", MatrixForm[V], "\< - (\>", c, "\<)\>", \ 
        MatrixForm[W]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(If[c \[LessEqual] 0, 
        Print["\<Q = \>", MatrixForm[V], "\< + \>", \(-c\), \ 
