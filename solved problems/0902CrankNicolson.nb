Cell[TextData[{
  StyleBox["Crank-Nicolson Method",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["\n\n\t",
    FontFamily->"Times New Roman",
    FontWeight->"Bold"],
  StyleBox["An implicit scheme, invented b  ",
    FontFamily->"Times New Roman"],
  StyleBox[ButtonBox["John Crank",
    ButtonData:>{
      URL[ 
      "http://www-groups.dcs.st-and.ac.uk/~history/Mathematicians/Crank.html"]\
, None},
    ButtonStyle->"Hyperlink"],
    FontFamily->"Times New Roman"],
  StyleBox[" (1916-) and  ",
    FontFamily->"Times New Roman"],
  StyleBox[ButtonBox["Phyllis Nicolson",
    ButtonData:>{
      URL[ 
      "http://www-groups.dcs.st-and.ac.uk/~history/Mathematicians/Nicolson.\
html"], None},
    ButtonStyle->"Hyperlink"],
    FontFamily->"Times New Roman"],
  StyleBox[" (1917-1968), is based on numerical approximations for solutions \
at the point  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\((x, t + k\/2)\)\)]],
  StyleBox[" ",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["that lies between the rows in the grid.  Specifically, the \
approximation used for  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(u\_t\) \((x, t + k\/2)\)\)]],
  "  ",
  StyleBox["is obtained from the central-difference formula, \n\n\t\t\t",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(u\_t\) \((x, 
            t + k\/2)\)\  = \ \(u \((x, t + k)\) - u \((x, t)\)\)\/k\  + \ 
          O \((k\^2)\)\)]],
  StyleBox[". ",
    FontFamily->"Times New Roman"]
}], "Text"],

Cell[TextData[{
