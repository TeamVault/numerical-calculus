  FontColor->RGBColor[1, 0, 1]],

Cell["Enter the n points.", "Text"],

Cell[BoxData[{
    RowBox[{\(XY\  = \ {{\(-4.5\), \ 0.7}, \ {\(-3.2\), \ 2.3}, {\(-1.4\), \ 
              3.8}, {\ 0.8, \ 5.0}, {\ 2.5, \ 5.5}, {\ 4.1, \ 5.6}};\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(n = Length[XY];\), " "}], "\[IndentingNewLine]", 
    RowBox[{\(X = \(Transpose[XY]\)\_\(\(\[LeftDoubleBracket]\)\(1\)\(\
\[RightDoubleBracket]\)\);\), " "}], "\[IndentingNewLine]", 
    RowBox[{\(Y = \(Transpose[XY]\)\_\(\(\[LeftDoubleBracket]\)\(2\)\(\
\[RightDoubleBracket]\)\);\), " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<{(\!\(x\_i\)\!\(\*
StyleBox[\",\",\nFontFamily->\"Times New Roman\",\nFontSize->12,\n\
FontWeight->\"Plain\",\nFontSlant->\"Plain\",\nFontColor->GrayLevel[0],\n\
FontVariations->{\"Underline\"->False,\n\
\"StrikeThrough\"->False}]\)\!\(y\_i\))\!\(\(}\_\(i = 1\)\%n\)\) = \>\"", ",",
             "XY"}], "]"}], ";"}], " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print[\*"\"\<    {\!\(x\_i\)\!\(\(}\_\(i = 1\)\%n\)\) = \>\"", 
          X];\), " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print[\*"\"\<    {\!\(y\_i\)\!\(\(}\_\(i = 1\)\%n\)\) = \>\"", 
          Y];\), " "}], "\[IndentingNewLine]", \(Print["\<         n = \>", 
        n];\)}], "Input"],

Cell["Enter the degree of the polynomial.", "Text"],

Cell[BoxData[{
    \(\(deg = 3;\)\ \), "\[IndentingNewLine]", 
    \(\(Clear[c, P];\)\ \), "\[IndentingNewLine]", 
    \(\(P = Table[x\^j, {j, 0, deg}];\)\ \), "\[IndentingNewLine]", 
    \(\(P\&^[x_] := 
        Prepend[Table[x\^j, {j, 1, deg}], 
          Table[1, {i, n}]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Construct the polynomial of degree = \>", 
        deg];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<p[x] = \>", \[Sum]\+\(j = 1\)\%\(deg + 1\)\(c\_j\) 
            P\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)];\)\
\)}], "Input"],

Cell[TextData[{
  "Construct the matrices  ",
  Cell[BoxData[
      RowBox[{
        StyleBox["F",
          FontWeight->"Bold"], 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], 
        StyleBox[" ",
          FontWeight->"Bold"], 
        SuperscriptBox[
          StyleBox["F",
            FontWeight->"Bold"], "T"], 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], " ", 
        RowBox[{
          StyleBox["and",
            FontFamily->"Times New Roman",
            FontSize->12,
            FontWeight->"Plain",
            FontSlant->"Plain",
            FontColor->GrayLevel[0],
            FontVariations->{"Underline"->False,
            "StrikeThrough"->False}], "   ", 
          SuperscriptBox[
            StyleBox["F",
              FontWeight->"Bold"], "T"], 
          StyleBox["F",
            FontWeight->"Bold"]}]}]]],
  ", and the vector  ",
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{
          SuperscriptBox[
            StyleBox["F",
              FontWeight->"Bold"], "T"], 
          StyleBox["Y",
            FontWeight->"Bold"]}]}]]]
}], "Text"],

Cell[BoxData[{
    \(\(Y\&^ = 
        Table[{Y\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\)},\
 {i, n}];\)\), "\[IndentingNewLine]", 
    \(\(F\&^ = \ Transpose[P\&^[X]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<  Y = \>", 
        MatrixForm[Y\&^]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\ "\< F = \>", MatrixForm[F\&^]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\ \ \*"\"\<\!\(F\^T\) = \>\"", 
        MatrixForm[Transpose[F\&^]]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<We will use the following matrix and \
vector.\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(F\^T\)F = \>\"", 
        MatrixForm[Transpose[F\&^] . F\&^]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(F\^T\)Y = \>\"", 
        MatrixForm[Transpose[F\&^] . Y\&^]];\)\)}], "Input"],

Cell[TextData[{
  "Solve the linear system  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          SuperscriptBox[
            StyleBox["F",
              FontWeight->"Bold"], "T"], 
          StyleBox["F",
            FontWeight->"Bold"], " ", 
          StyleBox["C",
            FontWeight->"Bold"]}], " ", "=", " ", 
        RowBox[{
          SuperscriptBox[
            StyleBox["F",
              FontWeight->"Bold"], "T"], " ", 
          StyleBox["Y",
            FontWeight->"Bold"]}]}]]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c];\)\), "\[IndentingNewLine]", 
    \(\(C\&^ = Table[c\_i, {i, deg + 1}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Solve\>", "\<\n\n\>", \*"\"\<\!\(F\^T\)F C = \!\(F\
\^T\)F\>\""];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(F\^T\)F = \>\"", 
        MatrixForm[Transpose[F\&^] . F\&^], MatrixForm[C\&^], "\< = \>", 
        MatrixForm[Transpose[F\&^] . Y\&^]];\)\), "\[IndentingNewLine]", 
    \(\(c = \ 
        LinearSolve[Transpose[F\&^] . F\&^, 
          Transpose[F\&^] . Y\&^];\)\ \), "\[IndentingNewLine]", 
    \(\(c = Flatten[c];\)\), "\[IndentingNewLine]", 
    \(Print["\<Get\>"]\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<{\!\(c\_j\)}\>\""\_\(j \[Equal] 1\)\%\(deg + 1\
\), "\< = \>", c];\)\ \)}], "Input"],

Cell["Construct the polynomial.", "Text"],

Cell[BoxData[{
    \(\(p[
          x_]\  = \ \[Sum]\+\(j = 1\)\%\(deg + 1\)\(c\_\(\(\
\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)\) 
            P\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\);\)\)\
, "\[IndentingNewLine]", 
    \(\(Print["\<p[x] = \>", p[x]];\)\)}], "Input"],

