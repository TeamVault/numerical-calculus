}], "Text"],

Cell[BoxData[
    \(\(\(PowerMeth[A0_, V0_, \[Epsilon]_, 
          m_] := \[IndentingNewLine]Module[{A\  = \ N[A0], c1, count, 
            err, \[Lambda], \[Lambda]0, X\ , X0\  = N[V0], 
            Y}, \[IndentingNewLine]maxsize[W_] := 
            Module[{w\  = \ Sort[W], msize}, \[IndentingNewLine]If[\ 
                Abs[w\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\[RightDoubleBracket]\
\)\)] \[GreaterEqual] 
                  Abs[w\_\(\(\[LeftDoubleBracket]\)\(1\)\(\
\[RightDoubleBracket]\)\)], \[IndentingNewLine]msize\  = \ 
                  w\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\[RightDoubleBracket]\)\
\), \[IndentingNewLine]msize\  = \ 
                  w\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\
\)]; \ \[IndentingNewLine]Return[msize];\ ]; \ \[IndentingNewLine]norm[
              V_] := \@\(V . V\); \ \[IndentingNewLine]\[Lambda]0\  = \ 
            0; \ \[IndentingNewLine]count\  = \ 
            0; \ \[IndentingNewLine]While[\ 
            count \[LessEqual] m, \[IndentingNewLine]count = 
              count + 1; \ \[IndentingNewLine]Y = \ 
              A . X0; \ \[IndentingNewLine]\[Lambda]\  = \ 
              maxsize[Y]; \ \[IndentingNewLine]X\  = 
              1\/\[Lambda]\ Y; \ \[IndentingNewLine]Print[
              NumberForm[\[Lambda], 6], "\< , \>", 
              PaddedForm[X, {6, 6}]]; \ \[IndentingNewLine]err\  = \ 
              Max[{\ Abs[\[Lambda] - \[Lambda]0], \ 
                  norm[X - X0]}]; \ \[IndentingNewLine]If[\ 
              err < \[Epsilon], \ 
              Return[{\[Lambda], X}]\ ]; \ \ \[IndentingNewLine]X0\  = \ 
              X; \ \[IndentingNewLine]\[Lambda]0\  = \ \[Lambda];\ \ ]; \ \
\[IndentingNewLine]Return[{\[Lambda], X}];\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->True],

Cell[TextData[{
