            x\^k_\  \[Rule] \ 
              x\^\(k/2\)]; \[IndentingNewLine]Print["\<f\>"\_v, "\<[x] = \
Replace[\>", "\<f\>"\_\(v - 1\), "\<[x]\>", "\<f\>"\_\(v - 1\), "\<[-x]\>", \
\*"\"\<], \!\(x\^2\) \[Rule] x]\>\""]; \[IndentingNewLine]Print["\<f\>"\_v, "\
\<[x] = Replace[(\>", f\_\(v - 1\)[x], "\<)(\>", 
          f\_\(v - 1\)[\(-x\)], \*"\"\<)], \!\(x\^2\) \[Rule] x]\>\""]; \
\[IndentingNewLine]Print["\<f\>"\_v, "\<[x] = Replace[(\>", 
          Expand[f\_\(v - 1\)[x] 
              f\_\(v - 1\)[\(-x\)]], \*"\"\<)], \!\(x\^2\) \[Rule] x]\>\""]; \
\[IndentingNewLine]Print["\<f\>"\_v, "\<[x] = \>", 
          f\_v[x]\ ]; \[IndentingNewLine]c\  = 
          CoefficientList[f\_v[x], x]; \[IndentingNewLine]n = 
