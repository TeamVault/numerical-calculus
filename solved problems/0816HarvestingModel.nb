}], "Text"],

Cell[TextData[{
  StyleBox["Case (ii)  If  ",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(a\^2 > 4\ b\ k\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" ",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" there are two stationary solutions  ",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(\(x\_1\) \((t)\) = \(a - \@\(a\^2 - 4\ b\ k\)\)\/\(2\ b\)\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  and  ",
  Cell[BoxData[
      \(\(x\_2\) \((t)\) = \(a + \@\(a\^2 - 4\ b\ k\)\)\/\(2\ b\)\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[".",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\n\tWhen  ",
  Cell[BoxData[
      \(a\^2 > 4\ b\ k\)]],
  ",  the differential equation has the form  ",
  Cell[BoxData[
      \(dx\/dt\  = \ a\ x\  - \ b\ x\^2\  - \ k\)]],
  "  and the solution is  \n\n\t\t\t",
  Cell[BoxData[
      \(x \((t)\) = 
        a\/\(2  b\)\  + \(\@\(a\^2 - 4\ b\ k\)\/\(2  b\)\) \((1 - 
                2\/\(1 + c\ \[ExponentialE]\^\(\@\(a\^2 - 4\ b\ k\)\ t\)\))\)\
\)]],
  ". "
}], "Text"],

