Cell[TextData[{
  StyleBox["Theorem 20.  (Taylor's Polynomial with Integral Remainder).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that the function  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  and its derivatives  ",
  Cell[BoxData[
      RowBox[{\(f' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f'' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], "...", " ", 
        StyleBox[",",
          FontFamily->"Times New Roman"], 
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((n + 1)\),
              Derivative],
            MultilineFunction->None], \((x)\)}]}]]],
  "  are all continuous on  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ".  If  both  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  and  ",
  Cell[BoxData[
      \(x = x\_0 + h\)]],
  "  lie in the interval  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  then  ",
  Cell[BoxData[
      \(f \((x)\)\  = \ \(P\_n\) \((x)\)\  + \ O \((h\^\(n + 1\))\)\)]],
  ",  where\t\n\n\t",
  Cell[BoxData[
      RowBox[{\(\(P\_n\) \((x)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox[
            UnderoverscriptBox["\[Sum]", 
              StyleBox[\(k = 0\),
                FontSize->10], 
              StyleBox["n",
                FontSize->10]],
            FontSize->18], " ", 
          RowBox[{
            FractionBox[
              StyleBox[
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((k)\),
                      Derivative],
                    MultilineFunction->None], \((x\_0)\)}],
                FontSize->12], 
              StyleBox[\(k!\),
                FontSize->12]], \(\((x - x\_0)\)\^k\)}]}]}]]],
  "  \n\t\nis the n-th degree Taylor polynomial expanded about  ",
  Cell[BoxData[
      \(x\_0\)]],
  ".  \n\n\t",
  Cell[BoxData[
      \(f \((x)\) = \(P\_n\) \((x)\) + \(R\_n\) \((x)\)\)]],
  "\n\nThe integral form of the remainder is  \n\n\t",
  Cell[BoxData[
      RowBox[{\(\(R\_n\) \((x)\)\), "=", 
        RowBox[{\(1\/\(n!\)\), 
          RowBox[{\(\[Integral]\_\(x\_0\)\%x\), 
            RowBox[{\(\((x - t)\)\^n\), 
              SuperscriptBox["f", 
                TagBox[\((n + 1)\),
                  Derivative],
                
                MultilineFunction->
                  None], \((t)\), \(\[DifferentialD]t\)}]}]}]}]]],
  ".  "
}], "Text",
  CellTags->"Taylor's Polynomial with Integral Remainder"],

Cell[TextData[{
  StyleBox["Theorem 21.  (",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["Taylor's Polynomial with O(",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(TraditionalForm\`h\^\(n + 1\)\)],
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[")  remainder",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[").",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that the function  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  and its derivatives  ",
  Cell[BoxData[
      RowBox[{\(f' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f'' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], "...", " ", 
        StyleBox[",",
          FontFamily->"Times New Roman"], 
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((n + 1)\),
              Derivative],
            MultilineFunction->None], \((x)\)}]}]]],
  "  are all continuous on  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ".  If  both  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  and  ",
  Cell[BoxData[
      \(x = x\_0 + h\)]],
  "  lie in the interval  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  then  \n\t\n\t",
  Cell[BoxData[
      RowBox[{\(\(P\_n\) \((x)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox[
            UnderoverscriptBox["\[Sum]", 
              StyleBox[\(k = 0\),
                FontSize->10], 
              StyleBox["n",
                FontSize->10]],
            FontSize->18], " ", 
          RowBox[{
            FractionBox[
              StyleBox[
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((k)\),
                      Derivative],
                    MultilineFunction->None], \((x\_0)\)}],
                FontSize->12], 
              StyleBox[\(k!\),
                FontSize->12]], \(\((x - x\_0)\)\^k\)}]}]}]]],
  "  \n\t\nis the n-th degree Taylor polynomial expanded about  ",
  Cell[BoxData[
      \(x\_0\)]],
  ".  We write    \n\n\t",
  Cell[BoxData[
      \(f \((x)\)\  = \ \(P\_n\) \((x)\)\  + \ O \((h\^\(n + 1\))\)\)]],
  "\n\nIn the  ",
  Cell[BoxData[
      \(O \((h\^\(n + 1\))\)\)]],
  "  remainder we are using  ",
  Cell[BoxData[
      \(h = x - x\_0\)]],
  " in Lagrange's formula for the remainder. \n\n\t",
  Cell[BoxData[
      RowBox[{\(\(R\_n\) \((x)\)\), "=", 
        RowBox[{
          FractionBox[
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((n + 1)\),
                  Derivative],
                
                MultilineFunction->None], \((c)\)}], \(\((n + 
                  1)\)!\)], \(\((x - x\_0)\)\^\(n + 1\)\)}]}]]],
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{"=", 
          RowBox[{
            FractionBox[
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((n + 1)\),
                    Derivative],
                  
                  MultilineFunction->None], \((c)\)}], \(\((n + 
                    1)\)!\)], \(\((h)\)\^\(n + 1\)\)}]}]}]]],
  Cell[BoxData[
      \(\(\(\ \)\(\(+\ O\) \((h\^\(n + 1\))\)\)\)\)]],
  "\n\nwhere ",
  Cell[BoxData[
      \(c\)]],
  " depends on ",
  Cell[BoxData[
      \(x\)]],
  " and lies somewhere between  ",
  Cell[BoxData[
      RowBox[{\(x\_0\), "  ", 
        StyleBox["and",
          FontFamily->"Times New Roman"], "  ", "x"}]]],
  ".  \n\n",
  StyleBox["Remark.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  "  Often times ",
  Cell[BoxData[
      FractionBox[
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((n + 1)\),
              Derivative],
            MultilineFunction->None], \((c)\)}], \(\((n + 1)\)!\)]]],
  " is not important and we think of it \"like a constant\" and  ",
  Cell[BoxData[
      \(h\^\(n + 1\)\)]],
  "  is the signal feature in the remainder."
}], "Text",
  CellTags->"Taylor's Polynomial with O(h^n+1) remainder"],

