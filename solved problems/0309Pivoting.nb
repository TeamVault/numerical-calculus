        n = Length[
            A]; \ \[IndentingNewLine]Print["\<Use 'No Pivoting' to solve\>"]; \
\ \[IndentingNewLine]Print[MatrixForm[A0], MatrixForm[vars], "\< = \>", 
          MatrixForm[
            B]]; \[IndentingNewLine]Print["\<Use the augmented matrix  \
[A,B]\>"]; \[IndentingNewLine]Print[
          MatrixForm[
            Prepend[A, 
              Append[Table["\<x\>"\_i, {i, 1, 
                    n}]\ , "\<\>"]]]]; \[IndentingNewLine]For[\ p = 1, \ 
          p \[LessEqual] 
            n - 1, \ \(p++\), \ \[IndentingNewLine]Print["\<Pivot element is  \
a\>", "\<\>"\_\(p, p\), "\< = \>", 
            A\_\(\(\[LeftDoubleBracket]\)\(p, p\)\(\[RightDoubleBracket]\)\), \
"\<\n\>"]; \[IndentingNewLine]mult\  = \ 
            A\_\(\(\[LeftDoubleBracket]\)\(p\)\(\[RightDoubleBracket]\)\)\/A\_\
\(\(\[LeftDoubleBracket]\)\(p, p\)\(\[RightDoubleBracket]\)\); \ \
\[IndentingNewLine]For[\ i = p + 1, \ 
            i \[LessEqual] 
              n, \ \(i++\), \ \
\[IndentingNewLine]A\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\
\)\  = \ A\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\)\  - \ 
                mult*A\_\(\(\[LeftDoubleBracket]\)\(i, p\)\(\
\[RightDoubleBracket]\)\)\ \ ]; \ \[IndentingNewLine]Print[
            MatrixForm[
              Prepend[A, 
                Append[Table["\<x\>"\_i, {i, 1, 
                      n}]\ , "\<\>"]]]];\ \ ]; \
\[IndentingNewLine]BackSubstitution[A]; \ \[IndentingNewLine]Return[
          X];\ ]\)\(\ \)\)\)}], "Input"],

