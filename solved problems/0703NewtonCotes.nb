Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(f[x_] = 
        1 + \[ExponentialE]\^\(-x\)\ Sin[
              4\ x];\)\ \), "\[IndentingNewLine]", 
    \(\(h = 0.5;\)\ \), "\[IndentingNewLine]", 
    \(x\_0 = 0.0; x\_1 = x\_0 + h; x\_2 = x\_0 + 2  h; x\_3 = x\_0 + 3  h; 
    x\_4 = x\_0 + 4  h;\), "\n", 
    \(f\_0 = f[x\_0]; f\_1 = f[x\_1]; f\_2 = f[x\_2]; f\_3 = f[x\_3]; 
    f\_4 = f[x\_4];\), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Using (8) the Trapezoidal Rule: \n\n\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\[Integral]\_0.0\%0.5\ f[x] \[DifferentialD]x\)]],
  " ",
  Cell[BoxData[
      \(\(\(\[TildeTilde]\)\(\ \)\(\(h\/2\) \((f\_0\  + \ f\_1)\)\)\)\)]],
  "  \n\t\t\t",
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(\(0.5\/2\) \((1.00000 + 1.55152)\)\)\)\)\)]],
  "  \n\t\t\t",
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(0.25 \((2.55152)\)\)\)\)\)]],
  "  \n\t\t\t",
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(0.637879\)\)\)\)]],
  "  \n\t\t\t\n",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s Computation is: "
}], "Text"],

Cell[BoxData[{
    \(\(Print["\<The Trapezoidal Rule with  h = \>", 
        h];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_0\%0.5\)f[x]\[DifferentialD]x \
\[TildeTilde] \>\"", \*"\"\<\!\(h\/2\)(\!\(f\_0\) + \!\(f\_1\)) = \>\"", \
\(h\/2\) \((f\_0\  + \ f\_1)\)];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Using (9) Simpson\[CloseCurlyQuote]s Rule: \n\n\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\[Integral]\_0.0\%1.0\ f[x] \[DifferentialD]x\)]],
  Cell[BoxData[
      \(\(\(\ \)\(\(\[TildeTilde]\)\(\ \)\(\(h\/3\) \((f\_0\  + \ 
              4  f\_1\  + \ f\_2)\)\)\)\)\)]],
  "  \n\t\t\t",
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(\(0.5\/3\) \((1.00000\  + \ 4*1.55152\  + \ 
              0.72159)\)\)\)\)\)]],
  "  \n\t\t\t",
  Cell[BoxData[
