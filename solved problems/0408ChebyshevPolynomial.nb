Cell[TextData[{
  StyleBox["Algorithm (Lagrange-",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[ButtonBox["Chebyshev Approximation",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/ChebyshevApproximationFormula.html"],
       None},
    ButtonStyle->"Hyperlink"],
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  StyleBox[").",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  The Chebyshev approximation polynomial  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(P\_n\) \((x)\)\)]],
  StyleBox["  of  degree  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(\(\[LessEqual]\)\(n\)\)\)]],
  "  ",
  StyleBox["for  f(x)  over  [",
    FontFamily->"Times New Roman"],
  StyleBox["-",
    FontFamily->"Times New Roman",
    FontSize->14],
  StyleBox["1,1]  can be written as a sum of  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \({\(T\_j\) \((x)\)}\)]],
  StyleBox[":  \n",
    FontFamily->"Times New Roman"],
  StyleBox["\n",
    FontFamily->"Times New Roman",
    FontSize->6],
  "\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f \((x)\)\), " ", 
          StyleBox["\[TildeTilde]",
            FontSize->14], " ", \(\(P\_n\) \((x)\)\)}], " ", "=", 
        " ", \(\[Sum]\+\(j = 0\)\%n\( c\_j\) \(T\_j\) \((x)\)\)}]]],
  ".  ",
  StyleBox["\n",
    FontFamily->"Times New Roman"],
  StyleBox["\n",
    FontFamily->"Times New Roman",
    FontSize->6],
  StyleBox["The coefficients  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \({c\_j}\)]],
  "  ",
  StyleBox["are computed with the formulas  \n\n\t",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(c\_0\  = \ 
        1\/\(n + 1\)\ \(\[Sum]\+\(k = 0\)\%n 
              f \((x\_k)\) \(T\_0\) \((x\_k)\)\)\)]],
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(1\/\(n + 1\)\ \(\[Sum]\+\(k = 0\)\%n 
              f \((x\_k)\)\)\)\)\)\)]],
  ",  \nand  \n\n\t",
  Cell[BoxData[
      \(c\_j\  = \ 
        2\/\(n + 1\)\ \(\[Sum]\+\(k = 0\)\%n 
              f \((x\_k)\) \(T\_j\) \((x\_k)\)\)\)]],
  Cell[BoxData[
      \(\(\(\ \)\(\(=\)\(\ \)\(2\/\(n + 1\)\ \(\[Sum]\+\(k = 0\)\%n 
              f \((x\_k)\) 
              cos \((j \(\( 2  k\  + \ 1\)\/\(2  n\  + \ 
                        2\)\) \[Pi])\)\)\)\)\)\)]],
  ",  \n\nfor  ",
  Cell[BoxData[
      \(j = 1, 2,  ... , n\)]],
  "  where  ",
  Cell[BoxData[
      \(x\_k = cos \((\(\(2  k + \ 1\)\/\(2  n\  + \ 2\)\) \[Pi])\)\)]],
  " for  ",
  Cell[BoxData[
      \(k = 0, 1, 2,  ... , n\)]],
  ".  "
}], "Text"],

