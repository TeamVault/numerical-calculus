Cell[TextData[StyleBox["Solution 2 (a).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(X = 1.0;\)\ \), "\[IndentingNewLine]", 
    \(\(H = 0.1;\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x] = \>", f[x]];\)\ \), "\[IndentingNewLine]", 
