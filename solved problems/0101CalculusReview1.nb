Cell[TextData[{
  StyleBox["Definition 1.  (Limit Definition).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is defined at all points in a deleted neighborhood of  ",
  Cell[BoxData[
      \(x\_0\)]],
  ",  i.e. to the left and right of  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  but not necessarily at  ",
  Cell[BoxData[
      \(x\_0\)]],
  ".  We write   \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["L",
          FontSize->12]}]]],
  ", \n\nand say that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  has the limit  L  as  x  approaches  ",
  Cell[BoxData[
      \(x\_0\)]],
  ",  provided that \" ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  " gets close to ",
  Cell[BoxData[
      \(L\)]],
  "\" when  \"",
  Cell[BoxData[
      \(x\)]],
  " gets close to ",
  Cell[BoxData[
      \(x\_0\)]],
  ".\"  In rigor we write   \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["L",
          FontSize->12]}]]],
  ", \n\t\nand say that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  has the limit  ",
  Cell[BoxData[
      \(L\)]],
  "  as  ",
  Cell[BoxData[
      \(x\)]],
  "  approaches  ",
  Cell[BoxData[
      \(x\_0\)]],
  ",  provided that for every  ",
  Cell[BoxData[
      \(\[Epsilon] > 0\)]],
  "  there exists a  ",
  Cell[BoxData[
      \(\[Delta] > 0\)]],
  "  such that  \n\n\t",
  Cell[BoxData[
      \(\(\(|\)\(f \((x)\)\)\) - L | \(\(<\)\(\[Epsilon]\)\)\)]],
  "  for all x  satisfying  ",
  Cell[BoxData[
      \(\(\(\(0\)\(<\)\)\(|\)\(x - 
          x\_0\)\(|\)\(\(<\)\(\[Delta]\)\)\(\ \)\)\)]],
  ".  "
}], "Text",
  CellTags->"Limit Definition"],

Cell[TextData[{
  StyleBox["Example 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      \(f \((x)\) = \(x\^2\  + \ 2\ x\  - \ 3\)\/\(x\  - \ 1\)\)]],
  ".  Show that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 1\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["4",
          FontSize->12]}]]],
  ".  Note that  ",
  Cell[BoxData[
      \(f \((1)\)\)]],
  "  is ",
  StyleBox["not",
    FontColor->RGBColor[1, 0, 1]],
  " defined, but the graph \"looks like every thing is o.k. !\""
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[f, x];\)\ \ \), "\n", 
    \(\(f[x_] = \(x\^2 + 2\ x - 3\)\/\(x - 1\)\ ;\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[f[x], {x, 0, 2}, 
        PlotStyle \[Rule] Red];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<f[1] = \>", f[1]\ ];\)\ \ \)}], "Input"],

Cell["\<\
Note both the numerator and denominator of f(x) evaluate to be zero, hence we \
have a zero divide by zero situation.  In this case, both numerator and \
denominator have a common factor of  (x - 1).\
\>", "Text"],

Cell[BoxData[{
    \(\(n\ \  = Factor[Numerator[f[x]]];\)\ \ \), "\n", 
    \(\(n1 = ReplaceAll[Numerator[f[x]], x \[Rule] 1];\)\ \ \), "\n", 
    \(\(d\ \  = Denominator[f[x]];\)\ \ \), "\n", 
    \(\(d1 = ReplaceAll[Denominator[f[x]], x \[Rule] 1];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", ToString[n]/ToString[d]];\)\ \ \), "\n", 
    \(\(Print["\<f[1] = \>", ToString[n1], "\</\>", 
        ToString[d1]];\)\ \ \)}], "Input"],

Cell["\<\
Simplify  f(x)  to remove the common factor and call this new function  g(x). \
 Now observe that the graphs of f(x) and g(x) \"appear to be similar.\"  \
\>", "Text"],

Cell[BoxData[{
    \(\(g[x_] = Simplify[f[x]];\)\ \ \), "\n", 
    \(\(Print["\<g[x] = \>", g[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<g[1] = \>", g[1]\ ];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(gr1 = 
        Plot[f[x], {x, 0, 2}, PlotStyle \[Rule] Red, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(gr2 = 
        Plot[g[x], {x, 0, 2}, PlotStyle \[Rule] Green, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(Show[GraphicsArray[{gr1, gr2}], 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \)}], "Input",
  CellTags->"GraphicsArray"],

Cell[TextData[{
  "Use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " to check out the limit of f(x)."
}], "Text"],

Cell[BoxData[{
    \(\(lim\  = \ 
        Limit[\ f[x], x \[Rule] 1\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] 1\)\) \>\""\ , f[x], "\< = \>", 
        lim\ ];\)\ \ \)}], "Input"],

Cell["We can define f(x) to be 4 at x = 1 as follows:", "Text"],

Cell[BoxData[{
    \(\(f[1]\  = \ 4;\)\ \ \), "\n", 
    \(\(f[x_] = \(x\^2 + 2\ x - 3\)\/\(x - 1\);\)\ \ \)}], "Input"],

Cell["Now check it out.", "Text"],

Cell[BoxData[
    \(\(Print["\<f[1] = \>", f[1]\ ];\)\)], "Input"],

Cell[TextData[{
  "What does ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " store for this function? Let's see.  "
}], "Text"],

Cell[BoxData[
    \(\(?f\)\)], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Example 2.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      \(f \((x)\) = \(\(|\)\(x\)\(|\)\)\/x\)]],
  ".  Show that  ",
  Cell[BoxData[
      RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 0\)\), 
        StyleBox["f",
          FontSize->12], 
        RowBox[{"(", 
          StyleBox["x",
            FontSize->12], 
          StyleBox[")",
            FontSize->12]}]}]]],
  "  does ",
  StyleBox["not",
    FontColor->RGBColor[1, 0, 1]],
  " exist, but the left and right limits both exist.  Note that  ",
  Cell[BoxData[
      \(f \((0)\)\)]],
  "  is ",
  StyleBox["not",
    FontColor->RGBColor[1, 0, 1]],
  " defined.  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 2.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[f, x];\)\ \ \), "\n", 
    \(\(f[x_]\  = \ Abs[x]\/x;\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[f[x], {x, \(-2\), 2}, PlotStyle \[Rule] Red];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<f[0] = \>", f[0]\ ];\)\ \ \)}], "Input"],

Cell["\<\
Note both the numerator and denominator of f(x) evaluate to be zero at x = 0. \
 Hence we have a zero divide by zero situation.  \
\>", "Text"],

Cell[BoxData[{
    \(\(n1 = ReplaceAll[Numerator[f[x]], x \[Rule] 0];\)\ \ \), "\n", 
    \(\(d1 = ReplaceAll[Denominator[f[x]], x \[Rule] 0];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<f[0] = \>", ToString[n1], "\</\>", 
        ToString[d1]];\)\ \ \)}], "Input"],

Cell[TextData[{
  "In this case, there is NO common factor.  \n\nUse ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " to check out the limit of f(x) approaching from the left, and observe \
that this function evaluates to be  -1  to the left of zero.  "
}], "Text"],

Cell[BoxData[{
    \(\(lim1 = 
        Limit[f[x], x \[Rule] 0, 
          Direction \[Rule] 1];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] \(0\^-\)\)\) f[x] = \>\"", 
        lim1\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(\(-1\)\/1000\)] = \>\"", 
        f[\(-1\)\/1000]\ ];\)\)}], "Input"],

Cell[TextData[{
  "Use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " to check out the limit of f(x) approaching from the right, and observe \
that this function evaluates to be  +1  to the right of zero.  "
}], "Text"],

Cell[BoxData[{
    \(\(lim2 = 
        Limit[f[x], x \[Rule] 0, 
          Direction \[Rule] \(-1\)];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] \(0\^+\)\)\) f[x] = \>\"", 
        lim2\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(1\/1000\)] = \>\"", f[1\/1000]\ ];\)\)}], "Input"],

Cell[TextData[{
  "Since the right and left limits are different, ",
  Cell[BoxData[
      \(\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 0\)\) f[x]\)]],
  " does ",
  StyleBox["NOT",
    FontColor->RGBColor[1, 0, 0]],
  " exist.  \n\n",
  StyleBox["Caution.",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  " The ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " default direction will give an answer corresponding to the right limit.  \
"
}], "Text"],

Cell[BoxData[
    \(Limit[\ f[x], x \[Rule] 0\ ]\)], "Input"],

Cell["\<\
Often times computer algebra systems are not programmed to handle all the \
\"special cases\" of involving mathematical concepts.  The concept of limit \
is one such instance where the \"true meaning\" of \"limit\" cannot be fully \
understood by \"artificial intelligence.\"  \
\>", "Text"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 1.  (Squeeze Theorem for Limits).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], "\[LessEqual]", \(g \((x)\)\), 
        "\[LessEqual]", \(h \((x)\)\)}]]],
  "  for  ",
  Cell[BoxData[
      \(a < x < x\_0\)]],
  "  and  ",
  Cell[BoxData[
      \(x\_0 < x < b\)]],
  ".  \n\n\tIf  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["L",
          FontSize->12]}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\), "h", 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["L",
          FontSize->12]}]]],
  "  then  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ x\_0\)\), "g", 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["L",
          FontSize->12]}]]],
  ".  "
}], "Text",
  CellTags->"Squeeze Theorem for Limits"],

Cell[TextData[{
  StyleBox["Example 3.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[\(1 - x\^2\/2\),
          FontSize->12]}]]],
  ",  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"g", 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[\(cos \((x)\)\),
          FontSize->12]}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{\(h \((x)\)\), 
        StyleBox["=",
          FontSize->12], 
        StyleBox[\(1 - x\^2\/3\),
          FontSize->12]}]]],
  ".  \nAssume that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], "\[LessEqual]", \(g \((x)\)\), 
        "\[LessEqual]", \(h \((x)\)\)}]]],
  "  for  ",
  Cell[BoxData[
      \(\(-\ \[Pi]\)\/2 < x < 0\)]],
  "  and  ",
  Cell[BoxData[
      \(0 < x < \[Pi]\/\(\(2\)\(\ \)\)\)]],
  "   (This is a fact that can be established !).  \nSince  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 0\)\), 
          StyleBox["f",
            FontSize->12], 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["1",
          FontSize->12]}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 0\)\), "h", 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["1",
          FontSize->12]}]]],
  "  we conclude that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(\(\(\ \)\(lim\)\(\ \)\)\+\(x\  \[Rule] \ 0\)\), "g", 
          RowBox[{"(", 
            StyleBox["x",
              FontSize->12], 
            StyleBox[")",
              FontSize->12]}]}], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["=",
          FontSize->12], 
        StyleBox[" ",
          FontSize->12], 
        StyleBox["1",
          FontSize->12]}]]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 3.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(f[x_] = 1 - x\^2\/2;\)\ \ \), "\n", 
    \(\(g[x_] = Cos[x];\)\ \ \), "\n", 
    \(\(h[x_] = 1 - x\^2\/3;\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[{f[x], g[x], h[x]}, {x, \(-\[Pi]\)\/2, \[Pi]\/2}, \n\t
        PlotRange \[Rule] {{\(-\[Pi]\)\/2, \[Pi]\/2}, {0, 1.1}}, \n\t
        Ticks \[Rule] {Range[\(-\(\[Pi]\/2\)\), \[Pi]\/2, \[Pi]\/4], 
            Range[0, 1, 0.2]}, \n\t
        PlotStyle \[Rule] {Green, Red, 
            Blue}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x], "\<,  g[x] = \>", g[x], "\<,  h[x] = \>", 
        h[x]];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(L1 = Limit[f[x], x \[Rule] 0];\)\ \ \), "\n", 
    \(\(L2 = Limit[h[x], x \[Rule] 0];\)\ \ \), "\[IndentingNewLine]", 
    \(\(L3 = Limit[g[x], x \[Rule] 0];\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] 0\)\) \>\"", f[x]\ , "\< = \>", 
        L1];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] 0\)\) \>\"", h[x]\ , "\< = \>", 
        L2];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] 0\)\) \>\"", g[x]\ , "\< = \>", 
        L3];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Definition 2.  (Continuous Function).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  " is defined at all points in a neighborhood of  ",
  Cell[BoxData[
