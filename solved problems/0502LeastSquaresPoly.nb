Cell[TextData[StyleBox["Mathematica Subroutine (Least Squares Parabola).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    RowBox[{
      RowBox[{
        RowBox[{\(LSParabola[XY0_]\), ":=", 
          RowBox[{"Module", "[", " ", 
            RowBox[{\({k, n, XY = XY0\ }\), ",", "\[IndentingNewLine]", 
              
              RowBox[{\(n = Length[XY]\), ";", 
                "\[IndentingNewLine]", \(X = \(Transpose[XY]\)\_\(\(\
\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\), ";", " ", 
                "\[IndentingNewLine]", \(Y = \(Transpose[XY]\)\_\(\(\
\[LeftDoubleBracket]\)\(2\)\(\[RightDoubleBracket]\)\)\), ";", " ", 
                "\[IndentingNewLine]", 
                RowBox[{"A", "=", " ", 
                  RowBox[{"(", GridBox[{
                        {
                          "n", \(\[Sum]\+\(k = 1\)\%n X\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\), \(\[Sum]\+\(k = 1\)\
\%n X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\^2\)},
                        {\(\[Sum]\+\(k = 1\)\%n X\_\(\(\[LeftDoubleBracket]\)\
\(k\)\(\[RightDoubleBracket]\)\)\), \(\[Sum]\+\(k = 1\)\%n X\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\^2\), \(\[Sum]\+\(k = \
1\)\%n X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\^3\)},
                        {\(\[Sum]\+\(k = 1\)\%n X\_\(\(\[LeftDoubleBracket]\)\
\(k\)\(\[RightDoubleBracket]\)\)\^2\), \(\[Sum]\+\(k = 1\)\%n X\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\^3\), \(\[Sum]\+\(k = \
1\)\%n X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\^4\)}
                        }], ")"}]}], ";", " ", "\[IndentingNewLine]", 
                RowBox[{"B", "=", " ", 
                  RowBox[{"(", GridBox[{
                        {\(\[Sum]\+\(k = 1\)\%n Y\_\(\(\[LeftDoubleBracket]\)\
\(k\)\(\[RightDoubleBracket]\)\)\)},
                        {\(\[Sum]\+\(k = 1\)\%n\( 
                                  X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\
\[RightDoubleBracket]\)\)\) 
                              Y\_\(\(\[LeftDoubleBracket]\)\(k\)\(\
\[RightDoubleBracket]\)\)\)},
                        {\(\[Sum]\+\(k = 1\)\%n\(\((X\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\))\)\^2\) 
                              Y\_\(\(\[LeftDoubleBracket]\)\(k\)\(\
\[RightDoubleBracket]\)\)\)}
                        }], ")"}]}], ";", " ", 
                "\[IndentingNewLine]", \(Z = LinearSolve[A, B]\), ";", " ", 
                "\[IndentingNewLine]", \(a = 
                  Z\_\(\(\[LeftDoubleBracket]\)\(1, \
1\)\(\[RightDoubleBracket]\)\)\), ";", " ", 
                "\[IndentingNewLine]", \(b = 
                  Z\_\(\(\[LeftDoubleBracket]\)\(2, \
1\)\(\[RightDoubleBracket]\)\)\), ";", " ", 
                "\[IndentingNewLine]", \(c = 
                  Z\_\(\(\[LeftDoubleBracket]\)\(3, \
1\)\(\[RightDoubleBracket]\)\)\), ";", " ", 
                "\[IndentingNewLine]", \(E2 = \@\(\(1.0\/n\) \(\[Sum]\+\(k = \
1\)\%n\((Y\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\) - a - \
b\ X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\) - c\ \
\((X\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\))\)\^2)\)\^2\)\
\)\), ";", " ", 
                "\[IndentingNewLine]", \(Return[\ 
                  a + \ b\ x\  + \ c\ x\^2\ ]\), ";"}]}], " ", "]"}]}], ";"}],
       " "}]], "Input"],

