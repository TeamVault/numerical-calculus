}], "Text"],

Cell[BoxData[
    \(\(\(\[Integral]\(1\/u\) \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \
\)\(Log[u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]\(\[ExponentialE]\^u\) \[DifferentialD]u\)\(\ \)\(\
\[Equal]\)\(\ \)\(\[ExponentialE]\^u\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]\(b\^u\) \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \
\)\(b\^u\/Log[b]\)\(\ \ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]Cos[u] \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \)\(Sin[
        u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\[Integral]Sin[u] \[DifferentialD]u\  \[Equal] \ \(\(-Cos[
          u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\[Integral]Tan[u] \[DifferentialD]u\  \[Equal] \ \(\(-Log[
          Cos[u]]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\[Integral]Sec[u] \[DifferentialD]u\  \[Equal] \ \(\(-Log[
            Cos[u\/2] - Sin[u\/2]]\)\(+\)\(Log[
          Cos[u\/2] + Sin[u\/2]]\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\(\(\[Integral]Sec[u] \[DifferentialD]u\)\(\ \)\(=\)\(\ \)\(Log[
          Sec[u] + Tan[u]]\)\(\ \)\)\)]],
  "  "
}], "Text"],

Cell[BoxData[
    \(\[Integral]Csc[u] \[DifferentialD]u\  \[Equal] \ \(\(-Log[
            Cos[u\/2]]\)\(+\)\(Log[Sin[u\/2]]\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]Csc[u] \[DifferentialD]u\  = \ Log[Csc[u] - Cot[u]]\)]],
  "  "
}], "Text"],

Cell[BoxData[
    \(\(\(\[Integral]Cot[u] \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \)\(Log[
        Sin[u]]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]\(Sec[
                u]\^2\) \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \)\(Tan[
        u]\)\(\ \ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]Sec[u]\ Tan[
            u] \[DifferentialD]u\)\(\ \)\(\[Equal]\)\(\ \)\(Sec[
        u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\[Integral]\(Csc[u]\^2\) \[DifferentialD]u\  \[Equal] \ \(\(-Cot[
          u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\[Integral]Cot[u]\ Csc[u] \[DifferentialD]u\  \[Equal] \ \(\(-Csc[
          u]\)\(\ \ \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(\[Integral]\(1\/\@\(a\^2 + u\^2\)\) \[DifferentialD]u\)\(\ \)\(\
\[Equal]\)\(\ \)\(Log[u + \@\(a\^2 + u\^2\)]\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\@\(a\^2 + u\^2\)\) \[DifferentialD]u\  = \ 
        ArcSinh[u\/a]\)]],
  "    "
}], "Text"],

Cell[BoxData[
    \(\[Integral]\(1\/\(a\^2 - 
                u\^2\)\) \[DifferentialD]u\  \[Equal] \ \(\(1\/a\) \(ArcTanh[
          u\/a]\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\(a\^2 - 
                  u\^2\)\) \[DifferentialD]u\  = \ \(-\(1\/\(2  a\)\)\)\ Log[
              u - a] + 1\/\(2  a\)\ Log[u + a]\)]],
  "  "
}], "Text"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\@\(a\^2 - u\^2\)\) \[DifferentialD]u\  = \ 
        ArcSin[u\/a]\)]],
  "  "
}], "Text"],

Cell[BoxData[
    \(\[Integral]\(1\/\(a\^2 + 
                u\^2\)\) \[DifferentialD]u\  \[Equal] \ \(\(1\/a\) \(ArcTan[
          u\/a]\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\(u \@\(\(\(\ \)\(u\^2 - a\^2\)\)\(\ \)\)\)\) \
\[DifferentialD]u\ \  = \ \(1\/a\) ArcSec[u\/a]\)]],
  "  "
}], "Text"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\(u \@\(\(\(\ \)\(u\^2 - 1\)\)\(\ \)\)\)\) \
\[DifferentialD]u\  = \ \[Pi]\/2 - ArcTan[1\/\@\(\(-1\) + u\^2\)]\)]],
  "  "
}], "Text"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\(u \@\(\(\(\ \)\(u\^2 - 1\)\)\(\ \)\)\)\) \
\[DifferentialD]u\ \  = \ \[Pi]\/2 - ArcCot[\@\(\(-1\) + u\^2\)]\)]],
  "  "
}], "Text"],

Cell[TextData[{
  "\t",
  Cell[BoxData[
      \(\[Integral]\(1\/\(u \@\(\(\(\ \)\(u\^2 - 1\)\)\(\ \)\)\)\) \
\[DifferentialD]u\  = \ ArcSec[u]\)]],
  "  "
}], "Text"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 14.  (Mean Value Theorem for Integrals).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  If  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is continuous over  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  then there exists a number  ",
  Cell[BoxData[
      \(c\)]],
  ",  with  ",
  Cell[BoxData[
      \(a < c < b\)]],
  ",  such that  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          FractionBox[
            StyleBox["1",
              FontSize->12], 
            StyleBox[\(b\  - \ a\),
              FontSize->12]], 
          RowBox[{
            StyleBox[
              SubsuperscriptBox["\[Integral]", 
                StyleBox["a",
                  FontSize->10], 
                StyleBox["b",
                  FontSize->10]],
              FontSize->18], 
            StyleBox[" ",
              FontSize->18], \(f \((x)\) \[DifferentialD]x\)}]}], " ", "=", 
        " ", \(f \((c)\)\)}]]],
  "."
}], "Text",
  CellTags->"Mean Value Theorem for Integrals"],

Cell[TextData[{
  StyleBox["Example 26.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      RowBox[{\(f \((x)\)\), "=", 
        RowBox[{\(sin \((x)\)\), "+", 
          RowBox[{
            FractionBox[
              StyleBox["1",
                FontSize->12], 
              StyleBox["3",
                FontSize->12]], " ", "sin", \((3\ x)\)}]}]}]]],
  ",  find  ",
  Cell[BoxData[
      \(c\)]],
  "  so that  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          FractionBox[
            StyleBox["1",
              FontSize->12], 
            StyleBox["2.5",
              FontSize->12]], 
          RowBox[{
            StyleBox[
              SubsuperscriptBox["\[Integral]", 
                StyleBox["0",
                  FontSize->10], 
                StyleBox["2.5",
                  FontSize->10]],
              
              FontSize->
                18], \(\((sin \((x)\) + 
                  1\/3\ sin \((3\ x)\))\) \[DifferentialD]x\)}]}], " ", "=", 
        " ", \(f \((c)\)\)}]]],
  "."
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 26.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, c, f, g, h, L, solset, v, val, x];\)\ \ \), "\n", 
    \(\(f[x_] = Sin[x] + 1\/3\ Sin[3\ x];\)\ \ \), "\n", 
    \(\(g[x_] = \[Integral]f[x] \[DifferentialD]x;\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<g[x] = \[Integral]f[x]\[DifferentialD]x = \>", 
        g[x]];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(a = 0;\)\ \ \), "\n", 
    \(\(b = 2.5;\)\ \ \), "\n", 
    \(\(v = \[Integral]\_a\%b f[x] \[DifferentialD]x;\)\ \ \), "\n", 
    \(\(val = \(\[Integral]\_a\%b f[x] \[DifferentialD]x\)\/\(2.5 - 0\);\)\ \ \
\), "\n", 
    \(\(solset = FindRoot[f[x] \[Equal] val, {x, 1}];\)\ \ \), "\n", 
    \(\(c = 
        solset\[LeftDoubleBracket]1, 2\[RightDoubleBracket];\)\ \ \), "\n", 
    \(\(h\  = \ f[c];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<    \!\(\[Integral]\_0.0\%2.5\)f[x]\[DifferentialD]x = \
\>\"", v];\)\), "\n", 
    \(\(Print[\*"\"\<\!\(1\/2.5\)\!\(\[Integral]\_0.0\%2.5\)f[x]\
\[DifferentialD]x = \>\"", val];\)\), "\n", 
    \(\(Print["\<          f[c] = \>", h];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<             c = \>", c];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`FilledPlot`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(gr1\  = \ 
        FilledPlot[{f[x]}, {x, a, b}, Fills \[Rule] {{{1, Axis}, Green}}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(grf = 
        Plot[f[x], {x, a, b}, 
          Ticks \[Rule] {Range[0, 2.5, 0.5], Range[0, 1, 0.2]}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(lines = 
        Graphics[{Line[{{a, 0}, {a, f[a]}}], 
            Line[{{b, 0}, {b, f[b]}}]}];\)\ \ \), "\n", 
    \(\(line2 = 
        Graphics[{Line[{{a, 0}, {a, h}}], Line[{{a, h}, {b, h}}], 
            Line[{{b, h}, {b, 0}}]}];\)\ \ \), "\n", 
    \(\(Show[gr1, grf, lines, line2, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] \*"\"\<y = f[x] = Sin[x]+\!\(1\/3\) Sin[3x]\>\"", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<The function is f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<The value of the integral is over [\>", a, "\<,\>", 
        b, "\<] is  \>", N[v]\ ];\)\ \ \), "\n", 
    \(\(Print["\<The area under the curve is the same as\>"];\)\ \ \), "\n", 
    \(\(Print["\<the area of a rectangle of height  h = \>", 
        h];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 15.  (Weighted Mean Value Theorem for Integrals).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that both  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  and  ",
  Cell[BoxData[
      \(g \((x)\)\)]],
  "  are continuous over  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  and that  ",
  Cell[BoxData[
      \(0 \[LessEqual] g \((x)\)\)]],
  "  for  ",
  Cell[BoxData[
      \(a \[LessEqual] x \[LessEqual] b\)]],
  ".  Then there exists a number  ",
  Cell[BoxData[
      \(c\)]],
  ",  with  ",
  Cell[BoxData[
      \(a < c < b\)]],
  ",  such that  \n\n\t",
  Cell[BoxData[
      \(\[Integral]\_a\%b\ f \((x)\) g \((x)\) \[DifferentialD]x\  = \ 
        f \((c)\)\ \(\[Integral]\_a\%b\ g \((x)\) \[DifferentialD]x\)\)]],
  ".   "
}], "Text",
  CellTags->"Weighted Mean Value Theorem for Integrals"],

Cell[TextData[{
  StyleBox["Example 27.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let   ",
  Cell[BoxData[
      \(f \((x)\) = x\^2\)]],
  "  and  ",
  Cell[BoxData[
      \(g \((x)\) = 1 + sin \((x)\)\)]],
  "  and let  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"[", 
          RowBox[{"a", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "b"}], "]"}], " ", "=", 
        RowBox[{"[", 
          RowBox[{"0", 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(\(3\ \[Pi]\)\/2\)}], 
          "]"}]}]]],
  ".   Find the value  ",
  Cell[BoxData[
      \(c\)]],
  ",  so that  \n\n\t",
  Cell[BoxData[
      \(\[Integral]\_0\%\(3  \[Pi]/2\)\ 
          f \((x)\) g \((x)\) \[DifferentialD]x\  = \ 
        f \((c)\)\ \(\[Integral]\_0\%\(3  \[Pi]/2\)\ 
            g \((x)\) \[DifferentialD]x\)\)]],
  ".   "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 27.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[c, f, fc, g, h\ , L, R, x];\)\ \ \), "\n", 
    \(\(f[x_] = x\^2;\)\ \ \), "\n", 
    \(\(g[x_] = 1 + Sin[x];\)\ \ \), "\n", 
    \(\(Print["\<     f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<     g[x] = \>", g[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<f[x] g[x] = \>", f[x]\ g[x]\ ];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(L = 
        Expand[\[Integral]\_0\%\(3  \[Pi]/2\)f[x]\ g[
                x] \[DifferentialD]x];\)\ \ \), "\n", 
    \(\(R = \[Integral]\_0\%\(3\ \[Pi]/2\)g[
              x] \[DifferentialD]x;\)\ \ \), "\n", 
    \(\(c = \@\(L\/R\);\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_0\%\(3  \[Pi]/2\)\)f[x]g[x]\
\[DifferentialD]x = f[c] \!\(\[Integral]\_0\%\(3  \[Pi]/2\)\)g[x]\
\[DifferentialD]x\>\""];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_0\%\(3  \[Pi]/2\)\)f[x]g[x]\
\[DifferentialD]x = \>\"", L];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_0\%\(3  \
\[Pi]/2\)\)g[x]\[DifferentialD]x = \>\"", R];\)\ \ \), "\n", 
    \(\(Print["\<f[c] = \>", L\/R];\)\ \ \), "\n", 
    \(\(Print["\<f[c] = \>", Simplify[L\/R]];\)\ \ \), "\n", 
    \(\(Print["\<f[c] = \>", N[L\/R]];\)\ \ \), "\n", 
    \(\(Print["\<  c  = \>", \@Simplify[L\/R], "\< = \>", 
        N[\@\(L\/R\)]\ ];\)\ \ \ \)}], "Input"],

Cell[BoxData[{
    \(\(Print["\<   c = \>", c];\)\ \ \), "\n", 
    \(\(Print["\<f[c] =  \>", f[c]];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", \*"\"\<\!\(\[Integral]\_0\%\(3  \[Pi]/2\)\)f[x]g[x]\
\[DifferentialD]x = f[c] \!\(\[Integral]\_0\%\(3  \[Pi]/2\)\)g[x]\
\[DifferentialD]x\>\""];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<  \>", L, "\< = (\>", f[c], "\<)(\>", 
        R, "\<)\>"];\)\ \ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<        \>", N[L], "\< = (\>", N[f[c]], "\<)(\>", 
        N[R], "\<)\>"];\)\ \ \ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 16.  (Taylor's Theorem and Lagrange's Remainder).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  and its derivatives  ",
  Cell[BoxData[
      RowBox[{\(f' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f'' \((x)\)\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], "...", " ", 
        StyleBox[",",
          FontFamily->"Times New Roman"], 
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((n + 1)\),
              Derivative],
            MultilineFunction->None], \((x)\)}]}]]],
  "  are all continuous on  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  and  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  is a fixed value.  Then  \n\n\t\t",
  Cell[BoxData[
      \(f \((x)\)\  = \ \(P\_n\) \((x)\)\  + \ \(R\_n\) \((x)\)\)]],
  "   \nwhere\t\n\t\t",
  Cell[BoxData[
      RowBox[{\(\(P\_n\) \((x)\)\), " ", "=", " ", 
        RowBox[{
          StyleBox[
            UnderoverscriptBox["\[Sum]", 
              StyleBox[\(k = 0\),
                FontSize->10], 
              StyleBox["n",
                FontSize->10]],
            FontSize->18], " ", 
          RowBox[{
            FractionBox[
              StyleBox[
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((k)\),
                      Derivative],
                    MultilineFunction->None], \((x\_0)\)}],
                FontSize->12], 
              StyleBox[\(k!\),
                FontSize->12]], \(\((x - x\_0)\)\^k\)}]}]}]]],
  "  \n\t\nis the n-th degree Taylor polynomial expanded about  ",
  Cell[BoxData[
      \(x\_0\)]],
  ".  There exists a value  ",
  Cell[BoxData[
      \(c\)]],
  "  that lies somewhere between  and  ",
  Cell[BoxData[
      \(x\)]],
  "  and  ",
  Cell[BoxData[
      \(x\_0\)]],
  "  and\n\n\t\t",
  Cell[BoxData[
      RowBox[{" ", 
        RowBox[{\(\(R\_n\) \((x)\)\), " ", "=", " ", 
          RowBox[{
            FractionBox[
              StyleBox[
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((n + 1)\),
                      Derivative],
                    MultilineFunction->None], \((c)\)}],
                FontSize->12], 
              StyleBox[\(\((n + 1)\)!\),
                FontSize->12]], \(\((x - x\_0)\)\^\(n + 1\)\)}]}]}]]],
  "  \n\t\nwhich is the ",
  StyleBox["Lagrange form of the remainder",
    FontColor->RGBColor[0, 0, 1]],
  "."
}], "Text",
  CellTags->"Taylor's Theorem and Lagrange's Remainder"],

Cell[TextData[{
  StyleBox["Example 28.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let   ",
  Cell[BoxData[
      \(f \((x)\) = sin \((x)\)\)]],
  ".  Find the Taylor polynomial of degree   ",
  Cell[BoxData[
      \(n = 9\)]],
  "  for  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  expanded about the point  ",
  Cell[BoxData[
      \(x\_0 = 0\)]],
  "."
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 28.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[f, n, P9, x0];\)\ \ \), "\n", 
    \(\(n = 9;\)\ \ \), "\n", 
    \(\(x0 = 0;\)\ \ \), "\n", 
    \(\(f[x_] = Sin[x];\)\ \ \), "\n", 
    \(\(P9[x_] = Normal[Series[f[x], {x, x0, n}]];\)\ \ \), "\n", 
    \(\(Print["\< f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(P\_9\)[x] = \>\"", P9[x]];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[{f[x], P9[x]}, {x, 0, 2\ \[Pi]}, 
        PlotRange \[Rule] {{0, 2  \[Pi]}, {\(-1\), 2}}, 
        PlotStyle \[Rule] {Magenta, Blue}, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] \*"\"\<y = Sin[x]  and  y = \!\(P\_9\)[x]\>\""];\)\ \
\ \), "\n", 
    \(\(Print["\<y =  f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<y = \!\(P\_9\)[x] = \>\"", P9[x]];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 17.  (Horner's Method for Polynomial Evaluation).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(P \((x)\) = \ \(a\_\(n + 1\)\) x\^n\  + \ \(a\_n\) 
            x\^\(n - 1\)\  + \[CenterEllipsis] + \ \(a\_3\) x\^2\  + \ 
          a\_2\ x\  + \ a\_1\)]],
  ".  \nThen\t",
  Cell[BoxData[
      \(P \((x)\)\)]],
  "  can be computed recursively as follows.  \n\n\tSet  ",
  Cell[BoxData[
      \(b\_\(n + 1\)\  = \ a\_\(n + 1\)\)]],
  " ,  \n\t",
  Cell[BoxData[
      \(b\_k\  = \ a\_k\  + \ x\ a\_\(k + 1\)\)]],
  "   for  ",
  Cell[BoxData[
      RowBox[{\(k = n\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(n - 1\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], "...", " ", 
        StyleBox[",",
          FontFamily->"Times New Roman"], "2", 
        StyleBox[",",
          FontFamily->"Times New Roman"], "1"}]]],
  ".  \n\nThen\t",
  Cell[BoxData[
      \(P \((x)\)\  = \ b\_1\)]],
  ".  \n\n",
  StyleBox["Remark.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  "  This is also known as synthetic division.  "
}], "Text",
  CellTags->"Horner's Method for Polynomial Evaluation"],

Cell[TextData[{
  StyleBox["Example 29.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Use Horner's method to evaluate  ",
  Cell[BoxData[
      \(P \((3)\)\)]],
  "  where  ",
  Cell[BoxData[
      \(P \((x)\) = x\^5 - 6\ x\^4 + 8\ x\^3 + 8\ x\^2 + 4\ x - 40\)]],
  ".  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 29.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "Construct a Horner's method subroutine called  ",
  StyleBox["H[x]",
    FontWeight->"Bold"],
  "."
}], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, H, k, n, P, Q, x];\)\ \ \), "\n", 
    \(\(P[x_] = 
        x\^5 - 6\ x\^4 + 8\ x\^3 + 8\ x\^2 + 4\ x - 40;\)\ \ \), "\n", 
    \(\(Q[
          x_] = \(-40\) + \((4 + \((8 + \((8 + \((\(-6\) + 
                                  x)\)\ x)\)\ x)\)\ x)\)\ x;\)\ \ \), "\n", 
    \(\(a\  = \ CoefficientList[P[x], x];\)\ \ \), "\n", 
    \(\(n\  = \ Length[a];\)\ \ \), "\n", 
    \(\(H[x_] := 
        Module[{}, \n\ \ \ \ b = 
            Table[0, {n}]; \n\ \ \ \ b\_\(\(\[LeftDoubleBracket]\)\(n\)\(\
\[RightDoubleBracket]\)\) = 
            a\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\); \ \
\ \n\ \ \ \ \ \ \ \ For[\ k = n - 1, \ 
            1 \[LessEqual] 
              k, \ \(k--\), \n\ \ \ \ \ \ \ \ \
b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\) = 
              a\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\  \
+ \ x\ b\_\(\(\[LeftDoubleBracket]\)\(k + 1\)\(\[RightDoubleBracket]\)\)]; \ \
\ \n\ \ \ \ Return[\ 
            b\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\ ];\
\ \ ];\)\ \)}], "Input"],

Cell[BoxData[{
    \(\(Print["\<P[x] = \>", P[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Q[x] = \>", Q[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<H[x] = \>", H[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<P[3] = \>", P[3]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Q[3] = \>", Q[3]\ ];\)\ \ \), "\n", 
    \(\(Print["\<H[3] = \>", 
        H[3], "\<\n\>"\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(For[\ k = n, \ 
        1 \[LessEqual] 
          k, \ \(k--\), \[IndentingNewLine]\(Print["\<b\>"\_k, "\< = \>", 
            b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)];\)\
\ ];\)\ \ \)}], "Input"],

Cell["\<\
Consider the computational time for vector arguments for evaluating  P(x)  \
and the nested form  Q(x).\
\>", "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(Clear[i, m, V];\)\ \ \), "\n", 
    \(\(m = 5000;\)\ \ \), "\[IndentingNewLine]", 
