Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    RowBox[{\(f[x_]\  = \ \[ExponentialE]\^\(-x\)\ \ Sin[x];\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<  f[x] = \>", f[x]\ ];\), " "}], "\[IndentingNewLine]",
   
    RowBox[{\(Print["\< f'[x] = \>", \(f'\)[x]\ ];\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<f''[x] = \>", \(f''\)[x]\ ];\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<\!\(f\^\((3)\)\)[x] = \>\"", ",", 
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((3)\),
                  Derivative],
                MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
        ";"}], " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<\!\(f\^\((4)\)\)[x] = \>\"", ",", 
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((4)\),
                  Derivative],
                MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
        ";"}], " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<\\n\>\"", ",", "\"\<\!\(f\^\((5)\)\)[x] = \>\"", ",", 
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((5)\),
                  Derivative],
                MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
        ";"}], " "}]}], "Input"],

Cell[BoxData[{
    RowBox[{\(Needs["\<Graphics`Colors`\>"];\), " "}], "\[IndentingNewLine]", 
  
    RowBox[{
      RowBox[{
        RowBox[{"extrema", "=", 
          RowBox[{"{", " ", 
            RowBox[{
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((5)\),
                    Derivative],
                  MultilineFunction->None], "[", "0", "]"}], ",", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((5)\),
                    Derivative],
                  MultilineFunction->None], "[", \(\[Pi]\/2\), "]"}]}], " ", 
            "}"}]}], ";"}], " "}], "\n", 
    RowBox[{\(M\_5\  = \ Max[\ Abs\ [\ extrema\ ]\ ];\), " "}], "\n", 
    RowBox[{\(EB\_five[h_]\  = \ \(\(M\_5\) h\^4\)\/30;\), 
      " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"Plot", "[", 
          RowBox[{
            RowBox[{"{", 
              RowBox[{
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((5)\),
                      Derivative],
                    MultilineFunction->None], "[", "x", "]"}], ",", 
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((5)\),
                      Derivative],
                    MultilineFunction->None], "[", "0", "]"}], ",", 
                RowBox[{
                  SuperscriptBox["f", 
                    TagBox[\((5)\),
                      Derivative],
                    MultilineFunction->None], "[", \(\[Pi]\/2\), "]"}]}], 
              "}"}], ",", \({x, 0, \[Pi]}\), 
            ",", \(PlotRange \[Rule] {\(-4\), 1}\), 
            ",", \(PlotStyle \[Rule] {Orange, Blue, Cyan}\), 
            ",", \(Ticks \[Rule] {Range[0, 2  \[Pi], \[Pi]\/4], 
                Range[\(-4\), 1, 1]}\)}], "]"}], ";"}], " "}], "\n", 
    RowBox[{
      RowBox[{
        RowBox[{"Print", "[", 
          RowBox[{"\"\<y = \!\(f\^\((5)\)\)[x] = \>\"", ",", 
            RowBox[{
              SuperscriptBox["f", 
                TagBox[\((5)\),
                  Derivative],
                MultilineFunction->None], "[", "x", "]"}]}], " ", "]"}], 
        ";"}], " "}], "\n", \(Print[\*"\"\<The minimum occurs at x = 0, and \
the maximum occurs at x = \!\(\[Pi]\/2\).\>\""];\), "\[IndentingNewLine]", 
    RowBox[{\(Print[\*"\"\<Extrema of \!\(f\^\((5)\)\)[x]: \>\"", 
          extrema\ ];\), " "}], "\n", 
    RowBox[{\(Print[\*"\"\<          |\!\(f\^\((5)\)\)[c]| \[LessEqual] \
\>\"", M\_5\ ];\), " "}], "\n", 
    RowBox[{\(Print["\<\n\>", "\<The absolute value of the remainder \
term:\>"];\), " "}], "\[IndentingNewLine]", 
