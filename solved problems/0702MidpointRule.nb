Cell[TextData[{
  StyleBox["Algorithm Composite Midpoint Rule.",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  "  To approximate the integral  \n\n\t",
  Cell[BoxData[
      RowBox[{
        StyleBox[
          SubsuperscriptBox["\[Integral]", 
            StyleBox["a",
              FontSize->10], 
            StyleBox["b",
              FontSize->10]],
          FontSize->18], \(f \((x)\) \[DifferentialD]x\)}]]],
  Cell[BoxData[
      RowBox[{
        StyleBox["\[TildeTilde]",
          FontSize->14], " "}]]],
  Cell[BoxData[
      \(h \(\[Sum]\+\(k = 1\)\%m f \((c\_k)\)\)\)]],
  ",  \n\nby sampling ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  " at the ",
  Cell[BoxData[
      \(m\)]],
  " equally spaced points  ",
  Cell[BoxData[
      \(c\_k = a + \((k - 1\/2)\)\ h\)]],
  "  for  ",
  Cell[BoxData[
      \(k = 1, 2, \[CenterEllipsis], m\)]],
  ",  where  ",
  Cell[BoxData[
      \(h = \(b\  - \ a\)\/m\)],
    AspectRatioFixed->False],
  ".  "
}], "Text"],

Cell[TextData[StyleBox["Mathematica Subroutine (Midpoint Rule).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(MidpointRule[a0_, b0_, 
          m0_] := \[IndentingNewLine]Module[{a = N[a0], b = N[b0], c, k, 
            m = m0}, \[IndentingNewLine]h\  = \ \(b - a\)\/m; \
\[IndentingNewLine]c\_k_ = 
            a + \((k - 1\/2)\)\ h; \ \[IndentingNewLine]Return[\ 
            h \(\[Sum]\+\(k = 1\)\%m f[c\_k]\)\ ];\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell["Or you can use the traditional program.", "Text"],

Cell[TextData[StyleBox["Mathematica Subroutine (Midpoint Rule).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(MidpointRule[a0_, b0_, 
          m0_] := \[IndentingNewLine]Module[{a = N[a0], b = N[b0], m = m0, 
            k}, \[IndentingNewLine]h\  = \ \(b - a\)\/m; \ \ \
\[IndentingNewLine]sum = 0; \ \[IndentingNewLine]For[\ k = 1, \ 
            k \[LessEqual] 
              m, \ \(k++\), \ \[IndentingNewLine]\(sum = 
                sum + f[
                    a + \((k - 
                            1/2)\)\ h];\)\ ]; \ \[IndentingNewLine]Return[\ 
            h\ sum];\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell[TextData[{
  StyleBox["Example 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Numerically approximate the integral  ",
  Cell[BoxData[
      \(\[Integral]\_0\%2\((2 + Cos[2 \@ x])\) \[DifferentialD]x\)]],
