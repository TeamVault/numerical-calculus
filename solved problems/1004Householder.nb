Cell[CellGroupData[{

Cell[BoxData[
    \(\(FormW[k_, 
          n_] := \[IndentingNewLine]Module[{i, j, 
            sum}, \[IndentingNewLine]sum\  = \ 0; \[IndentingNewLine]For[
            j = k + 1, 
            j \[LessEqual] 
              n, \(j++\), \[IndentingNewLine]\(sum\  = \ 
                sum\  + \ \((A1\_\(\(\[LeftDoubleBracket]\)\(j, k\)\(\
\[RightDoubleBracket]\)\))\)\^2;\)]; \[IndentingNewLine]s0\  = \ \@sum; \
\[IndentingNewLine]If[
            A1\_\(\(\[LeftDoubleBracket]\)\(k + 1, k\)\(\[RightDoubleBracket]\
\)\) < 0, \(s0 = \(-s0\);\)\ ]; \[IndentingNewLine]r0\  = \ \@\(2\ \
\((A1\_\(\(\[LeftDoubleBracket]\)\(k + 1, k\)\(\[RightDoubleBracket]\)\) + \
s0)\)\ s0\); \[IndentingNewLine]For[j = 1, 
            j \[LessEqual] 
              k, \(j++\), \(W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\) = 
                0;\)\ ]; \[IndentingNewLine]W1\_\(\(\[LeftDoubleBracket]\)\(k \
+ 1\)\(\[RightDoubleBracket]\)\)\  = \ \(A1\_\(\(\[LeftDoubleBracket]\)\(k + \
1, k\)\(\[RightDoubleBracket]\)\)\  + \ s0\)\/r0; \[IndentingNewLine]For[
            j = k + 2, 
            j \[LessEqual] 
              n, \(j++\), \[IndentingNewLine]\(W1\_\(\(\[LeftDoubleBracket]\)\
\(j\)\(\[RightDoubleBracket]\)\) = 
                A1\_\(\(\[LeftDoubleBracket]\)\(j, k\)\(\[RightDoubleBracket]\
\)\)\/r0;\)\ ];\ ];\)\)], "Input",
  AspectRatioFixed->True],

Cell[BoxData[
    \(\(FormV[k_, 
          n_] := \[IndentingNewLine]Module[{i, j, 
            sum}, \[IndentingNewLine]For[j = 1, 
            j \[LessEqual] 
              k, \(j++\), \(V1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\) = 0;\)\ ]; \[IndentingNewLine]For[i = k + 1, 
            i \[LessEqual] n, \(i++\), \[IndentingNewLine]sum\  = \ 
              0; \[IndentingNewLine]For[j = k + 1, 
              j \[LessEqual] 
                n, \(j++\), \[IndentingNewLine]\(sum\  = \ 
                  sum\  + \ 
                    A1\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\)\ W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\);\)\ ]; \[IndentingNewLine]V1\_\(\(\
\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\)\  = \ 
              sum;\ ];\ ];\)\)], "Input",
  AspectRatioFixed->True],

Cell[BoxData[
    \(\(FormQ[k_, 
          n_] := \[IndentingNewLine]Module[{j}, \[IndentingNewLine]c\  = \ 
            0; \[IndentingNewLine]For[j = k + 1, 
            j \[LessEqual] 
              n, \(j++\), \[IndentingNewLine]\(c\  = \ 
                c\  + \ 
                  W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\
\)\ V1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\);\)\ ]; \
\[IndentingNewLine]For[j = 1, 
            j \[LessEqual] 
              k, \(j++\), \[IndentingNewLine]\(Q1\_\(\(\[LeftDoubleBracket]\)\
\(j\)\(\[RightDoubleBracket]\)\)\  = \ 0;\)\ ]; \[IndentingNewLine]For[
            j = k + 1, 
            j \[LessEqual] 
              n, \(j++\), \[IndentingNewLine]\(Q1\_\(\(\[LeftDoubleBracket]\)\
\(j\)\(\[RightDoubleBracket]\)\)\  = \ 
                V1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)\
\  - \ c\ \ W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\);\)\
\ ];\ ];\)\)], "Input",
  AspectRatioFixed->True],

Cell[BoxData[
    \(\(FormA[k_, 
          n_] := \[IndentingNewLine]Module[{i, j}, \[IndentingNewLine]For[
            j = k + 2, 
            j \[LessEqual] 
              n, \(j++\), \
\[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\)\(j, k\)\(\
\[RightDoubleBracket]\)\)\  = \ 
              0; \[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\)\(k, j\)\(\
\[RightDoubleBracket]\)\)\  = \ 
              0;\ ]; \[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\)\(k + \
1, k\)\(\[RightDoubleBracket]\)\)\  = \ \(-\ 
              s0\); \[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\)\(k, k + \
1\)\(\[RightDoubleBracket]\)\)\  = \ \(-\ s0\); \[IndentingNewLine]For[j = k, 
            j \[LessEqual] 
              n, \(j++\), \[IndentingNewLine]\(A1\_\(\(\[LeftDoubleBracket]\)\
\(j, j\)\(\[RightDoubleBracket]\)\)\  = \ 
                A1\_\(\(\[LeftDoubleBracket]\)\(j, j\)\(\[RightDoubleBracket]\
\)\)\  - \ 
                  4\ Q1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\)\ W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\);\)\ ]; \[IndentingNewLine]For[i = k + 1, 
            i \[LessEqual] 
              n, \(i++\), \[IndentingNewLine]\(For[j = i + 1, 
                j \[LessEqual] 
                  n, \(j++\), \[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\
\)\(i, j\)\(\[RightDoubleBracket]\)\)\  = \ 
                  A1\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\)\  - \ 
                    2\ W1\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)\ Q1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\)\  - \ 
                    2\ Q1\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)\ W1\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\); \
\[IndentingNewLine]A1\_\(\(\[LeftDoubleBracket]\)\(j, i\)\(\
\[RightDoubleBracket]\)\)\  = \ 
                  A1\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\);\ ];\)\ ];\ ];\)\)], "Input",
  AspectRatioFixed->True],

Cell[BoxData[
