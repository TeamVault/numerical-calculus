Cell[TextData[StyleBox["Mathematica Subroutine (Neville Interpolation).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    RowBox[{
      RowBox[{\(NevilleInterpolation[X0_, z_]\), ":=", 
        RowBox[{"Module", "[", 
          
          RowBox[{\({i, j, n = Length[X0] - 1, X = X0}\), ",", 
            "\[IndentingNewLine]", 
            
            RowBox[{\(x\_i_ := 
                X\_\(\(\[LeftDoubleBracket]\)\(i + 1\)\(\[RightDoubleBracket]\
\)\)\), ";", "\[IndentingNewLine]", \(p\_\(i_, 0\) := f[x\_i]\), ";", 
              "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
                
                RowBox[{\(i = 1\), ",", \(i \[LessEqual] \ n\), ",", \(i++\), 
                  ",", "\[IndentingNewLine]", 
                  RowBox[{
                    RowBox[{"For", "[", 
                      
                      RowBox[{\(j = 1\), ",", \(j \[LessEqual] \ i\), 
                        ",", \(j++\), ",", "\[IndentingNewLine]", 
                        RowBox[{
                          RowBox[{\(p\_\(i, j\)\), ":=", 
                            RowBox[{"Release", "[", " ", 
                              RowBox[{\(1\/\(x\_i - x\_\(i - j\)\)\), 
                                RowBox[{"Det", "[", 
                                  RowBox[{"(", GridBox[{
                                        {\(p\_\(i - 1, 
                                        j - 1\)\), \(x\_\(i - j\) - z\)},
                                        {\(p\_\(i, j - 1\)\), \(x\_i - z\)}
                                        }], ")"}], "]"}]}], "]"}]}], ";"}]}], 
                      " ", "]"}], ";"}]}], "  ", "]"}], ";", "  ", 
              "\[IndentingNewLine]", \(k = i - 1\), ";"}]}], "]"}]}], 
      ";"}]], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Aitken Interpolation).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    RowBox[{
      RowBox[{\(AitkenInterpolation[X0_, z_]\), ":=", 
        RowBox[{"Module", "[", 
          
          RowBox[{\({i, j, n = Length[X0] - 1, X = X0}\), ",", 
            "\[IndentingNewLine]", 
            
            RowBox[{\(x\_i_ := 
                X\_\(\(\[LeftDoubleBracket]\)\(i + 1\)\(\[RightDoubleBracket]\
\)\)\), ";", "\[IndentingNewLine]", \(q\_\(i_, 0\) := f[x\_i]\), ";", 
              "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
                
                RowBox[{\(i = 1\), ",", \(i \[LessEqual] \ n\), ",", \(i++\), 
                  ",", "\[IndentingNewLine]", 
                  RowBox[{
                    RowBox[{"For", "[", 
                      
                      RowBox[{\(j = 1\), ",", \(j \[LessEqual] \ i\), 
                        ",", \(j++\), ",", "\[IndentingNewLine]", 
                        RowBox[{
                          RowBox[{\(q\_\(i, j\)\), ":=", " ", 
                            RowBox[{"Release", "[", 
                              RowBox[{\(1\/\(x\_i - x\_\(j - 1\)\)\), 
                                RowBox[{"Det", "[", 
                                  RowBox[{"(", GridBox[{
                                        {\(q\_\(j - 1, 
                                        j - 1\)\), \(x\_\(j - 1\) - z\)},
                                        {\(q\_\(i, j - 1\)\), \(x\_i - z\)}
                                        }], ")"}], "]"}]}], "]"}]}], ";"}]}], 
                      " ", "]"}], ";"}]}], "  ", "]"}], ";", "  ", 
              "\[IndentingNewLine]", \(k = i - 1\), ";"}]}], "]"}]}], 
      ";"}]], "Input"],

