Cell[TextData[{
  StyleBox["Program ",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox[" (",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox["Crank-Nicolson",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox[" method for the heat equation)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox["  To approximate the solution of the heat equation  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\(u\_t\) \((x, t)\) = \(c\^2\) \(u\_xx\) \((x, t)\)\)]],
  "  ",
  StyleBox["over the rectangle  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Italic"], 
        "=", \({\((x, t)\) : 0 \[LessEqual] x \[LessEqual] a, 
          0 \[LessEqual] t \[LessEqual] b}\)}]]],
  "  with    ",
  Cell[BoxData[
      \(u \((x, 0)\) = f \((x)\)\)]],
  ",  for  ",
  Cell[BoxData[
      \(0 \[LessEqual] x \[LessEqual] a\)]],
  ".  and  ",
  Cell[BoxData[
      RowBox[{\(u \((0, t)\)\), "=", 
        RowBox[{
          RowBox[{\(c\_1\), "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "  ", "u", \((a, t)\)}], 
          "=", \(c\_2\)}]}]]],
  ",  for  ",
  Cell[BoxData[
      \(0 \[LessEqual] t \[LessEqual] b\)]],
  ".  "
}], "Text"],

Cell[BoxData[
    \(\(\(CNgrid[n_, 
          m_] := \[IndentingNewLine]Module[{i, j}, \[IndentingNewLine]u\  = \ 
            Table[1, {n}, {m}]; \[IndentingNewLine]For[\ i = 1, \ 
            i \[LessEqual] 
              n, \ \(i++\), \
\[IndentingNewLine]\(u\_\(\(\[LeftDoubleBracket]\)\(i, 1\)\(\
\[RightDoubleBracket]\)\) = f[i];\)\ ]; \[IndentingNewLine]For[\ j = 1, \ 
            j \[LessEqual] 
              m, \ \(j++\), \
\[IndentingNewLine]u\_\(\(\[LeftDoubleBracket]\)\(1, j\)\(\
\[RightDoubleBracket]\)\)\  = \ 
              g\_1[j]; \[IndentingNewLine]u\_\(\(\[LeftDoubleBracket]\)\(n, j\
\)\(\[RightDoubleBracket]\)\)\  = \ 
              g\_2[j];\ \ ];\ \ ];\)\(\ \ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell[BoxData[
    \(\(\(CoeffMat[
          n_] := \[IndentingNewLine]Module[{}, \[IndentingNewLine]Va\  = \ \
\(Vc\  = \ Table[\(-1\), {n - 
                    1}]\); \[IndentingNewLine]Va\_\(\(\[LeftDoubleBracket]\)\(\
n - 1\)\(\[RightDoubleBracket]\)\)\  = \ \
\(Vc\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\  = \ 
              0\); \[IndentingNewLine]Vd\  = \ 
            Table[2 + 
                2\/r, {n}]; \[IndentingNewLine]Vd\_\(\(\[LeftDoubleBracket]\)\
\(1\)\(\[RightDoubleBracket]\)\)\  = \ \
\(Vd\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\)\  = \ 
              1\);\ \ ];\)\(\ \ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell[BoxData[
    \(\(\(TriDiagonal[a0_, d0_, c0_, 
          b0_]\  := \[IndentingNewLine]Module[{\ a = a0, b = b0, c = c0, 
            d = d0, k, m, n = Length[b0], x\ }, \[IndentingNewLine]For[\ 
            k = 2, \ k \[LessEqual] 
              n, \ \(k++\), \
\[IndentingNewLine]d\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\
\)\  = \ d\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\ \  - \
\ \(\(\ \)\(a\_\(\(\[LeftDoubleBracket]\)\(k - 1\)\(\[RightDoubleBracket]\)\)\
\)\)\/\(\(\ \)\(d\_\(\(\[LeftDoubleBracket]\)\(k - 1\)\(\[RightDoubleBracket]\
\)\)\)\)\ \ c\_\(\(\[LeftDoubleBracket]\)\(k - \
1\)\(\[RightDoubleBracket]\)\); \ \[IndentingNewLine]b\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\  = \ 
              b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\  \
- \ \(\(\ \)\(a\_\(\(\[LeftDoubleBracket]\)\(k - 1\)\(\[RightDoubleBracket]\)\
\)\)\)\/\(\(\ \)\(d\_\(\(\[LeftDoubleBracket]\)\(k - 1\)\(\
\[RightDoubleBracket]\)\)\)\)\ b\_\(\(\[LeftDoubleBracket]\)\(k - 1\)\(\
\[RightDoubleBracket]\)\);\ ]; \ \[IndentingNewLine]x = 
            Table[0, {n}]; \ \[IndentingNewLine]x\_\(\(\[LeftDoubleBracket]\)\
\(n\)\(\[RightDoubleBracket]\)\)\  = \(\(\ \
\)\(b\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\)\)\)\/d\_\(\(\
\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\); \ \
\[IndentingNewLine]For[\ k = n - 1, \ 
            1 \[LessEqual] 
              k, \ \(k--\), \
\[IndentingNewLine]\(x\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\
\)\)\  = \ \(b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\  \
- \ c\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\ x\_\(\(\
\[LeftDoubleBracket]\)\(k + 1\)\(\[RightDoubleBracket]\)\)\)\/d\_\(\(\
\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\);\)\ ]; \ \
\[IndentingNewLine]Return[x];\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell[BoxData[
    \(\(\(CNsolve[n_, 
          m_] := \[IndentingNewLine]Module[{b, i, 
            j}, \[IndentingNewLine]b\  = \ 
            Table[0, {n}]; \[IndentingNewLine]For[\ j = 2, \ 
            j \[LessEqual] 
              m, \ \(j++\), \
\[IndentingNewLine]b\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\
\)\  = \ g\_1[
                j]; \[IndentingNewLine]b\_\(\(\[LeftDoubleBracket]\)\(n\)\(\
\[RightDoubleBracket]\)\)\  = \ g\_2[j]; \[IndentingNewLine]For[\ i = 2, \ 
              i \[LessEqual] 
                n - 1, \ \(i++\), \[IndentingNewLine]\(b\_\(\(\
\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\)\  = \ 
                  u\_\(\(\[LeftDoubleBracket]\)\(i - 1, j - 1\)\(\
\[RightDoubleBracket]\)\) + \((2\/r - 
                          2)\)\ u\_\(\(\[LeftDoubleBracket]\)\(i, j - 1\)\(\
\[RightDoubleBracket]\)\) + 
                    u\_\(\(\[LeftDoubleBracket]\)\(i + 1, j - 1\)\(\
\[RightDoubleBracket]\)\);\)\ ]; \[IndentingNewLine]u\_\(\(\
\[LeftDoubleBracket]\)\(All, j\)\(\[RightDoubleBracket]\)\)\  = \ 
              TriDiagonal[Va, Vd, Vc, b];\ \ ];\ \ ];\)\(\ \ \)\)\)], "Input",\

  AspectRatioFixed->False],

Cell[TextData[{
  StyleBox["Example 3.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Consider the heat equation where  ",
