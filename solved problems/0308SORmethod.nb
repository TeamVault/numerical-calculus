Cell[BoxData[
    \(\(\(Jacobi[A0_, B0_, P0_, 
          max_] := \[IndentingNewLine]Module[{A = N[A0], B = N[B0], i, j, 
            k = 0, n = Length[P0], P = P0, 
            oldP = P0}, \ \[IndentingNewLine]Print["\<P\>"\_0, "\< = \>", 
            P]; \ \[IndentingNewLine]While[\ 
            k < max, \[IndentingNewLine]For[\ i = 1, 
              i \[LessEqual] 
                n, \(i++\), \
\[IndentingNewLine]P\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\
\)\  = \ \(1\/A\_\(\(\[LeftDoubleBracket]\)\(i, \
i\)\(\[RightDoubleBracket]\)\)\) \((B\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = 1\)\%\(i - 1\)A\_\(\(\
\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ oldP\_\(\(\
\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = i \
+ 1\)\%n A\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ \
oldP\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\))\)\ ]; \ \
\[IndentingNewLine]Print["\<P\>"\_\(k + 1\), "\< = \>", 
              P]; \ \[IndentingNewLine]oldP\  = \ 
              P; \ \[IndentingNewLine]k\  = \ 
              k + 1;\ ]; \ \[IndentingNewLine]Return[
            P];\ ];\)\(\ \ \)\)\)], "Input"],

Cell[TextData[{
  StyleBox["Mathematica Subroutine (",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox["Gauss-Seidel Iteration",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]],
  StyleBox[").",
    FontWeight->"Bold",
    FontColor->RGBColor[0.500008, 0, 0.996109]]
}], "Text"],

Cell[BoxData[
    \(\(\(GaussSeidel[A0_, B0_, P0_, 
          max_] := \[IndentingNewLine]Module[{A = N[A0], B = N[B0], i, j, 
            k = 0, n = Length[P0], 
            P = P0}, \ \[IndentingNewLine]Print["\<P\>"\_0, "\< = \>", 
            P]; \ \[IndentingNewLine]While[\ 
            k < max, \[IndentingNewLine]For[\ i = 1, 
              i \[LessEqual] 
                n, \(i++\), \
\[IndentingNewLine]P\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\
\)\  = \ \(1\/A\_\(\(\[LeftDoubleBracket]\)\(i, \
i\)\(\[RightDoubleBracket]\)\)\) \((B\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = 1\)\%\(i - 1\)A\_\(\(\
\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ P\_\(\(\
\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = i \
+ 1\)\%n A\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ \
P\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\))\)\ ]; \ \
\[IndentingNewLine]Print["\<P\>"\_\(k + 1\), "\< = \>", 
              P]; \ \[IndentingNewLine]k\  = \ 
              k + 1;\ ]; \ \[IndentingNewLine]Return[
            P];];\)\(\ \ \)\)\)], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Successive Over Relaxation).",
  
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(SORmethod[A0_, B0_, P0_, \[Omega]_, 
          max_] := \[IndentingNewLine]Module[{A = N[A0], B = N[B0], i, j, 
            k = 0, n = Length[P0], P = P0, 
            oldP = P0}, \ \[IndentingNewLine]Print["\<P\>"\_0, "\< = \>", 
            P]; \ \[IndentingNewLine]While[\ 
            k < max, \[IndentingNewLine]For[\ i = 1, 
              i \[LessEqual] 
                n, \(i++\), \
\[IndentingNewLine]P\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\)\
\)\  = \((1 - \[Omega])\) 
                    oldP\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\) + \ \(\[Omega]\/A\_\(\(\[LeftDoubleBracket]\)\(i, i\
\)\(\[RightDoubleBracket]\)\)\) \((B\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = 1\)\%\(i - 1\)A\_\(\(\
\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ P\_\(\(\
\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\)\  - \ \[Sum]\+\(j = i \
+ 1\)\%n A\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\ \
oldP\_\(\(\[LeftDoubleBracket]\)\(j\)\(\[RightDoubleBracket]\)\))\)\ ]; \ \
\[IndentingNewLine]Print["\<P\>"\_\(k + 1\), "\< = \>", 
              P]; \ \[IndentingNewLine]oldP\  = \ 
              P; \ \[IndentingNewLine]k\  = \ 
              k + 1;\ ]; \ \[IndentingNewLine]Return[
            P];\ ];\)\(\ \ \)\)\)], "Input"],

