Cell[BoxData[
    \(\(\(Simpson[a_, b_] := 
        Module[{}, \[IndentingNewLine]Return[\(\(b - a\)\/6\) \((f[a] + 
                  4  f[\(a + b\)\/2] + f[b])\)]\ ];\)\(\ \)\)\)], "Input",
  AspectRatioFixed->False],

Cell[BoxData[
    \(\(\(Adapt[a_, b_, tol_] := 
        Module[{c}, \[IndentingNewLine]c = \(a + b\)\/2; \ \
\[IndentingNewLine]Sab = Simpson[a, b]; \ \[IndentingNewLine]Sac = 
            Simpson[a, c]; \ \[IndentingNewLine]Scb = 
            Simpson[c, b]; \ \[IndentingNewLine]If[\ 
            Abs[Sab - Sac - Scb] < tol, \[IndentingNewLine]Return[\ 
              Sac + Scb\ ], \[IndentingNewLine]Return[
              Adapt[a, c, tol\/2] + 
                Adapt[c, b, tol\/2]]\ \ ];\ ];\)\(\ \)\)\)], "Input"],

