}], "Text"],

Cell[TextData[{
  StyleBox["When the trapezoidal rule is applied on the four subintervals  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(x\_1\)}], "]"}]]],
  StyleBox[", ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{\(x\_1\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(x\_2\)}], "]"}]]],
  StyleBox[", ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{\(x\_2\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(x\_3\)}], "]"}]]],
  StyleBox[", and ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{\(x\_3\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(x\_4\)}], "]"}]]],
  "  ",
  StyleBox["it is called a ",
    FontFamily->"Times-Roman"],
  StyleBox["composite trapezoidal rule",
    FontFamily->"Times-Roman",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[": \n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\[Integral]\_\(x\_0\)\%\(x\_4\)\ 
          f \((x)\) \[DifferentialD]x\  \[TildeTilde] \ \(h\/2\) \((f\_0 + 
                f\_1)\)\  + \ \(h\/2\) \((f\_1 + 
                f\_2)\)\  + \ \(h\/2\) \((f\_2 + 
                f\_3)\)\  + \ \(h\/2\) \((f\_3 + f\_4)\)\)]],
  "  \n\t\t\n(12)\t\t",
  Cell[BoxData[
      \(\[Integral]\_\(x\_0\)\%\(x\_4\)\ 
          f \((x)\) \[DifferentialD]x\  \[TildeTilde] \ \(h\/2\) \((f\_0\  + \
\ 2  f\_1\  + \ 2  f\_2\  + \ 2  f\_3\  + \ f\_4)\)\)]],
  "  "
}], "Text"],

