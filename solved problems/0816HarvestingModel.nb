Cell[TextData[{
  StyleBox["Case (ii)  If  ",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(a\^2 > 4\ b\ k\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" ",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" there are two stationary solutions  ",
    FontWeight->"Bold",
