Cell[TextData[StyleBox["Rules 2.  Rules for Integration.",
  FontWeight->"Bold",
  FontColor->RGBColor[0, 0, 1]]], "Text",
  Evaluatable->False,
  CellTags->"Rules for Integration"],

Cell[BoxData[
    StyleBox[
      RowBox[{
        InterpretationBox[\("\n"\[InvisibleSpace]"Integral of a scalar \
multiple."\),
          SequenceForm[ "\n", "Integral of a scalar multiple."],
          Editable->False], "\n", 
        InterpretationBox[\("\[Integral]c f[x]\[DifferentialD]x = "\
\[InvisibleSpace]\(c\ \(\[Integral]f[x] \[DifferentialD]x\)\)\),
          SequenceForm[ "\[Integral]c f[x]\[DifferentialD]x = ", 
            Times[ c, 
              Integrate[ 
                f[ x], x]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Integral of a linear \
combination."\),
          SequenceForm[ "\n", "Integral of a linear combination."],
          Editable->False], "\n", 
        InterpretationBox[\("\[Integral](a f[x]+b g[x])\[DifferentialD]x = "\
\[InvisibleSpace]\(a\ \(\[Integral]f[x] \[DifferentialD]x\) + 
              b\ \(\[Integral]g[x] \[DifferentialD]x\)\)\),
          SequenceForm[ "\[Integral](a f[x]+b g[x])\[DifferentialD]x = ", 
            Plus[ 
              Times[ a, 
                Integrate[ 
                  f[ x], x]], 
              Times[ b, 
                Integrate[ 
                  g[ x], x]]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Integral of a composition \
of functions."\),
          SequenceForm[ "\n", "Integral of a composition of functions."],
          Editable->False], "\n", 
        InterpretationBox[\("\[Integral] f'[g[x]]g'[x]\[DifferentialD]x = "\
\[InvisibleSpace]f[g[x]]\),
          SequenceForm[ "\[Integral] f'[g[x]]g'[x]\[DifferentialD]x = ", 
            f[ 
              g[ x]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Integral involving products \
of functions."\),
          SequenceForm[ "\n", "Integral involving products of functions."],
          Editable->False], "\n", 
        InterpretationBox[\("\[Integral](f'[x]g[x] + f[x]g'[x])\
\[DifferentialD]x = "\[InvisibleSpace]\(f[x]\ g[x]\)\),
          SequenceForm[ 
          "\[Integral](f'[x]g[x] + f[x]g'[x])\[DifferentialD]x = ", 
            Times[ 
              f[ x], 
              g[ x]]],
          Editable->False], "\n", 
        InterpretationBox[\("\n"\[InvisibleSpace]"Integral involving \
quotients of functions."\),
          SequenceForm[ "\n", "Integral involving quotients of functions."],
          Editable->False], "\n", 
        InterpretationBox[\("\[Integral]\!\(\(g[x]\\ \(f'\)[x] - f[x]\\ \
\(g'\)[x]\)\/g[x]\^2\)\[DifferentialD]x = "\[InvisibleSpace]f[x]\/g[x]\),
          SequenceForm[ 
          "\[Integral]\!\(\(g[x]\\ \(f'\)[x] - f[x]\\ \(g'\)[x]\)\/g[x]\^2\)\
\[DifferentialD]x = ", 
            Times[ 
              f[ x], 
              Power[ 
                g[ x], -1]]],
          Editable->False]}],
      FontColor->GrayLevel[0]]], "Print",
  GeneratedCell->False,
  CellAutoOverwrite->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Derivations for the Rules for Integration.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, c, F, f, g, g1, g2, g3, g4, g5, x];\)\ \ \), "\n", 
    \(\(F1[x_]\  = \ \[Integral]c\ f[x] \[DifferentialD]x;\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<Integral of a scalar multiple.\>"];\)\ \ \), "\n", \

    \(\(Print["\<\[Integral]c f[x]\[DifferentialD]x = \>", 
        F1[x]];\)\ \ \), "\n", 
    \(\(F2[x_]\  = \ 
        Thread[\[Integral]\((a\ f[x] + \ b\ g[x])\) \[DifferentialD]x, 
          Plus];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<Integral of a linear combination.\>"];\)\ \ \), "\n\
", 
    \(\(Print["\<\[Integral](a f[x]+b g[x])\[DifferentialD]x = \>", 
        F2[x]];\)\ \ \), "\n", 
    \(\(F3[
          x_]\  = \[Integral]\ \(f'\)[g[x]] \(g'\)[
              x] \[DifferentialD]x\ ;\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<Integral of a composition of functions.\>"];\)\ \ \
\), "\n", 
    \(\(Print["\<\[Integral] f'[g[x]]g'[x]\[DifferentialD]x = \>", 
        F3[x]];\)\ \ \), "\n", 
    \(\(F4[
          x_]\  = \[Integral]\((\(f'\)[x] g[x] + 
                f[x] \(g'\)[x])\) \[DifferentialD]x\ ;\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<Integral involving products of functions.\>"];\)\ \
\ \), "\n", 
    \(\(Print["\<\[Integral](f'[x]g[x] + f[x]g'[x])\[DifferentialD]x = \>", 
        F4[x]];\)\ \ \), "\n", 
    \(\(F5[
          x_]\  = \[Integral]\(\(g[x]\ \(f'\)[x] - 
                  f[x]\ \(g'\)[
                      x]\)\/g[x]\^2\) \[DifferentialD]x\ ;\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<Integral involving quotients of functions.\>"];\)\ \
\ \), "\n", 
    \(\(Print[\*"\"\<\[Integral]\!\(\(g[x] \(f'\)[x] - f[x] \(g'\)[x]\)\/g[x]\
\^2\)\[DifferentialD]x = \>\"", F5[x]];\)\ \)}], "Input"]
}, Closed]],

