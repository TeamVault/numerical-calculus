    \(\(Print["\<L = \>", L];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(c\_1\) = \>\"", c1];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<f[\!\(c\_1\)] = \>\"", f[c1]\ ];\)\ \ \), "\n", 
