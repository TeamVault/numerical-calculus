    \(\(dot\  = \ 
        Graphics[{Blue, PointSize[0.025], 
            Point[{c0, f[c0]}]}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(lines = 
        Graphics[{RGBColor[0, 1, 0], Line[{{a, f[a]}, {b, f[b]}}], 
            Line[{{c0 - 1\/2, f[c0] - m\/2}, {c0 + 1\/2, 
                  f[c0] + m\/2}}]}];\)\ \ \), "\n", 
    \(\(line1 = 
        Graphics[{Blue, 
            Line[{{c0 - 1\/2, f[c0] - m\/2}, {c0 + 1\/2, 
                  f[c0] + m\/2}}]}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(line2 = 
        Graphics[{Red, 
            Line[{{a, f[a]}, {b, f[b]}}]}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(grf = 
        Plot[f[x], {x, 0.1, 2.1}, PlotStyle \[Rule] Magenta, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(Show[grf, dots, dot, line1, line2, 
        PlotRange \[Rule] {{0, 2.2}, {0, 1.12}}, 
        Prolog \[Rule] {PointSize[0.02]}, 
        Ticks \[Rule] {Range[0, 3, 0.5], Range[0, 1, 0.5]}, 
        AxesLabel \[Rule] {x, y}, \n\tPlotLabel \[Rule] "\<y = Sin[x]\>", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<The interval is [\>", a, "\< , \>", 
        b, "\<]\>"];\)\ \ \), "\n", 
    \(\(Print["\<f'[x] = \>", \(f'\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(\(f[2.1] - f[0.1]\)\/\(2.1 - 0.1\)\) = \>\"", 
        m];\)\ \ \), "\n", 
    \(\(Print["\<The value c where  f'[c] = m = \>", 
        m, "\<  is:\>"];\)\ \ \), "\n", 
    \(\(Print["\<c = \>", c0];\)\ \ \), "\n", 
    \(\(Print[a, "\<  <  \>", c0, "\<  <  \>", b];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 10.  (Extreme Value Theorem for a Differentiable \
Function).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
