    \(\(Print["\<f'[\>", X, "\<] \[TildeTilde] (\>", 
        f[X + H]\  - \ f[X - H], "\<)/(\>", 
        2  H, "\<)\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f'[\>", 
        X, "\<] \[TildeTilde] \>", \(f[X + H] - f[X - H]\)\/\(2  H\)\ ];\)\ \
\), "\[IndentingNewLine]", 
    \(\(H = 0.01;\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x] = \>", f[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Using  h = \>", H];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f'[\>", X, "\<] \[TildeTilde] ((\>", f[X + H], "\<) - (\>", 
        f[X - H], "\<))/(\>", 
        2  H, "\<)\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f'[\>", X, "\<] \[TildeTilde] (\>", 
        f[X + H]\  - \ f[X - H], "\<)/(\>", 
        2  H, "\<)\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f'[\>", 
        X, "\<] \[TildeTilde] \>", \(f[X + H] - f[X - H]\)\/\(2  H\)\ ];\)\ \
\), "\[IndentingNewLine]", 
