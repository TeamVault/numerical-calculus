Cell[TextData[StyleBox["Mathematica Subroutine (Secant Method).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(SecantMethod[x0_, x1_, 
        max_]\)\(:=\)\(\[IndentingNewLine]\)\(Module[{\(k\)\(,\)}, \
\[IndentingNewLine]k\  = \ 1; \ \[IndentingNewLine]p0\  = \ 
          N[x0]; \ \[IndentingNewLine]p1\  = \ 
          N[x1]; \ \[IndentingNewLine]Print[\*"\"\<\!\(p\_0\) = \>\"", 
          PaddedForm[p0, {16, 16}], \*"\"\<,   g[\!\(p\_0\)] = \>\"", 
          NumberForm[g[p0], 
            16]\ ]; \ \[IndentingNewLine]Print[\*"\"\<\!\(p\_1\) = \>\"", 
          PaddedForm[p1, {16, 16}], \*"\"\<,   g[\!\(p\_1\)] = \>\"", 
          NumberForm[g[p1], 16]\ ]; \ \ \[IndentingNewLine]p2\  = \ 
          p1; \ \[IndentingNewLine]p1\  = \ p0; \ \[IndentingNewLine]While[\ 
          k < max, \[IndentingNewLine]p0\  = \ 
            p1; \ \[IndentingNewLine]p1\  = \ 
            p2; \ \[IndentingNewLine]p2\  = \ 
            p1\  - \ \(g[p1] \((p1\  - \ p0)\)\)\/\(g[p1]\  - \ g[p0]\); \ \
\[IndentingNewLine]k\  = \ 
            k + 1; \ \[IndentingNewLine]Print["\<p\>"\_k, "\< = \>", 
            PaddedForm[p2, {16, 16}], "\<,   g[\>", "\<p\>"\_k, "\<] = \>", 
            NumberForm[g[p2], 
              16]\ ];\ ]; \ \[IndentingNewLine]Print["\<  p  = \>", 
          NumberForm[p2, 
            16]\ ]; \ \[IndentingNewLine]Print["\< \[CapitalDelta]p  = \
\[PlusMinus]\>", Abs[p2 - p1]\ ]; \ \[IndentingNewLine]Print["\<g[p] = \>", 
          NumberForm[g[p2], 16]\ ];\ ]\)\(\ \)\)\)], "Input"],

Cell[TextData[{
  StyleBox["Example 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  " ",
  StyleBox[" ",
    FontFamily->"Times-Bold",
    FontWeight->"Bold"],
  StyleBox["Find the minimum of the unimodal function  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((x)\)\  = \ x\^2 - sin \((x)\)\)],
    AspectRatioFixed->True],
  StyleBox["  on the interval  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\([0, 1]\)\)]],
  StyleBox[".  ",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text",
  Evaluatable->False,
  AspectRatioFixed->False],

Cell[BoxData[{
    \(\(f[x_] = x\^2 - Sin[x];\)\ \ \), "\[IndentingNewLine]", 
    \(\(g[x_] = \(f'\)[x];\)\ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[f[x], {x, 0, 1}, PlotRange \[Rule] {{0, 1}, {\(-0.25\), 0.2}}, 
        Ticks \[Rule] {Range[0, 1, 0.2], Range[\(-0.4\), 0.2, 0.1]}, 
        PlotStyle \[Rule] Magenta];\)\ \), "\n", 
    \(\(Print["\<   y = f[x] = \>", f[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<g[x] = f'[x] = \>", g[x]];\)\ \)}], "Input",
  AspectRatioFixed->True],

Cell[TextData[{
  StyleBox["Solution by solving",
    FontFamily->"Times-Bold",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontFamily->"Times-Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(f' \((x)\) = 0\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["\n\n\tA root-finding method can be used to determine where the \
derivative  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f' \((x)\)\  = \ 2\ x - cos \((x)\)\)]],
  StyleBox["  is zero. \n\nSince  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f' \((0)\)\  = \ \(-1\)\)]],
  StyleBox["  and  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f' \((1)\)\  = \ 1.4596977\)]],
  "  have opposite signs,",
  StyleBox[" by the intermediate value theorem a root of ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f' \((x)\)\)]],
  StyleBox[" lies in the interval  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\([0, 1]\)\)]],
  StyleBox[".  The results obtained by using the secant method with the \
initial values  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(p\_0 = 0\)]],
  "  ",
  StyleBox["and  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(p\_1 = 1\)]],
  "  are given by the following computations.  "
}], "Text"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Plot[g[x], {x, 0, 1}, PlotRange \[Rule] {{0, 1}, {\(-1.0\), 1.5}}, 
        Ticks \[Rule] {Range[0, 1, 0.2], Range[\(-1.0\), 1.5, 0.5]}, 
        PlotStyle \[Rule] Blue];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<g[x] = f'[x] = \>", 
        g[x], "\<\n\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(SecantMethod[0, 1, 7];\)\ \)}], "Input"],

Cell[TextData[{
  StyleBox["\tThe conclusion from applying the ",
    FontFamily->"Times-Roman"],
  ButtonBox["secant method",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/SecantMethod.html"], None},
    ButtonStyle->"Hyperlink"],
  StyleBox[" is that  \n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f' \((0.45018361129475976)\)\  = \ \
\(-2.7711166694643907\)*10\^\(-13\)\  \[TildeTilde] \ 0\)]],
  " ",
  StyleBox["\n\nThe second derivative is  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f'' \((x)\)\  = \ 2 + sin \((x)\)\)]],
  "  ",
  StyleBox["and we compute  \n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f'' \((0.45018361129475976)\)\  = \ 2.435130859036607\  > \ 0\)]],
  "  ",
  StyleBox["\n\nHence, by the ",
    FontFamily->"Times-Roman"],
  ButtonBox["second derivative test",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/SecondDerivativeTest.html"], None},
    ButtonStyle->"Hyperlink"],
  StyleBox[", the ",
    FontFamily->"Times-Roman"],
  " ",
  ButtonBox["local minimum",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/LocalMinimum.html"], None},
    ButtonStyle->"Hyperlink"],
  " ",
  StyleBox[" of  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((x)\)\)],
    AspectRatioFixed->True],
  StyleBox["  on the ",
    FontFamily->"Times-Roman"],
  "interval ",
  Cell[BoxData[
      \(\([0, 1]\)\)]],
  " ",
  StyleBox["is  \n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((0.45018361129475976)\)\  = \ \(-0.23246557515821564\)\)]],
  "  "
}], "Text"],

Cell[TextData[{
  StyleBox["Solution using the golden ratio search for the minimum of",
    FontFamily->"Times-Bold",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontFamily->"Times-Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  Cell[BoxData[
      \(f \((x)\)\)],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["\n\nLet  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(a\_0 = 0\)]],
  "  ",
  StyleBox["and  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(b\_0 = 1\)]],
  ", and start with the initial interval  ",
  Cell[BoxData[
      \(\([a\_0, b\_0]\) = \([0, 1]\)\)]],
  StyleBox[".   Formulas (1) and (2) yield",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[BoxData[{
    \(\(a\_0 = 0.0;\)\ \), "\n", 
    \(\(b\_0 = 1.0;\)\), "\n", 
    \(\(r\  = \ 1\/2\ \((\(-1\) + \@5)\);\)\ \), "\[IndentingNewLine]", 
    \(\(c\_0\  = \ 
        a\_0\  + \ \((1 - \(\(-1\) + \@5\)\/2\ )\) \((b\_0 - 
                a\_0)\);\)\ \), "\n", 
    \(\(d\_0\  = \ 
        b\_0\  - \ \((1 - \(\(-1\) + \@5\)\/2\ )\) \((b\_0 - 
                a\_0)\);\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_0\) = \!\(a\_0\) + (1-\!\(\(\(-1\) + \@5\)\/2\) \
)(\!\(b\_0\)-\!\(a\_0\))\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_0\) = \>\"", a\_0, "\< + \>", "\<(\>", 
        1 - \(\(-1\) + \@5\)\/2\ , "\<)(\>", 
        b\_0 - a\_0, "\<)\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_0\) = \>\"", a\_0, "\< + \>", "\<(\>", 
        N[1 - \(\(-1\) + \@5\)\/2\ ], "\<)(\>", 
        b\_0 - a\_0, "\<)\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_0\) = \>\"", 
        c\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(d\_0\) = \!\(b\_0\) - (1-\!\(\(\(-1\) + \@5\)\/2\) \
)(\!\(b\_0\)-\!\(a\_0\))\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(d\_0\) = \>\"", b\_0, "\< - \>", "\<(\>", 
        1 - \(\(-1\) + \@5\)\/2\ , "\<)(\>", 
        b\_0 - a\_0, "\<)\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(d\_0\) = \>\"", b\_0, "\< - \>", "\<(\>", 
        N[1 - \(\(-1\) + \@5\)\/2\ ], "\<)(\>", 
        b\_0 - a\_0, "\<)\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(d\_0\) = \>\"", 
        d\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<The function values are\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(c\_0\)] = \>\"", 
        f[c\_0]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(d\_0\)] = \>\"", 
        f[d\_0]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<Is  f[\!\(c\_0\)] < f[\!\(d\_0\)] ?  \>\"", 
        f[c\_0]\  < \ f[d\_0]];\)\ \)}], "Input"],

Cell[TextData[{
  StyleBox["Since  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f[c\_0]\  < \ f[d\_0]\)]],
  StyleBox[", the new subinterval is",
    FontFamily->"Times-Roman"],
  "  ",
  Cell[BoxData[
      \(\([a\_0, d\_0]\) = \([0.000000, 0.618034]\)\)]],
  StyleBox[".  \nTo continue the iteration we set ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(a\_1 = a\_0\)]],
  StyleBox[", ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(b\_1 = d\_0\)]],
  StyleBox[", ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(d\_1 = c\_0\)]],
  StyleBox[", and compute ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(c\_1 = a\_1\  + \ \((1 - r)\) \((b\_1 - a\_1)\)\)]],
  StyleBox[".  ",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[BoxData[{
    \(\(a\_1 = a\_0;\)\ \), "\n", 
    \(\(b\_1 = d\_0;\)\), "\[IndentingNewLine]", 
    \(\(d\_1 = c\_0;\)\), "\[IndentingNewLine]", 
    \(\(c\_1 = 
        a\_1\  + \ \((1 - r)\) \((b\_1 - 
                a\_1)\);\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(a\_1\) = \>\"", 
        a\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(b\_1\) = \>\"", 
        b\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_1\) = \>\"", 
        c\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(d\_1\) = \>\"", 
        d\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<The function values are\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(c\_1\)] = \>\"", 
        f[c\_1]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<f[\!\(d\_1\)] = \>\"", f[d\_1]];\)\ \)}], "Input"],

Cell[TextData[{
  StyleBox["Now compute and compare ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((c\_1)\)\)]],
  " ",
  StyleBox["and ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((d\_1)\)\)]],
  " ",
  StyleBox["to determine the new subinterval and continue the iteration \
process.  The list of computations are obtained by using the ",
    FontFamily->"Times-Roman"],
  StyleBox["GoldenSearch",
    FontFamily->"Courier New"],
  StyleBox[" subroutine are:  ",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[BoxData[{
    \(\(GoldenSearch[0, 1, 25];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \)}], "Input"],

Cell[TextData[{
  StyleBox["At the twenty-fifth iteration the interval has been narrowed down \
to \n\n\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\([a\_25, b\_25]\)\  = \ \([0.450179, 0.450185]\)\)]],
  StyleBox[".   \n\n",
    FontFamily->"MTSYN"],
  StyleBox["This interval has width  ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(\(\(\ \)\(b\_25 - a\_25 = \(0.000006 = 
          6.0\[Times]10\^\(-6\)\)\)\)\)]],
  StyleBox[".  \n\nHowever, the computed function values at the endpoints \
agree to ten decimal places  \n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((a\_25)\)\  = \ \(-0.232465575132\)\)]],
  "  ",
  StyleBox["\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((b\_25)\)\  = \ \(-0.232465575156\)\)]],
  "  ",
  StyleBox["\n\nhence the algorithm is terminated. A difficulty in using \
search methods is that the function may be \"flat\" near the minimum, and \
this limits the accuracy that can be obtained.  The secant method was able to \
find the more accurate answer\n\n\t\t",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(p\_6\  = \ 0.4501836112947597\)]],
  "\n\tand\n\t\t",
  Cell[BoxData[
      \(f \((p\_6)\)\  = \ \(-0.23246557515821564\)\)]],
  StyleBox["\n",
    FontFamily->"Times-Roman"],
  StyleBox["\n",
    FontFamily->"RMTMI",
    FontSlant->"Italic"],
  StyleBox["Although the golden ratio search is the",
    FontFamily->"Times-Roman"],
  StyleBox[" ",
    FontFamily->"RMTMI"],
  StyleBox["slower in this example, it has desirable feature that it can be \
applied in cases where ",
    FontFamily->"Times-Roman"],
  Cell[BoxData[
      \(f \((x)\)\)]],
  " ",
  StyleBox["is not differentiable.  ",
    FontFamily->"Times-Roman"]
}], "Text"],

Cell[TextData[{
  "Let us compare these answers with ",
