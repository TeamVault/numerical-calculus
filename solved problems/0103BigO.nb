Cell[TextData[{
  StyleBox["Example 4.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  First find Maclaurin expansions for  ",
  Cell[BoxData[
      \(f \((h)\) = \[ExponentialE]\^h\)]],
  "  and  ",
  Cell[BoxData[
      \(g \((h)\) = cos \((h)\)\)]],
  "  of order  ",
  Cell[BoxData[
      \(n = 4\)]],
  "  and   ",
  Cell[BoxData[
      \(m = 6\)]],
  ",  respectively.\nThen experiment and find the order of approximation for \
their sum, product and quotient.  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 4.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[h, i, m, n, p, prod, P, Q, quot, sum];\)\ \ \), "\n", 
    \(\(n = 4;\)\ \ \), "\n", 
    \(\(m = 6;\)\ \ \), "\n", 
    \(\(p = Min[n, m];\)\ \ \), "\n", 
    \(\(f[h_] = \[ExponentialE]\^h;\)\ \ \), "\[IndentingNewLine]", 
    \(\(g[h_] = Cos[h];\)\ \ \), "\[IndentingNewLine]", 
    \(\(P[h_]\  = \ 
        Series[\[ExponentialE]\^h, {h, 0, n - 1}];\)\ \ \), "\n", 
    \(\(Q[h_]\  = \ Series[Cos[h], {h, 0, m - 1}];\)\ \ \), "\n", 
    \(\(Print["\<f[h] = \>", f[h]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<P[h] = \>", 
        Normal[P[h]]\ \ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[h] = \>", Normal[P[h]], "\< + \>", 
        O[h]\^n\ ];\)\ \ \), "\n", 
    \(\(Print[
        P[h]\  \[Equal] \ 
          Normal[P[h]] + O[h]\^n\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g[h] = \>", g[h]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q[h] = \>", 
        Normal[Q[h]]\ \ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[h] = \>", Normal[Q[h]], "\< + \>", 
        O[h]\^m\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[Q[h]\  \[Equal] \ Normal[Q[h]] + O[h]\^m\ ];\)\)}], "Input"],

Cell["Then find the series for their sum.  ", "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(p = Min[n, m];\)\ \ \), "\[IndentingNewLine]", 
    \(\(sum\  = \ 
        Series[\[ExponentialE]\^h + Cos[h], {h, 0, p - 1}];\)\ \ \), "\n", 
