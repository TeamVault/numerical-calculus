Cell[TextData[{
  StyleBox["Data Linearization Method for ",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[ButtonBox["Exponential Curve Fitting",
    ButtonData:>{
      URL[ 
      "http://mathworld.wolfram.com/LeastSquaresFittingExponential.html"], 
      None},
    ButtonStyle->"Hyperlink"],
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[".",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontColor->RGBColor[0, 0, 1]],
  "\n\n\tFit the curve  ",
  Cell[BoxData[
      \(y = c\ \[ExponentialE]\^\(a\ x\)\)]],
  "  to the data points  ",
  Cell[BoxData[
      RowBox[{\((x\_1, y\_1)\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \((x\_2, y\_2)\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "...", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \((x\_n, y\_n)\)}]]],
  ".  \n\nTaking the logarithm of both sides we obtain  ",
  Cell[BoxData[
      \(ln \((y)\) = \(\(ln \((c\ \[ExponentialE]\^\(a\ x\))\)\)\(=\)\(\ \)\)\
\)]],
  Cell[BoxData[
      \(ln \((c)\) + ln \((\[ExponentialE]\^\(a\ x\))\) = 
        ln \((c)\) + a\ x\)]],
  ",  thus\n\n\t\t",
  Cell[BoxData[
      \(ln \((y)\) = a\ x + ln \((c)\)\)]],
  ".  \n\nIntroduce the change of variable  ",
  Cell[BoxData[
      RowBox[{"X", "=", 
        RowBox[{
          RowBox[{"x", "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "  ", "Y"}], 
          "=", \(ln \((y)\)\)}]}]]],
  ".  Then the previous equation becomes  \n\n\t\t",
  Cell[BoxData[
      \(Y = a\ X + ln \((c)\)\)]],
  "  \n\nwhich is a linear equation in the variables X and Y.  \n"
}], "Text",
  Evaluatable->False,
  AspectRatioFixed->False],

Cell[TextData[{
  "\tUse the change of variables  ",
  Cell[BoxData[
      RowBox[{"X", "=", 
        RowBox[{
          RowBox[{"x", "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], 
            StyleBox[
              RowBox[{
                StyleBox[" ",
                  FontFamily->"Times New Roman"], " "}]], "Y"}], 
          "=", \(ln \((y)\)\)}]}]]],
  "  on all the data points and obtain\n\n\t\t",
  Cell[BoxData[
      \(X\_k = \(x\_k\ \ \ and\ \ \ Y\_k = ln \((y\_k)\)\)\)]],
  "  for  ",
  Cell[BoxData[
      \(k = 1, 2,  ... , n\)]],
  ".  \n\n",
  StyleBox["Fit the points",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" ",
    FontWeight->"Bold"],
  " ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"(", 
          RowBox[{\(X\_1\), 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], \(Y\_1\)}], ")"}], 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], 
        RowBox[{"(", 
          RowBox[{\(X\_2\), 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], \(Y\_2\)}], ")"}], 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "...", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], 
        RowBox[{"(", 
          RowBox[{\(X\_n\), 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], \(Y\_n\)}], ")"}]}]]],
  "  with a \"",
  StyleBox["least squares line",
    FontColor->RGBColor[0, 0, 1]],
  "\" of the form  ",
  Cell[BoxData[
      \(Y = A\ X + B\)]],
  ".  \n\nComparing the equations  ",
  Cell[BoxData[
      RowBox[{"Y", "=", 
        RowBox[{
          RowBox[{\(A\ X\), "+", 
            RowBox[{"B", "  ", 
              StyleBox["and",
                FontFamily->"Times New Roman"], 
              StyleBox[
                RowBox[{
                  StyleBox[" ",
                    FontFamily->"Times New Roman"], " "}]], "Y"}]}], 
          "=", \(a\ X + ln \((c)\)\)}]}]]],
  "  we see that  ",
  Cell[BoxData[
      RowBox[{"A", "=", 
        RowBox[{
          RowBox[{"a", "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "  ", "B"}], 
          "=", \(ln \((c)\)\)}]}]]],
  ".  Thus\n\n\t\t",
  Cell[BoxData[
      RowBox[{"a", "=", 
        RowBox[{
          RowBox[{"A", "  ", 
            StyleBox["and",
              FontFamily->"Times New Roman"], "  ", "c"}], " ", 
          "=", \(\[ExponentialE]\^B\)}]}]]],
  " \n\nare used to construct the coefficients which are then used to \"fit \
the curve\"  \n\n\t\t  ",
  Cell[BoxData[
      \(y = c\ \[ExponentialE]\^\(a\ x\)\)]],
  "  \n\nto the given data points  ",
  Cell[BoxData[
      RowBox[{\((x\_1, y\_1)\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \((x\_2, y\_2)\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "...", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \((x\_n, y\_n)\)}]]],
  "  in the xy-plane. "
}], "Text",
  Evaluatable->False,
  AspectRatioFixed->False],

