Cell[TextData[StyleBox["Solution 2.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "Enter the functions  ",
  Cell[BoxData[
      \(f\_1[t, x, y]\)]],
  "  and  ",
  Cell[BoxData[
      \(f\_2[t, x, y]\)]],
  "  and form the vector function  ",
  Cell[BoxData[
      \(F[t, {x, y}]\  = \ {f\_1[t, x, y], f\_2[t, x, y]\ }\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(k\_1 = 1;\)\), "\[IndentingNewLine]", 
    \(\(k\_2 = 1;\)\), "\[IndentingNewLine]", 
    \(\(k\_3 = 1;\)\), "\[IndentingNewLine]", 
    \(\(k\_4 = 1;\)\), "\[IndentingNewLine]", 
    \(\(f\_1[t_, x_, 
          y_] = \ \(k\_1\) x\  - \ \(k\_2\) 
            x\ y;\)\ \ \), "\[IndentingNewLine]", 
    \(\(f\_2[t_, x_, 
          y_] = \(-\ k\_3\) y\  + \ \(k\_4\) 
            x\ y;\)\ \ \), "\[IndentingNewLine]", 
    \(\(F[t_, {x_, y_}]\  = \ {f\_1[t, x, y], 
          f\_2[t, x, y]\ };\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Find numerical solutions to the system of D.E.'s\>"];\)\ \ \
\), "\[IndentingNewLine]", 
    \(\(Print["\<x' = \>", f\_1[t, x, y]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<y' = \>", f\_2[t, x, y]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<F[t,{x,y}] = \>", F[t, {x, y}]\ ];\)\ \ \)}], "Input"],

Cell["Compute the Runge-Kutta solution.", "Text"],

Cell[BoxData[{
    \(\(x\_0\  = \ 2.0;\)\ \), "\[IndentingNewLine]", 
    \(\(y\_0\  = \ 1.0;\)\ \), "\[IndentingNewLine]", 
    \(\(a = 0.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(b = 3.3;\)\ \ \), "\[IndentingNewLine]", 
    \(\(n = 50;\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(pts1\  = Runge2D[a, b, x\_0, y\_0, n];\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\n", 
    \(\(graph1 = \ 
        ListPlot[pts1, PlotJoined \[Rule] True, PlotStyle \[Rule] \ Magenta, 
          PlotRange \[Rule] {{0, 2.0}, {0, 2.0}}, 
          Ticks \[Rule] {Range[0, 2, 0.5], Range[0, 2, 0.5]}, 
          AspectRatio \[Rule] 1];\)\ \ \), "\n", 
    \(\(Print["\<The Runge-Kutta solution for\>"];\)\ \ \), "\
\[IndentingNewLine]", 
    \(\(Print[MatrixForm[{x', y'}], "\< = \>", 
        MatrixForm[F[t, {x, y}]]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[
        MatrixForm[{\*"\"\<\!\(x\_0\)\>\"", \*"\"\<\!\(y\_0\)\>\""}], "\< = \
\>", MatrixForm[{x\_0, y\_0}]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Over the interval [a,b] = [\>", a, "\< , \>", 
