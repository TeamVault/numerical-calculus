Cell[BoxData[{
    \(\(a\  = \ 1.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(b\  = \ 0.2;\)\ \ \), "\[IndentingNewLine]", 
    \(\(c\  = \ 1.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(n\  = \ 6;\)\ \ \), "\n", 
    \(\(m\  = 11;\)\ \ \), "\[IndentingNewLine]", 
    \(\(F[x_] = \ 4  x\  - \ 4  x\^2;\)\ \ \), "\[IndentingNewLine]", 
    \(\(G\_1[t_] = \ 0.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(G\_2[t_] = \ 0.0;\)\ \ \), "\[IndentingNewLine]", 
    \(\(h\  = \ a\/\(n - 1\);\)\ \ \), "\n", 
    \(\(k\  = \ b\/\(m - 1\);\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[i_]\  = F[h\ \((i - 1)\)];\)\ \ \), "\n", 
    \(\(g\_1[j_]\  = G\_1[k \((j - 1)\)];\)\ \ \), "\[IndentingNewLine]", 
    \(\(g\_2[j_]\  = G\_2[k \((j - 1)\)];\)\ \ \)}], "Input",
  AspectRatioFixed->False],

Cell["Now set up the table of solutions.", "Text",
