Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(f[x_]\ \  = \ 2 + Cos[2 \@ x];\)\ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`FilledPlot`\>"];\)\ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\n", 
    \(\(FilledPlot[f[x], {x, 0, 2}, PlotRange \[Rule] {{0, 2}, {0, 3}}, 
        PlotStyle \[Rule] {{Thickness[0.01], Magenta}}, Fills \[Rule] Cyan, 
        AxesLabel \[Rule] {"\<x\>", "\<y\>"}];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \)}], "Input"],

Cell["We will use simulated hand computations for the solution.", "Text"],

Cell[BoxData[
    \(\(\(f[x_] = 2 + Cos[2 \@ x];\)\(\ \)\)\)], "Input"],

Cell[BoxData[{
    \(m1 = \(\(\(2 - 0\)\/1\) \((f[1])\)\(\ \ \)\)\), "\[IndentingNewLine]", 
    \(N[m1]\ \ \)}], "Input"],

Cell[BoxData[{
    \(m2 = \(\(\(2 - 0\)\/2\) \((f[1\/2] + 
            f[3\/2])\)\(\ \ \)\)\), "\[IndentingNewLine]", 
    \(N[m2]\ \ \)}], "Input"],

Cell[BoxData[{
    \(m4 = \(\(\(2 - 0\)\/4\) \((f[1\/4] + f[3\/4] + f[5\/4] + 
            f[7\/4])\)\(\ \ \)\)\), "\[IndentingNewLine]", 
    \(N[m4]\ \ \)}], "Input"],

Cell[BoxData[{
    \(m8 = \(\(\(2 - 0\)\/8\) \((f[1\/8] + f[3\/8] + f[5\/8] + f[7\/8] + 
            f[9\/8] + f[11\/8] + f[13\/8] + 
            f[15\/8])\)\(\ \ \)\)\), "\[IndentingNewLine]", 
    \(N[m8]\ \ \)}], "Input"],

Cell[BoxData[{
    \(m16 = \(\(\(2 - 0\)\/16\) \((f[1\/16] + f[3\/16] + f[5\/16] + 
            f[7\/16] + f[9\/16] + f[11\/16] + f[13\/16] + f[15\/16] + 
            f[17\/16] + f[19\/16] + f[21\/16] + f[23\/16] + f[25\/16] + 
            f[27\/16] + f[29\/16] + 
            f[31\/16])\)\(\ \ \)\)\), "\[IndentingNewLine]", 
    \(N[m16]\ \ \)}], "Input"]
}, Closed]],

