}], "Text"],

Cell[BoxData[
    \(\(\(Adapt[a_, b_, tol_] := 
        Module[{c}, \[IndentingNewLine]c = \(a + b\)\/2; \[IndentingNewLine]X \
= Append[X, c]; \ \[IndentingNewLine]Sab = 
            Simpson[a, b]; \ \[IndentingNewLine]Sac = 
            Simpson[a, c]; \ \[IndentingNewLine]Scb = 
            Simpson[c, b]; \ \[IndentingNewLine]If[
            Abs[Sab - Sac - Scb] < 
              tol, \[IndentingNewLine]Print["\<\[Integral]\>"\_a\^b, 
              f[x], "\<\[DifferentialD]x = \>", 
              Sab]; \ \[IndentingNewLine]Print["\<\[Integral]\>"\_a\^c, 
              f[x], "\<\[DifferentialD]x = \>", 
              Sac]; \ \[IndentingNewLine]Print["\<\[Integral]\>"\_c\^b, 
              f[x], "\<\[DifferentialD]x = \>", 
              Scb]; \ \[IndentingNewLine]Print["\<Is the agreement within tol \
= \>", tol]; \ \[IndentingNewLine]Print["\<----------------\>"]; \ \
\[IndentingNewLine]Return[Sac + Scb], \[IndentingNewLine]Return[
              Adapt[a, c, tol\/2] + 
                Adapt[c, b, tol\/2]]\ \ ];\ ];\)\(\ \)\)\)], "Input"],

