Cell[TextData[{
  "Determine the nature of the singularity at ",
  Cell[BoxData[
      \(x\_0 = 0\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(X\_0 = 0;\)\ \), "\[IndentingNewLine]", 
    \(\(P[x_] = 2  x;\)\), "\[IndentingNewLine]", 
    \(\(Q[x_] = 3;\)\), "\[IndentingNewLine]", 
    \(\(R[x_] = \(-1\);\)\), "\[IndentingNewLine]", 
    \(\(f\_1[x_] = Q[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(f\_2[x_] = R[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(de = 
        P[x]\ \(y''\)[x] + \ \ Q[x]\ \(y'\)[x] + R[x] y[x]\  \[Equal] \ 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(If[
        P[X\_0] \[Equal] 0, \[IndentingNewLine]Print["\<P[x] = \>", 
          P[x]]; \ \[IndentingNewLine]Print["\<P[\>", X\_0, "\<] = \>", 
          P[X\_0]]; \[IndentingNewLine]Print[\*"\"\<The point \!\(x\_0\) = \>\
\"", X\_0, "\< is a singular point.\>"]; \[IndentingNewLine]Print["\<Proceed \
with a Frobenius solution.\>"];, \[IndentingNewLine]Print[\*"\"\<The point \!\
\(x\_0\) = \>\"", 
          X\_0, "\< is NOT singular point.\>"]; \
\[IndentingNewLine]Print["\<Use the power series method for an ordinary \
point.\>"]; \[IndentingNewLine]Print[\*"\"\<y''[x] + \!\(f\_1\)[x]y'[x] + \
\!\(f\_2\)[x]y[x] = 0\>\""]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_1\)[x]=\!\
\(Q[x]\/P[x]\) = \>\"", Together[Q[x]\/P[x]], "\< = \>", 
          Series[f\_1[x], {x, X\_0, 
              3}]]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_2\)[x]=\!\(R[x]\/P[\
x]\) = \>\"", Together[R[x]\/P[x]], "\< = \>", 
          Series[f\_2[x], {x, X\_0, 3}]]; \[IndentingNewLine]If[
          X\_0 \[Equal] 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\!\(x\^n\)\>\"", "\<, etc.\>"]]; \ \
\[IndentingNewLine]If[X\_0 > 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x-\>", 
            X\_0, \*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]]; \
\[IndentingNewLine]If[X\_0 < 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x+\>", \(-X\_0\), \
\*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]];\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Construct the Indicial Equation.",
    FontWeight->"Bold",
    FontColor->RGBColor[0.996109, 0, 0.996109]],
  StyleBox[" ",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]]
}], "Text"],

Cell[BoxData[{
    \(\(\[Phi]\_1[x_] = \ \ x  Q[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(\[Phi]\_2[x_] = \ \ \(x\^2\) 
          R[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(p\_0\  = \ 
        Limit[x  Q[x]\/P[x], x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(q\_0\  = \ 
        Limit[\(x\^2\) R[x]\/P[x], 
          x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(Clear[r];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = 
        r \((r - 1)\) + \(p\_0\) r\  + \ q\_0\  \[Equal] \ 
          0;\)\), "\[IndentingNewLine]", 
    \(\(Print["\<P[x] = \>", P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q[x] = \>", Q[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<R[x] = \>", R[x], "\<\n\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_1\)[x_] =   x\!\(Q[x]\/P[x]\) = \>\"", 
        x  Q[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(p\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< x\!\(Q[x]\/P[x]\) = \>\"", p\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_2\)[x_] =  \!\(x\^2\)\!\(R[x]\/P[x]\) = \
\>\"", \(x\^2\) R[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(q\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< \!\(x\^2\)\!\(R[x]\/P[x]\) = \>\"", q\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<The indicial equation is\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<r(r-1) + \!\(p\_0\) r + \!\(q\_0\) \[Equal] \
0\>\""];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = MapAll[Expand, ieqn];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \)}], "Input"],

Cell[TextData[StyleBox["Find the Roots of the Indicial Equation. ",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Print["\<The indicial equation is\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<The roots are\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(solset = Solve[ieqn, r];\)\ \), "\[IndentingNewLine]", 
    \(\(r\_1\  = \ 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(r\_2\  = 
        solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_1\) = \>\"", 
        r\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_2\) = \>\"", r\_2];\)\ \)}], "Input"],

Cell[TextData[{
  "Form the first Frobenius solution corresponding to the root  ",
  Cell[BoxData[
      \(r\_1 = \(-1\)\/2\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, s, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r = \(-1\)\/2;\)\), "\n", 
    \(\(n = 7;\)\), "\n", 
    \(\(s[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%n c\_k\ x\^k)\) + \(x\^r\) 
              O[x]\^\(n + 1\)]\ ;\)\), "\n", 
    \(\(coeff = Table[c\_k, {k, 1, n}];\)\), "\n", 
    \(\(Print["\<  s[x] = \>", s[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\< s'[x] = \>", \(s'\)[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s''[x] = \>", \(s''\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Substitute into\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<2 x s''[x] + 3 s'[x] - s[x] \[Equal] 0\>"];\)\), "\n", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(\(deqn = 
        Expand[2  x\ \(\(s'\)'\)[x] + \ \ 3\ \(s'\)[x] - \ s[x]\ ] \[Equal] 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[deqn];\)\)}], "Input"],

Cell["Form the set of equations to solve and do it. ", "Text"],

Cell[BoxData[{
    \(\(eqns = LogicalExpand[deqn];\)\), "\n", 
    \(\(Print[deqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[TableForm[ReplaceAll[eqns, And \[Rule] List]]\ ];\)\ \), "\n", 
    \(\(solset = Solve[eqns, coeff];\)\), "\n", 
    \(\(Print[
        TableForm[
          Sort[solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)]]\ ];\)\ \), "\n", 
    \(\(s[x_] = 
        ReplaceAll[s[x], 
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)];\
\)\), "\n", 
    \(\(Print["\<y = \>", s[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", Normal[s[x]], "\< + ...\>"\ ];\)\)}], "Input"],

Cell["The first Frobenius solution is: ", "Text"],

Cell[BoxData[{
    \(\(s1[x_] = ReplaceAll[s[x], c\_0 \[Rule] 1];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", s1[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \>", 
        Normal[s1[x]], "\< + ...\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \[InvisibleSpace]\>", \((\[InvisibleSpace]x\^r)\), "\
\<(\>", Normal[\(x\^\(-r\)\) s1[x]], "\< + ...)\>"];\)\)}], "Input"],

Cell[TextData[{
  "Form the second Frobenius solution corresponding to the root  ",
  Cell[BoxData[
      \(r\_2 = 0\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, s, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r = 0;\)\), "\n", 
    \(\(n = 7;\)\), "\n", 
    \(\(s[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%n c\_k\ x\^k)\) + \(x\^r\) 
              O[x]\^\(n + 1\)]\ ;\)\), "\n", 
    \(\(coeff = Table[c\_k, {k, 1, n}];\)\ \), "\n", 
    \(\(deqn = 
        Expand[2  x\ \(\(s'\)'\)[x] + \ \ 3\ \(s'\)[x] - \ s[x]\ ] \[Equal] 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<  s[x] = \>", s[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\< s'[x] = \>", \(s'\)[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s''[x] = \>", \(s''\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Substitute into\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<2 x s''[x] + 3 s'[x] - s[x] \[Equal] 0\>"];\)\), "\n", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[deqn];\)\ \)}], "Input"],

Cell["Form the set of equations to solve and do it. ", "Text"],

Cell[BoxData[{
    \(\(eqns = LogicalExpand[deqn];\)\), "\n", 
    \(\(Print[deqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[TableForm[ReplaceAll[eqns, And \[Rule] List]]\ ];\)\ \), "\n", 
    \(\(solset = Solve[eqns, coeff];\)\), "\n", 
    \(\(Print[
        TableForm[
          Sort[solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)]]\ ];\)\ \), "\n", 
    \(\(s[x_] = 
        ReplaceAll[s[x], 
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)];\
\)\), "\n", 
    \(\(Print["\<y = \>", s[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", Normal[s[x]], "\<+ ...\>"\ ];\)\)}], "Input"],

Cell["The second Frobenius solution is: ", "Text"],

Cell[BoxData[{
    \(\(s2[x_] = ReplaceAll[s[x], c\_0 \[Rule] 1];\)\), "\n", 
    \(\(Print["\<f2[x] = \>", s2[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f2[x] = \>", Normal[s2[x]], "\< + ...\>"\ ];\)\ \)}], "Input"],

Cell[TextData[{
  "After you are done, use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s DSolve subroutine to get the answer and check out its series expansion. \
\nThis will require slight fussing around with the appropriate multiple of  \
\"",
  StyleBox["Sinh",
    FontFamily->"Courier New"],
  "\". "
}], "Text"],

Cell[BoxData[{
    \(x = 0; \ y = 0; \ Remove[x, y];\), "\[IndentingNewLine]", 
    \(\(de = 
        2  x\ \(\(y'\)'\)[x] + \ \ 3\ \(y'\)[x] - \ y[x] \[Equal] 
          0;\)\), "\n", 
    \(\(sol1 = 
        DSolve[2  x\ \(\(y'\)'\)[x] + \ \ 3\ \(y'\)[x] - \ y[x] \[Equal] 0, 
          y[x], x];\)\), "\n", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(Print[sol1];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<Two linearly independent solutions are\>", "\<\n\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(g1[x_] = Cosh[\@2\ \@x]\/\@x;\)\), "\n", 
    \(\(sol11 = 
        ReplaceAll[
          sol1, {C[1] \[Rule] 1\/2, 
            C[2] \[Rule] \ \(-\(\@2\/2\)\)}];\)\), "\n", 
    \(\(g1[x_] = 
        Expand[sol11\_\(\(\[LeftDoubleBracket]\)\(1, 1, 2\)\(\
\[RightDoubleBracket]\)\)];\)\), "\[IndentingNewLine]", 
    \(\(g1[x_] = 
        FullSimplify[g1[x], Trig \[Rule] False];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\), "\n", 
    \(\(sol22 = 
        ReplaceAll[
          sol1, {C[1] \[Rule] 1\/\(2 \@ 2\), 
            C[2] \[Rule] \ 1\/2}];\)\), "\n", 
    \(\(g2[x_] = 
        Expand[sol22\_\(\(\[LeftDoubleBracket]\)\(1, 1, 2\)\(\
\[RightDoubleBracket]\)\)];\)\), "\[IndentingNewLine]", 
    \(\(g2[x_] = 
        FullSimplify[g2[x], Trig \[Rule] False];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\)}], "Input"],

Cell["\<\
At this time we could plot the series approximations and the analytic \
solutions. To see the difference in the graphs we will reduce the number of \
terms in the series.\
\>", "Text"],

Cell[BoxData[{
    \(\(S1[x_] = Take[\ Normal[s1[x]\ ], 4];\)\), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Plot[{S1[x], g1[x]}, {x, 0, 9}, PlotRange \[Rule] {{0, 9}, {0, 10}}, 
        PlotStyle\  \[Rule] {Green, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S1[x] = \>", S1[x]\ ];\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(S2[x_] = Take[Normal[s2[x]], 4];\)\), "\n", 
    \(\(Plot[{S2[x], g2[x]}, {x, 0, 10}, 
        PlotRange \[Rule] {{0, 10}, {0, 10}}, 
        PlotStyle\  \[Rule] {Cyan, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S2[x] = \>", S2[x]\ ];\)\), "\n", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["The recursive formula for the coefficients.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\n\tIf we look at the series in more depth we will be able to obtain the \
analytic solutions as infinite sums.  First find the recursive formula for \
the coefficients of  ",
  Cell[BoxData[
      \(x\^\(r + n\)\)]],
  ".  If you try this be sure to use the  \" := \"  replacement delayed \
structure to avoid an infinite recursion.  For this example the trial term  \
",
  Cell[BoxData[
      \(t[x] = \[Sum]\+\(k = n - 1\)\%n c\_k\ x\^\(r + k\)\)]],
  " works with the replacements ",
  Cell[BoxData[
      \({x\^\(n + r\) \[Rule] 0, x\^\(\(-1\) + n + r\) \[Rule] 1, 
        x\^\(\(-2\) + n + r\) \[Rule] 0, x\^\(1 + n + r\) \[Rule] 0, 
        x\^\(2 + n + r\) \[Rule] 0}\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, t, x];\)\ \), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(t[x_] := \[Sum]\+\(k = n - 1\)\%n c\_k\ x\^\(r + k\);\)\), "\n", 
    \(\(nterm = 
        2  x\ \(\(t'\)'\)[x] + \ \ 3\ \(t'\)[x] - \ t[x] \[Equal] 
          0\ ;\)\), "\n", 
    \(\(nterm = ExpandAll[nterm];\)\ \), "\n", 
    \(\(Print["\<The general term in the sum is\>", "\<\n\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print[nterm];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Simplify it.\>", "\<\n\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(nterm = 
        ReplaceAll[
          nterm, {x\^\(n + r\) \[Rule] 0, x\^\(\(-1\) + n + r\) \[Rule] 1, 
            x\^\(\(-2\) + n + r\) \[Rule] 0, x\^\(1 + n + r\) \[Rule] 0, 
            x\^\(2 + n + r\) \[Rule] 0}];\)\ \), "\n", 
    \(\(Print[nterm];\)\ \), "\[IndentingNewLine]", 
    \(\(solset\  = \ Solve[nterm, c\_n];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Solve it for the recursive formula.\>", "\<\n\>"];\
\)\ \), "\[IndentingNewLine]", 
    \(\(Print[
        solset\_\(\(\[LeftDoubleBracket]\)\(1, \
1\)\(\[RightDoubleBracket]\)\)];\)\ \)}], "Input"],

Cell["\<\
If you can't get the above computation to work, then just type in the \
recursive formula. \
\>", "Text"],

Cell[BoxData[
    \(\(\(c\_n_ := 
        c\_\(n - 1\)\/\(n + 2\ n\^2 + r + 4\ n\ r + 2\ r\^2\);\)\(\ \)\)\)], \
"Input"],

Cell[TextData[{
  "Now look at each series individually.  The first Frobenius corresponds to  \
",
  Cell[BoxData[
      \(r\_1 = \(-1\)\/2\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, x];\)\ \), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r\  = \ \(-1\)\/2\ ;\)\), "\n", 
    \(\(c\_0 = 1\ ;\)\), "\n", 
    \(\(c\_n_ := 
        c\_\(n - 1\)\/\(n + 2\ n\^2 + r + 4\ n\ r + 2\ r\^2\);\)\ \), "\
\[IndentingNewLine]", 
    \(\(s1[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%11\( c\_k\) 
                  x\^k)\)]\ ;\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<c\>"\_i, "\< = \>", c\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_n\) = \>\"", 
        Simplify[\*"\"\<\!\(c\_\(n - 1\)\)\>\""\/\(n + 2\ n\^2 + r + 4\ n\ r \
+ 2\ r\^2\)]];\)\ \), "\[IndentingNewLine]", 
    \(Print["\<s1[x] = \>", s1[x]\ ]\)}], "Input"],

Cell[TextData[{
  "Now look at the second Frobenius series corresponding to  ",
  Cell[BoxData[
      \(r\_2 = 0\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r\  = 0\ ;\)\), "\n", 
    \(\(c\_0 = 1\ ;\)\), "\n", 
    \(\(c\_n_ := 
        c\_\(n - 1\)\/\(n + 2\ n\^2 + r + 4\ n\ r + 2\ r\^2\);\)\ \), "\
\[IndentingNewLine]", 
    \(\(s2[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%11\( c\_k\) 
                  x\^k)\)\ ];\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<c\>"\_i, "\< = \>", c\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_n\) = \>\"", 
        Simplify[\*"\"\<\!\(c\_\(n - 1\)\)\>\""\/\(n + 2\ n\^2 + r + 4\ n\ r \
+ 2\ r\^2\)]];\)\ \), "\[IndentingNewLine]", 
    \(Print["\<s2[x] = \>", s2[x]\ ]\)}], "Input"],

Cell["\<\
The explicit formulas for the coefficients for the first Frobenius series \
are: \
\>", "Text"],

Cell[BoxData[{
    \(\(a\_k_ := 1\/\(\(k!\)\ \(\((2  k - 1)\)!!\)\);\)\), "\n", 
    \(\(r = \(-\(1\/2\)\);\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<a\>"\_i, "\< = \>", a\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(a\_k\) = \>\"", 
        1\/\(\(k!\)\ \(\((2  k - 1)\)!!\)\)];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \>", 
        x\^r, "\<(\>", \((\[Sum]\+\(k = 0\)\%11\( a\_k\) 
              x\^k)\)\ , "\< + ... )\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(f1[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%11\( a\_k\) 
                  x\^k)\)\ ];\)\), "\n", 
    \(\(Print["\<s1[x] = \>", f1[x]\ ];\)\ \)}], "Input"],

Cell[BoxData[{
    \(\(S1[x_] = Take[\ Normal[s1[x]\ ], 4];\)\), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Plot[{S1[x], f1[x]}, {x, 0, 9}, PlotRange \[Rule] {{0, 9}, {0, 10}}, 
        PlotStyle\  \[Rule] {Green, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S1[x] = \>", S1[x]\ ];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", f1[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(b\_k_ := 1\/\(\(k!\)\ \(\((2  k + 1)\)!!\)\);\)\), "\n", 
    \(\(r = 0;\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<b\>"\_i, "\< = \>", b\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(b\_k\) = \>\"", 
        1\/\(\(k!\)\ \(\((2  k + 1)\)!!\)\)];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f2[x] = \>", 
        x\^r, "\<(\>", \((\[Sum]\+\(k = 0\)\%11\( b\_k\) 
              x\^k)\), "\< + ... )\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(f2[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%11\( b\_k\) 
                  x\^k)\)\ ];\)\), "\n", 
    \(\(Print["\<s2[x] = \>", f2[x]\ ];\)\ \)}], "Input"],

Cell[BoxData[{
    \(\(S2[x_] = Take[Normal[s2[x]], 4];\)\), "\n", 
    \(\(Plot[{S2[x], f2[x]}, {x, 0, 10}, 
        PlotRange \[Rule] {{0, 10}, {0, 10}}, 
        PlotStyle\  \[Rule] {Cyan, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S2[x] = \>", S2[x]\ ];\)\), "\n", 
    \(\(Print["\<f2[x] = \>", f2[x]\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Note.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  " We cannot add up the infinite number of terms in this sequence because ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " does not know this formula yet. "
}], "Text"],

Cell[TextData[{
  StyleBox["Remark.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  " However, ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " can treat infinite series \"like a function.\" "
}], "Text"],

Cell[BoxData[{
    \(\(S1[x_] = Take[\ Normal[s1[x]\ ], 4];\)\), "\n", 
    \(\(f1[x_] = 
        Expand[\(x\^\(-\(1\/2\)\)\) \((\[Sum]\+\(k = 0\)\%\[Infinity]\( 
                      1\/\(\(k!\)\ \(\((2  k - 1)\)!!\)\)\) 
                  x\^k)\)\ ];\)\), "\[IndentingNewLine]", 
    \(\(Plot[{S1[x], f1[x]}, {x, 0, 9}, PlotRange \[Rule] {{0, 9}, {0, 10}}, 
        PlotStyle\  \[Rule] {Green, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S1[x] = \>", S1[x]\ ];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", f1[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(S2[x_] = Take[Normal[s2[x]], 4];\)\), "\n", 
    \(\(f2[x_] = 
        Expand[\(x\^0\) \((\[Sum]\+\(k = 0\)\%\[Infinity]\( 
                      1\/\(\(k!\)\ \(\((2  k + 1)\)!!\)\)\) 
                  x\^k)\)\ ];\)\), "\[IndentingNewLine]", 
    \(\(Plot[{S2[x], f2[x]}, {x, 0, 10}, 
        PlotRange \[Rule] {{0, 10}, {0, 10}}, 
        PlotStyle\  \[Rule] {Cyan, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S2[x] = \>", S2[x]\ ];\)\), "\n", 
    \(\(Print["\<f2[x] = \>", f2[x]\ ];\)\)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Example 3.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Use Frobenius series to solve the D. E.   \n\n\t\t",
  Cell[BoxData[
      \(2  x\^2\ \(\(y'\)'\)[x] + 
          3\ x\ \ \(y'\)[x] - \((\ x\^2 + 1)\)\ y[x]\  = \ 0\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 3.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "Determine the nature of the singularity at ",
  Cell[BoxData[
      \(x\_0 = 0\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(X\_0 = 0;\)\ \), "\[IndentingNewLine]", 
    \(\(P[x_] = 2  x\^2;\)\), "\[IndentingNewLine]", 
    \(\(Q[x_] = 3\ x;\)\), "\[IndentingNewLine]", 
    \(\(R[x_] = \(-\((\ x\^2 + 1)\)\);\)\), "\[IndentingNewLine]", 
    \(\(f\_1[x_] = Q[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(f\_2[x_] = R[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(de = 
        P[x]\ \(y''\)[x] + \ \ Q[x]\ \(y'\)[x] + R[x] y[x]\  \[Equal] \ 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(If[
        P[X\_0] \[Equal] 0, \[IndentingNewLine]Print["\<P[x] = \>", 
          P[x]]; \ \[IndentingNewLine]Print["\<P[\>", X\_0, "\<] = \>", 
          P[X\_0]]; \[IndentingNewLine]Print[\*"\"\<The point \!\(x\_0\) = \>\
\"", X\_0, "\< is a singular point.\>"]; \[IndentingNewLine]Print["\<Proceed \
with a Frobenius solution.\>"];, \[IndentingNewLine]Print[\*"\"\<The point \!\
\(x\_0\) = \>\"", 
          X\_0, "\< is NOT singular point.\>"]; \
\[IndentingNewLine]Print["\<Use the power series method for an ordinary \
point.\>"]; \[IndentingNewLine]Print[\*"\"\<y''[x] + \!\(f\_1\)[x]y'[x] + \
\!\(f\_2\)[x]y[x] = 0\>\""]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_1\)[x]=\!\
\(Q[x]\/P[x]\) = \>\"", Together[Q[x]\/P[x]], "\< = \>", 
          Series[f\_1[x], {x, X\_0, 
              3}]]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_2\)[x]=\!\(R[x]\/P[\
x]\) = \>\"", Together[R[x]\/P[x]], "\< = \>", 
          Series[f\_2[x], {x, X\_0, 3}]]; \[IndentingNewLine]If[
          X\_0 \[Equal] 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\!\(x\^n\)\>\"", "\<, etc.\>"]]; \ \
\[IndentingNewLine]If[X\_0 > 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x-\>", 
            X\_0, \*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]]; \
\[IndentingNewLine]If[X\_0 < 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x+\>", \(-X\_0\), \
\*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]];\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Construct the Indicial Equation.",
    FontWeight->"Bold",
    FontColor->RGBColor[0.996109, 0, 0.996109]],
  StyleBox[" ",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]]
}], "Text"],

Cell[BoxData[{
    \(\(\[Phi]\_1[x_] = \ \ x  Q[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(\[Phi]\_2[x_] = \ \ \(x\^2\) 
          R[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(p\_0\  = \ 
        Limit[x  Q[x]\/P[x], x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(q\_0\  = \ 
        Limit[\(x\^2\) R[x]\/P[x], 
          x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(Clear[r];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = 
        r \((r - 1)\) + \(p\_0\) r\  + \ q\_0\  \[Equal] \ 
          0;\)\), "\[IndentingNewLine]", 
    \(\(Print["\<P[x] = \>", P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q[x] = \>", Q[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<R[x] = \>", R[x], "\<\n\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_1\)[x_] =   x\!\(Q[x]\/P[x]\) = \>\"", 
        x  Q[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(p\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< x\!\(Q[x]\/P[x]\) = \>\"", p\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_2\)[x_] =  \!\(x\^2\)\!\(R[x]\/P[x]\) = \
\>\"", \(x\^2\) R[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(q\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< \!\(x\^2\)\!\(R[x]\/P[x]\) = \>\"", q\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<The indicial equation is\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<r(r-1) + \!\(p\_0\) r + \!\(q\_0\) \[Equal] \
0\>\""];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = MapAll[Expand, ieqn];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \)}], "Input"],

Cell[TextData[StyleBox["Find the Roots of the Indicial Equation. ",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Print["\<The indicial equation is\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<The roots are\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(solset = Solve[ieqn, r];\)\ \), "\[IndentingNewLine]", 
    \(\(r\_1\  = \ 
        solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(r\_2\  = 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_1\) = \>\"", 
        r\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_2\) = \>\"", r\_2];\)\ \)}], "Input"],

Cell[TextData[{
  "Form the first Frobenius solution corresponding to the root  ",
  Cell[BoxData[
      \(r\_1 = 1\/2\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, s, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r = 1\/2;\)\), "\n", 
    \(\(n = 10;\)\), "\n", 
    \(\(s[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%n c\_k\ x\^k)\) + 
            O[x]\^\(n + 1\)];\)\), "\n", 
    \(\(coeff = Table[c\_k, {k, 1, n}];\)\), "\n", 
    \(\(Print["\<  s[x] = \>", s[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\< s'[x] = \>", \(s'\)[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s''[x] = \>", \(s''\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Substitute into\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<2 \!\(x\^2\) s''[x] + 3 x s'[x] - (\!\(x\^2\)+1) s[x] \
\[Equal] 0\>\""];\)\), "\n", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(\(deqn = 
        Expand[2 
                 x\^2\ \(\(s'\)'\)[x] + \ \ 3\ x\ \ \(s'\)[x] - \((\ 
                  x\^2\  + 1)\)\ s[x]] \[Equal] 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[deqn];\)\)}], "Input"],

Cell["Form the set of equations to solve and do it. ", "Text"],

Cell[BoxData[{
    \(\(eqns = LogicalExpand[deqn];\)\), "\n", 
    \(\(Print[deqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[TableForm[ReplaceAll[eqns, And \[Rule] List]]\ ];\)\ \), "\n", 
    \(\(solset = Solve[eqns, coeff];\)\), "\n", 
    \(\(Print[
        TableForm[
          Sort[solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)]]\ ];\)\ \), "\n", 
    \(\(s[x_] = 
        ReplaceAll[s[x], 
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)];\
\)\), "\n", 
    \(\(Print["\<y = \>", s[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", Normal[s[x]], "\< + ...\>"\ ];\)\)}], "Input"],

Cell["The first Frobenius solution is: ", "Text"],

Cell[BoxData[{
    \(\(s1[x_] = ReplaceAll[s[x], c\_0 \[Rule] 1];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", s1[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \>", 
        Normal[s1[x]], "\< + ...\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \[InvisibleSpace]\>", \((\[InvisibleSpace]x\^r)\), \
"\<(\>", Normal[\(x\^\(-r\)\) s1[x]], "\< + ...)\>"];\)\)}], "Input"],

Cell[TextData[{
  "Form the second ",
  "Frobenius solution corresponding to the root  ",
  Cell[BoxData[
      \(r\_2 = \(-1\)\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, s, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r = \(-1\);\)\), "\n", 
    \(\(n = 10;\)\), "\n", 
    \(\(s[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%n c\_k\ x\^k)\) + 
            O[x]\^\(n + 1\)]\ ;\)\), "\n", 
    \(\(coeff = Table[c\_k, {k, 1, n}];\)\), "\n", 
    \(\(Print["\<  s[x] = \>", s[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\< s'[x] = \>", \(s'\)[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s''[x] = \>", \(s''\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Substitute into\>"];\)\), "\n", 
    \(\(Print[\*"\"\<2 \!\(x\^2\) s''[x] + 3 x s'[x] - (\!\(x\^2\)+1) s[x] \
\[Equal] 0\>\""];\)\), "\n", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(deqn = 
      Expand[2  x\^2\ \(\(s'\)'\)[x] + \ \ 3\ x\ \ \(s'\)[x] - \((\ 
                x\^2\  + 1)\)\ s[x]] \[Equal] 0\)}], "Input"],

Cell["Form the set of equations to solve and do it. ", "Text"],

Cell[BoxData[{
    \(\(eqns = LogicalExpand[deqn];\)\), "\n", 
    \(\(Print[deqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[TableForm[ReplaceAll[eqns, And \[Rule] List]]\ ];\)\ \), "\n", 
    \(\(solset = Solve[eqns, coeff];\)\), "\n", 
    \(\(Print[
        TableForm[
          Sort[solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)]]\ ];\)\ \), "\n", 
    \(\(s[x_] = 
        ReplaceAll[s[x], 
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)];\
\)\), "\n", 
    \(\(Print["\<y = \>", s[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", Normal[s[x]], "\< + ...\>"\ ];\)\ \)}], "Input"],

Cell["The second Frobenius solution is: ", "Text"],

Cell[BoxData[{
    \(\(s2[x_] = ReplaceAll[s[x], c\_0 \[Rule] 1];\)\), "\n", 
    \(\(Print["\<f2[x] = \>", s2[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<f2[x] = \>", Normal[s2[x]], "\< + ...\>"\ ];\)\ \)}], "Input"],

Cell[TextData[{
  "After you are done, use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s DSolve subroutine to get the answer and check out its series expansion. \
\nThis will require some fussing around with the appropriate multiple of the \
Bessel function. "
}], "Text"],

Cell[BoxData[{
    \(x = 0; \ y = 0; \ Remove[x, y];\), "\[IndentingNewLine]", 
    \(\(de = 
        2  x\^2\ \(\(y'\)'\)[x] + \ \ 3\ x\ \ \(y'\)[x] - \((\ 
                x\^2\  + 1)\)\ y[x] \[Equal] 0;\)\), "\n", 
    \(\(sol1 = DSolve[de, y[x], x];\)\), "\n", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(Print[sol1];\)\), "\[IndentingNewLine]", 
    \(\(Print[FunctionExpand[sol1]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<Two linearly independent solutions are\>", "\<\n\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(g1[x_] = 
        2\ 2\^\(1/8\)\ Gamma[7\/4] 
          BesselI[3\/4, x\/\@2]\/x\^\(1/4\);\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\), "\n", 
    \(\(g1[x_] = FullSimplify[g1[x]\ ];\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\), "\n", 
    \(\(g2[x_] = \(Gamma[1\/4]\/\(2\ 2\^\(\(1/8\)\(\ \)\)\)\) 
          BesselI[\(-\(3\/4\)\), x\/\@2]\/x\^\(1/4\);\)\), "\n", 
    \(\(Print["\<\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\), "\n", 
    \(\(g2[x_] = FullSimplify[g2[x]\ ];\)\), "\n", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\)}], "Input"],

Cell["\<\
At this time we could plot the series approximations and the analytic \
solutions. To see the difference in the graphs we will reduce the number of \
terms in the series. \
\>", "Text"],

Cell[BoxData[{
    \(\(S1[x_] = Take[\ Normal[s1[x]], 3];\)\), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Plot[{S1[x], g1[x]}, {x, 0, 5}, PlotRange \[Rule] {{0, 5}, {0, 10}}, 
        PlotStyle\  \[Rule] {Green, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S1[x] = \>", S1[x]\ ];\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(S2[x_] = Take[Normal[s2[x]], 3];\)\), "\n", 
    \(\(Plot[{S2[x], g2[x]}, {x, 0, 5}, PlotRange \[Rule] {{0, 5}, {0, 10}}, 
        PlotStyle\  \[Rule] {Cyan, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S2[x] = \>", S2[x]\ ];\)\), "\n", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["The recursive formula for the coefficients.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\nIf we look at the series in more depth we will be able to obtain the \
analytic solutions as infinite sums.  First find the recursive formula for \
the coefficients of  ",
  Cell[BoxData[
      \(x\^\(r + n\)\)]],
  ".  If you try this be sure to use the  \" := \"  replacement delayed \
structure to avoid an infinite recursion.  Also, include the semicolon at the \
end of the lines. "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, t, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(t[x_] := \[Sum]\+\(k = n - 2\)\%n c\_k\ x\^\(r + k\)\ ;\)\), "\n", 
    \(\(nterm = 
        2 \( x\^2\) \(\(t'\)'\)[x] + 
            3  x\ \(t'\)[x] - \((\ x\^2\  + 1)\)\ t[x] \[Equal] 
          0;\)\), "\n", 
    \(\(nterm = ExpandAll[nterm];\)\ \), "\n", 
    \(\(Print["\<The general term in the sum is\>", "\<\n\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print[nterm];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Simplify it.\>", "\<\n\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(nterm = 
        ReplaceAll[
          nterm, {x\^\(n + r\) \[Rule] 1, x\^\(\(-1\) + n + r\) \[Rule] 0, 
            x\^\(\(-2\) + n + r\) \[Rule] 0, x\^\(1 + n + r\) \[Rule] 0, 
            x\^\(2 + n + r\) \[Rule] 0}];\)\ \), "\n", 
    \(\(Print[nterm];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Solve it.\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(solset\  = \ Solve[nterm, c\_n];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<Solve it for the recursive formula.\>", "\<\n\>"];\
\)\ \), "\[IndentingNewLine]", 
    \(\(Print[
        solset\_\(\(\[LeftDoubleBracket]\)\(1, \
1\)\(\[RightDoubleBracket]\)\)];\)\ \)}], "Input"],

Cell["\<\
If you can't get the above computation to work, then just type in the \
recursive formula. \
\>", "Text"],

Cell[BoxData[
    \(\(c\_n_ := 
        c\_\(n - 2\)\/\(2\ \((n + r)\)\^2 + n + \ r - 1\);\)\)], "Input"],

Cell[TextData[{
  "Now look at each series individually.  The first Frobenius series \
corresponding to  ",
  Cell[BoxData[
      \(r\_1 = 1\/2\)]],
  "  is: "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\), "\[IndentingNewLine]", 
    \(\(r\  = \ 1\/2\ ;\)\), "\n", 
    \(\(c\_0 = 1\ ;\)\), "\n", 
    \(\(c\_1 = 0\ ;\)\), "\n", 
    \(\(c\_n_ := 
        c\_\(n - 2\)\/\(2\ \((n + r)\)\^2 + n + \ r - 1\);\)\), "\
\[IndentingNewLine]", 
    \(\(f1[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%13\( c\_k\) 
                  x\^k)\)]\ ;\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<c\>"\_i, "\< = \>", c\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_n\) = \>\"", 
        Simplify[\*"\"\<\!\(c\_\(n - 2\)\)\>\""\/\(2\ \((n + r)\)\^2 + n + \ \
r - 1\)]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \>", f1[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(S1[x_] = Take[\ Normal[s1[x]], 3];\)\), "\n", 
    \(\(Plot[{S1[x], f1[x]}, {x, 0, 5}, PlotRange \[Rule] {{0, 5}, {0, 10}}, 
        PlotStyle\  \[Rule] {Green, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S1[x] = \>", S1[x]\ ];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", f1[x]\ ];\)\)}], "Input"],

Cell[TextData[{
  "Now look at the second Frobenius series corresponds to  ",
  Cell[BoxData[
      \(r\_2 = \(-1\)\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\), "\[IndentingNewLine]", 
    \(\(r\  = \(-1\)\ ;\)\), "\n", 
    \(\(c\_0 = 1\ ;\)\), "\n", 
    \(\(c\_1 = 0\ ;\)\), "\n", 
    \(\(c\_n_ := 
        c\_\(n - 2\)\/\(2\ \((n + r)\)\^2 + n + \ r - 1\);\)\), "\
\[IndentingNewLine]", 
    \(\(f2[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%15\( c\_k\) 
                  x\^k)\)\ ];\)\), "\n", 
    \(\(For[i = 0, i \[LessEqual] 10, \(i++\), 
        Print["\<c\>"\_i, "\< = \>", c\_i]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(c\_n\) = \>\"", 
        Simplify[\*"\"\<\!\(c\_\(n - 2\)\)\>\""\/\(2\ \((n + r)\)\^2 + n + \ \
r - 1\)]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f2[x] = \>", f2[x]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(S2[x_] = Take[Normal[s2[x]], 3];\)\), "\n", 
    \(\(Plot[{S2[x], f2[x]}, {x, 0, 5}, PlotRange \[Rule] {{0, 5}, {0, 10}}, 
        PlotStyle\  \[Rule] {Cyan, Red}];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<S2[x] = \>", S2[x]\ ];\)\), "\n", 
    \(\(Print["\<f2[x] = \>", f2[x]\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Note.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  " We cannot add up an infinite number of terms in this sequence because we \
do not have a closed formula for the coefficients c[k], it is a recursive \
formula and will exceed the finite recursion depth of ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  ". "
}], "Text"],

Cell[TextData[{
  StyleBox["Remark.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  " We would like to have a explicit formula for  ",
  Cell[BoxData[
      \(c\_n\)]],
  "  instead of the recursive formula for  ",
  Cell[BoxData[
      \(c\_n\)]],
  ". \nSuch formulas need to be discovered.  How can we do in.  For this \
example, we can resort to \"picking\" ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s mind. \nThat is to say, we can look at the way ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " solves it and reverse engineer the solution. "
}], "Text"],

Cell[BoxData[
    \(Series[\(Gamma[1\/4]\/\(2\ 2\^\(\(1/8\)\(\ \)\)\)\) 
        BesselI[\(-\(3\/4\)\), x\/\@2]\/x\^\(1/4\), {x, 0, 7}]\)], "Input"],

Cell[TextData[{
  "Since ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " thinks that the Gamma function is used, we will discover \"how to do \
it.\" "
}], "Text"],

Cell[BoxData[{
    \(\(r\  = \ 1\/2\ ;\)\), "\[IndentingNewLine]", 
    \(\(c\_0 = 1\ ;\)\), "\n", 
    \(\(c\_1 = 0\ ;\)\), "\[IndentingNewLine]", 
    \(\(c\_n_ := 
        c\_\(n - 2\)\/\(2\ \((n + r)\)\^2 + n + \ r - 1\);\)\), "\
\[IndentingNewLine]", 
    \(\*"\"\<\!\(c\_n\)\>\"" == 
      ExpandDenominator[\*"\"\<\!\(c\_\(n - 2\)\)\>\""\/\(2\ \((n + \
1\/2)\)\^2 + n + 1\/2 - 1\)]\), "\[IndentingNewLine]", 
    \(\*"\"\<\!\(c\_n\)\>\"" == \*"\"\<\!\(c\_\(n - 2\)\)\>\""\/Factor[3\ n + \
2\ n\^2]\)}], "Input"],

Cell[BoxData[
    \(\(Print[\*"\"\<\!\(c\_2\) = \>\"", 
        c\_2, "\< = \>", \(\(\(1\)\(\ \)\)\/\((2)\)\) \(\(1\)\(\ \)\)\/\((4 + \
3)\), "\< = \>", \(\(\(1\)\(\ \)\)\/\((2)\)\) \(\(\(1\)\(\ \)\)\/4\) \
\(\(1\)\(\ \)\)\/\((1 + 3\/4)\), "\< = \>", 
        FullSimplify[
          Gamma[7\/4]\/\(\(\((2*1)\)!!\) \(4\^1\) Gamma[\(4*1 + \
7\)\/4]\)]];\)\)], "Input"],

Cell[BoxData[
    \(\(Print[\*"\"\<\!\(c\_4\) = \>\"", 
        c\_4, "\< = \>", \(\(\(1\)\(\ \)\)\/\(\((2)\) \((4)\)\)\) \(\(1\)\(\ \
\)\)\/\(\((4 + 3)\) \((8 + 3)\)\), "\< = \>", \(\(\(1\)\(\ \)\)\/\(\((2)\) \
\((4)\)\)\) \(1\/4\^2\) \(\(1\)\(\ \)\)\/\(\((1 + 3\/4)\) \((2 + 3\/4)\)\), "\
\< = \>", 
        FullSimplify[
          Gamma[7\/4]\/\(\(\((2*2)\)!!\) \(\(4\^2\) Gamma[\(4*2 + \
7\)\/4]\)\)]];\)\)], "Input"],

Cell[BoxData[
    \(\(Print[\*"\"\<\!\(c\_6\) = \>\"", 
        c\_6, "\< = \>", \(\(\(1\)\(\ \)\)\/\(\((2)\) \((4)\) \((6)\)\)\) \
\(\(1\)\(\ \)\)\/\(\((4 + 3)\) \((8 + 3)\) \((12 + 3)\)\), "\< = \>", 
        FullSimplify[
          Gamma[7\/4]\/\(\(\((2*3)\)!!\) \(\(4\^3\) Gamma[\(4*3 + \
7\)\/4]\)\)]];\)\)], "Input"],

Cell[BoxData[
    \(\(Print[\*"\"\<\!\(c\_8\) = \>\"", 
        c\_8, "\< = \>", \(\(\(1\)\(\ \)\)\/\(\((2)\) \((4)\) \((6)\) \((8)\)\
\)\) \(\(1\)\(\ \)\)\/\(\((4 + 3)\) \((8 + 3)\) \((12 + 3)\) \((16 + 3)\)\), \
"\< = \>", 
        FullSimplify[
          Gamma[7\/4]\/\(\(\((2*4)\)!!\) \(\(4\^4\) Gamma[\(4*4 + \
7\)\/4]\)\)]];\)\)], "Input"],

Cell[TextData[{
  "We now see the pattern and define the coefficient  ",
  Cell[BoxData[
      \(a\_n = c\_\(2  n\)\)]],
  " \n\n\t",
  Cell[BoxData[
      \(a\_n\  = \ \(c\_\(2  n\)\  = \ 
          Gamma[7\/4]\/\(\(\((2  n)\)!!\) \(4\^n\) Gamma[\(4  n + 7\)\/4]\)\)\
\)]]
}], "Text"],

Cell[BoxData[{
    \(\(Clear[n];\)\), "\[IndentingNewLine]", 
    \(a\_n_\  := 
      FullSimplify[
        Gamma[7\/4]\/\(\(\((2  n)\)!!\) \(4\^n\) Gamma[\(4  n + \
7\)\/4]\)]\)}], "Input"],

Cell[TextData[{
  "Compare the coefficients  ",
  Cell[BoxData[
      \(a\_n\)]],
  "  with  ",
  Cell[BoxData[
      \(c\_\(2  n\)\)]],
  ".  "
}], "Text"],

Cell[BoxData[
    \(FullSimplify[Table[a\_n, {n, 0, 5}]]\)], "Input"],

Cell[BoxData[
    \(FullSimplify[Table[c\_\(2  n\), {n, 0, 5}]]\)], "Input"],

Cell["\<\
Now sum the series to get the first few terms in the solution. \
\>", "Text"],

Cell[BoxData[{
    \(\(g1[x_] = 
        2\ 2\^\(1/8\)\ Gamma[7\/4] 
          BesselI[3\/4, x\/\@2]\/x\^\(1/4\);\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\), "\n", 
    \(\(g1[x_] = FullSimplify[g1[x]\ ];\)\), "\n", 
    \(\(Print["\<g1[x] = \>", g1[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g1[x] = \>", Series[g1[x], {x, 0, 11}]\ ];\)\)}], "Input"],

Cell[BoxData[
    \(\(Print["\<g1[x] = \>", \[Sum]\+\(n = 0\)\%5 a\_n\ x\^\(\((4*n + 1)\)/2\
\), "\< + ...\>"\ ];\)\)], "Input"],

Cell[TextData[{
  "Can ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " find the sum of the series ? "
}], "Text"],

Cell[BoxData[
    \(\[Sum]\+\(n = 0\)\%\[Infinity] Gamma[7\/4]\/\(\(\((2  n)\)!!\) \(4\^n\) \
Gamma[\(4  n + 7\)\/4]\)\ x\^\(\((4  n + 1)\)/2\)\)], "Input"],

Cell[TextData[{
  "Although ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " cannot sum the series ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 0\)\%\[Infinity] Gamma[7\/4]\/\(\(\((2  n)\)!!\) \(4\^n\
\) Gamma[\(4  n + 7\)\/4]\)\ x\^\(\((4  n + 1)\)/2\)\)]],
  ", \nwe have at least found the formula for the coefficients. "
}], "Text"],

Cell[BoxData[
    \(Series[Hypergeometric0F1[1\/4, x\^2\/8]\/x, {x, 0, 11}]\)], "Input"],

Cell["\<\
Similarly, we can find a formula for the coefficients of the second solution. \
\
\>", "Text"],

Cell[BoxData[{
    \(\(g2[x_] = \(Gamma[1\/4]\/\(2\ 2\^\(\(1/8\)\(\ \)\)\)\) 
          BesselI[\(-\(3\/4\)\), x\/\@2]\/x\^\(1/4\);\)\), "\n", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\), "\n", 
    \(\(g2[x_] = FullSimplify[g2[x]\ ];\)\), "\n", 
    \(\(Print["\<g2[x] = \>", g2[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g2[x] = \>", 
        Series[\(Gamma[1\/4]\/\(2\ 2\^\(\(1/8\)\(\ \)\)\)\) 
            BesselI[\(-\(3\/4\)\), x\/\@2]\/x\^\(1/4\), {x, 0, 
            11}]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g2[x] = \>", Series[g2[x], {x, 0, 11}]\ ];\)\)}], "Input"],

Cell[BoxData[{
    \(\(Clear[n];\)\), "\[IndentingNewLine]", 
    \(b\_n_\  := 
      FullSimplify[
        Gamma[1\/4]\/\(\(\((2  n)\)!!\) \(4\^n\) Gamma[\(4  n + \
1\)\/4]\)]\)}], "Input"],

Cell[BoxData[
    \(FullSimplify[Table[b\_n, {n, 0, 5}]]\)], "Input"],

Cell[BoxData[{
    \(\(Print["\<g2[x] = \>", 
        Series[g2[x], {x, 0, 11}]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<g2[x] = \>", \[Sum]\+\(n = 0\)\%6 b\_n\ x\^\((2  n - 1)\), "\
\< + ...\>"\ ];\)\)}], "Input"],

Cell[TextData[{
  "Can ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " find the sum of the series ? "
}], "Text"],

Cell[BoxData[
    \(\[Sum]\+\(n = 0\)\%\[Infinity] Gamma[1\/4]\/\(\(\((2  n)\)!!\) \(4\^n\) \
Gamma[\(4  n + 1\)\/4]\)\ x\^\((2  n - 1)\)\)], "Input"],

Cell[TextData[{
  "Although ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " cannot sum the series  ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 0\)\%\[Infinity] Gamma[1\/4]\/\(\(\((2  n)\)!!\) \(4\^n\
\) Gamma[\(4  n + 1\)\/4]\)\ x\^\((2  n - 1)\)\)]],
  ", \nwe have at least found the formula for the coefficients. "
}], "Text"]
}, Closed]],

Cell[TextData[{
  StyleBox["Example 4.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Use Frobenius series to solve the D. E.   \n\n\t\t",
  Cell[BoxData[
      \(x\^2\ \(\(y'\)'\)[x] + x\ \ \(y'\)[x] + \ x\^2\ y[x]\  = \ 0\)]],
  ".  \n\t\t\nA solution is known to be the celebrated ",
  ButtonBox["Bessel function",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/BesselFunctionoftheFirstKind.html"], 
      None},
    ButtonStyle->"Hyperlink"],
  "  ",
  Cell[BoxData[
      \(\(J\_0\) \((x)\)\)]],
  ". "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 4.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "Determine the nature of the singularity at ",
  Cell[BoxData[
      \(x\_0 = 0\)]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(X\_0 = 0;\)\ \), "\[IndentingNewLine]", 
    \(\(P[x_] = x\^2;\)\), "\[IndentingNewLine]", 
    \(\(Q[x_] = \ x;\)\), "\[IndentingNewLine]", 
    \(\(R[x_] = x\^2;\)\), "\[IndentingNewLine]", 
    \(\(f\_1[x_] = Q[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(f\_2[x_] = R[x]\/P[x];\)\), "\[IndentingNewLine]", 
    \(\(de = 
        P[x]\ \(y''\)[x] + \ \ Q[x]\ \(y'\)[x] + R[x] y[x]\  \[Equal] \ 
          0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(If[
        P[X\_0] \[Equal] 0, \[IndentingNewLine]Print["\<P[x] = \>", 
          P[x]]; \ \[IndentingNewLine]Print["\<P[\>", X\_0, "\<] = \>", 
          P[X\_0]]; \[IndentingNewLine]Print[\*"\"\<The point \!\(x\_0\) = \>\
\"", X\_0, "\< is a singular point.\>"]; \[IndentingNewLine]Print["\<Proceed \
with a Frobenius solution.\>"];, \[IndentingNewLine]Print[\*"\"\<The point \!\
\(x\_0\) = \>\"", 
          X\_0, "\< is NOT singular point.\>"]; \
\[IndentingNewLine]Print["\<Use the power series method for an ordinary \
point.\>"]; \[IndentingNewLine]Print[\*"\"\<y''[x] + \!\(f\_1\)[x]y'[x] + \
\!\(f\_2\)[x]y[x] = 0\>\""]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_1\)[x]=\!\
\(Q[x]\/P[x]\) = \>\"", Together[Q[x]\/P[x]], "\< = \>", 
          Series[f\_1[x], {x, X\_0, 
              3}]]; \[IndentingNewLine]Print[\*"\"\<\!\(f\_2\)[x]=\!\(R[x]\/P[\
x]\) = \>\"", Together[R[x]\/P[x]], "\< = \>", 
          Series[f\_2[x], {x, X\_0, 3}]]; \[IndentingNewLine]If[
          X\_0 \[Equal] 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\!\(x\^n\)\>\"", "\<, etc.\>"]]; \ \
\[IndentingNewLine]If[X\_0 > 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x-\>", 
            X\_0, \*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]]; \
\[IndentingNewLine]If[X\_0 < 0, 
          Print[\*"\"\<y[x] = \!\(\[Sum]\+\(n = \
0\)\%\[Infinity]\)\!\(a\_n\)\>\"", "\<(x+\>", \(-X\_0\), \
\*"\"\<\!\(\()\^n\)\)\>\"", "\<, etc.\>"]];\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Construct the Indicial Equation.",
    FontWeight->"Bold",
    FontColor->RGBColor[0.996109, 0, 0.996109]],
  StyleBox[" ",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]]
}], "Text"],

Cell[BoxData[{
    \(\(\[Phi]\_1[x_] = \ \ x  Q[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(\[Phi]\_2[x_] = \ \ \(x\^2\) 
          R[x]\/P[x];\)\ \), "\[IndentingNewLine]", 
    \(\(p\_0\  = \ 
        Limit[x  Q[x]\/P[x], x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(q\_0\  = \ 
        Limit[\(x\^2\) R[x]\/P[x], 
          x \[Rule] X\_0];\)\ \), "\[IndentingNewLine]", 
    \(\(Clear[r];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = 
        r \((r - 1)\) + \(p\_0\) r\  + \ q\_0\  \[Equal] \ 
          0;\)\), "\[IndentingNewLine]", 
    \(\(Print["\<P[x] = \>", P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<Q[x] = \>", Q[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<R[x] = \>", R[x], "\<\n\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_1\)[x_] =   x\!\(Q[x]\/P[x]\) = \>\"", 
        x  Q[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(p\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< x\!\(Q[x]\/P[x]\) = \>\"", p\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Phi]\_2\)[x_] =  \!\(x\^2\)\!\(R[x]\/P[x]\) = \
\>\"", \(x\^2\) R[x]\/P[x]];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(q\_0\) = \>\"", "\<lim\>"\+\(x \[Rule] X\_0\), \ \*"\
\"\< \!\(x\^2\)\!\(R[x]\/P[x]\) = \>\"", q\_0];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<The indicial equation is\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<r(r-1) + \!\(p\_0\) r + \!\(q\_0\) \[Equal] \
0\>\""];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(ieqn = MapAll[Expand, ieqn];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \)}], "Input"],

Cell[TextData[StyleBox["Find the Roots of the Indicial Equation. ",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Print["\<The indicial equation is\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[ieqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<The roots are\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(solset = Solve[ieqn, r];\)\ \), "\[IndentingNewLine]", 
    \(\(r\_1\  = \ 
        solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(r\_2\  = 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\);\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_1\) = \>\"", 
        r\_1];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(r\_2\) = \>\"", r\_2];\)\ \)}], "Input"],

Cell[TextData[{
  "Form the first Frobenius solution corresponding to the root  ",
  Cell[BoxData[
      \(r\_1 = 0\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, k, n, r, s, x];\)\), "\n", 
    \(c = 0; \ Remove[c];\ \), "\[IndentingNewLine]", 
    \(\(r = 0;\)\), "\n", 
    \(\(n = 10;\)\), "\n", 
    \(\(s[x_] = 
        Expand[\(x\^r\) \((\[Sum]\+\(k = 0\)\%n c\_k\ x\^k)\) + 
            O[x]\^\(n + 1\)];\)\), "\n", 
    \(\(coeff = Table[c\_k, {k, 1, n}];\)\), "\n", 
    \(\(Print["\<  s[x] = \>", s[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\< s'[x] = \>", \(s'\)[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s''[x] = \>", \(s''\)[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Substitute into\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(x\^2\) s''[x] + x s'[x] + \!\(x\^2\) s[x] \[Equal] 0\
\>\""];\)\), "\n", 
    \(\(Print["\<Get\>"];\)\), "\[IndentingNewLine]", 
    \(\(deqn = 
        Expand[x\^2\ \(\(s'\)'\)[x] + \ \ x\ \ \(s'\)[x] + 
              x\^2\ s[x]] \[Equal] 0;\)\ \), "\[IndentingNewLine]", 
    \(\(Print[deqn];\)\)}], "Input"],

Cell["Form the set of equations to solve and do it. ", "Text"],

Cell[BoxData[{
    \(\(eqns = LogicalExpand[deqn];\)\), "\n", 
    \(\(Print[deqn];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[TableForm[ReplaceAll[eqns, And \[Rule] List]]\ ];\)\ \), "\n", 
    \(\(solset = Solve[eqns, coeff];\)\), "\n", 
    \(\(Print[
        TableForm[
          Sort[solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\
\)\)]]\ ];\)\ \), "\n", 
    \(\(s[x_] = 
        ReplaceAll[s[x], 
          solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)];\
\)\), "\n", 
    \(\(Print["\<y = \>", s[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", Normal[s[x]], "\< + ...\>"\ ];\)\)}], "Input"],

Cell["The first Frobenius solution is: ", "Text"],

Cell[BoxData[{
    \(\(s1[x_] = ReplaceAll[s[x], c\_0 \[Rule] 1];\)\), "\n", 
    \(\(Print["\<f1[x] = \>", s1[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<f1[x] = \>", 
        Normal[s1[x]], "\< + ...\>"\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \[InvisibleSpace]\>", \((\[InvisibleSpace]x\^r)\), \
"\<(\>", Normal[\(x\^\(-r\)\) s1[x]], "\< + ...)\>"];\)\)}], "Input"],

Cell[TextData[{
  "This first Frobenius series solution is a ",
  ButtonBox["Bessel Function of the First Kind",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/BesselFunctionoftheFirstKind.html"], 
      None},
