Cell[TextData[{
  StyleBox["Theorem 23.  (Linear Approximation).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}]}]]],
  "  is a continuous function with continuous partial derivatives at  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{\(z\_0\), "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]}]]],
  ".  The linear approximation to  ",
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "y"}], ")"}]}]]],
  ",  at the point  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  is  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}], " ", 
        "\[TildeTilde]", " ", 
        RowBox[{
          RowBox[{"f", 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], " ", 
          "+", " ", 
          RowBox[{\(f\_x\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \((x - x\_0)\), " "}]}]}]]],
  Cell[BoxData[
      RowBox[{\(+\ \ f\_y\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], 
          ")"}], \((y - y\_0)\)}]]],
  ".  \n\nIf we use the increments  ",
  Cell[BoxData[
      \(\[CapitalDelta]x = x - x\_0\)]],
  ",  and  ",
  Cell[BoxData[
      \(\[CapitalDelta]y = y - y\_0\)]],
  ",  then this can be written as  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}], " ", 
        "\[TildeTilde]", " ", 
        RowBox[{
          RowBox[{"f", 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], " ", 
          "+", " ", 
          RowBox[{\(f\_x\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], 
            "\[CapitalDelta]x", " "}]}]}]]],
  Cell[BoxData[
      RowBox[{\(+\ \ f\_y\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], 
        "\[CapitalDelta]y", " "}]]],
  ".  \n\t\nIf we include the increment  ",
  Cell[BoxData[
      \(\[CapitalDelta]z = z - z\_0\)]],
  "  where  ",
  Cell[BoxData[
      RowBox[{\(z\_0\), "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]}]]],
  " and  ",
  Cell[BoxData[
      RowBox[{\(z\_0\), "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0 + \[CapitalDelta]x\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0 + \[CapitalDelta]y\)}],
             ")"}]}]}]]],
  "  then the this can be written as  \n\n\t",
  Cell[BoxData[
      RowBox[{"\[CapitalDelta]z", " ", "\[TildeTilde]", " ", 
        RowBox[{
          RowBox[{\(f\_x\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], 
            "\[CapitalDelta]x"}], " ", "+", " ", 
          RowBox[{\(f\_y\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], 
            "\[CapitalDelta]y"}]}]}]]],
  ".  "
}], "Text",
  CellTags->"Linear Approximation"],

Cell[TextData[{
  StyleBox["Theorem 24 (a).  (First Chain Rule for Several Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}]}]]],
  ",  ",
  Cell[BoxData[
      \(x = g \((t)\)\)]],
  ", and ",
  Cell[BoxData[
      \(y = h \((t)\)\)]],
  "  are all differentiable functions.  \nThen   ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(g \((t)\)\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(h \((t)\)\)}], ")"}]}]}]]],
  "  and the derivative of   ",
  Cell[BoxData[
      \(z\)]],
  "  with respect to  ",
  Cell[BoxData[
      \(t\)]],
  "  is \n\n\t",
  Cell[BoxData[
      \(dz\/dt = \(\[PartialD]z\/\[PartialD]x\) 
            dx\/dt + \(\[PartialD]z\/\[PartialD]y\) dy\/dt\)]],
  "."
}], "Text",
  CellTags->"First Chain Rule for Several Variables"],

Cell[BoxData[{
    \(\(z = f[x, y];\)\ \ \), "\n", 
    \(\(x = g[t];\)\ \ \), "\n", 
    \(\(y = h[t];\)\ \ \), "\n", 
    \(\(Print["\<  z = \>", z\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(dz\/dt\) =  \>\"", \[PartialD]\_t\ 
          z\ \ ];\)\ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["Theorem 24 (b).  (Second Chain Rule for Several Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}]}]]],
  ",  ",
  Cell[BoxData[
      RowBox[{"x", "=", 
        RowBox[{"g", 
          RowBox[{"(", 
            RowBox[{"s", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "t"}], ")"}]}]}]]],
  ", and  ",
  Cell[BoxData[
      RowBox[{"y", "=", 
        RowBox[{"h", 
          RowBox[{"(", 
            RowBox[{"s", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "t"}], ")"}]}]}]]],
  "  are all differentiable functions.  \nThen   ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{
              RowBox[{"g", 
                RowBox[{"(", 
                  RowBox[{"s", 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], "t"}], ")"}]}], ",", 
              RowBox[{"h", 
                RowBox[{"(", 
                  RowBox[{"s", 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], "t"}], ")"}]}]}], 
            ")"}]}]}]]],
  "  and the partial derivatives of  ",
  Cell[BoxData[
      \(z\)]],
  "  with respect to  ",
  Cell[BoxData[
      \(s\)]],
  "  and  ",
  Cell[BoxData[
      \(t\)]],
  "  are   \n\n\t",
  Cell[BoxData[
      \(\[PartialD]z\/\[PartialD]s = \(\[PartialD]z\/\[PartialD]x\) \
\[PartialD]x\/\[PartialD]s + \(\[PartialD]z\/\[PartialD]y\) \[PartialD]y\/\
\[PartialD]s\)]],
  ",  \nand\n\t",
  Cell[BoxData[
      \(\[PartialD]z\/\[PartialD]t = \(\[PartialD]z\/\[PartialD]x\) \
\[PartialD]x\/\[PartialD]t + \(\[PartialD]z\/\[PartialD]y\) \[PartialD]y\/\
\[PartialD]t\)]],
  ".  "
}], "Text",
  CellTags->"Second Chain Rule for Several Variables"],

Cell[BoxData[{
    \(\(Clear[x, y, z, f, g, h];\)\ \), "\[IndentingNewLine]", 
    \(\(z = f[x, y];\)\ \ \), "\n", 
    \(\(x = g[s, t];\)\ \ \), "\n", 
    \(\(y = h[s, t];\)\ \ \), "\n", 
    \(\(Print["\<  z = \>", z\ ];\)\ \ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(\[PartialD]z\/\[PartialD]s\) =  \>\"", \
\[PartialD]\_s\ z\ \ ];\)\ \ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", \*"\"\<\!\(\[PartialD]z\/\[PartialD]t\) =  \>\"", \
\[PartialD]\_t\ z\ \ ];\)\ \ \ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["Theorem 25.  (Taylor's Series for Two Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}]}]]],
  "  is a continuous function with continuous partial derivatives at  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{\(z\_0\), "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]}]]],
  ".  The Taylor polynomial approximation of degree  ",
  Cell[BoxData[
      \(n = 2\)]],
  "  for  ",
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "y"}], ")"}]}]]],
  ",  at the point  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  is  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}], " ", 
        "\[TildeTilde]", " ", 
        RowBox[{
          RowBox[{"f", 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], " ", 
          "+", " ", 
          RowBox[{\(f\_x\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \((x - x\_0)\), " "}]}]}]]],
  Cell[BoxData[
      RowBox[{\(+\ \ f\_y\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], 
          ")"}], \((y - y\_0)\)}]]],
  "\n\t\t\t",
  Cell[BoxData[
      RowBox[{\(+\ \(1\/2\)\), \(f\_xx\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], 
          ")"}], \(\((x - x\_0)\)\^2\)}]]],
  " ",
  Cell[BoxData[
      RowBox[{\(+\ f\_xy\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], 
          ")"}], \((x - x\_0)\), \((y - y\_0)\)}]]],
  " ",
  Cell[BoxData[
      RowBox[{\(+\ \(1\/2\)\), " ", \(f\_yy\), 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], 
          ")"}], \(\((y - y\_0)\)\^2\)}]]]
}], "Text",
  CellTags->"Taylor's Series for Two Variables"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Derivation.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[f, t, x, y];\)\ \ \), "\[IndentingNewLine]", 
    \(\(n = 2;\)\ \ \), "\[IndentingNewLine]", 
    \(\(s = 
        Series[f[X\_0 + \[CapitalDelta]x\ \ t, 
            Y\_0 + \[CapitalDelta]y\ t], {t, 0, 
            n}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[Normal[s], t \[Rule] 1];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[
          S, {X \[Rule] x, Y \[Rule] y}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Taylor polynomial approximation of degree  n = \>", 
        n\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x,y] \[TildeTilde] \>", 
        S];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[
          S, {\[CapitalDelta]x\  \[Rule] x - x\_0, \[CapitalDelta]y\  \[Rule] 
              y - y\_0}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x,y] \[TildeTilde] \>", S];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(Clear[f, t, x, y];\)\ \ \), "\[IndentingNewLine]", 
    \(\(n = 3;\)\ \ \), "\[IndentingNewLine]", 
    \(\(s = 
        Series[f[X\_0 + \[CapitalDelta]x\ \ t, 
            Y\_0 + \[CapitalDelta]y\ t], {t, 0, 
            n}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[Normal[s], t \[Rule] 1];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[
          S, {X \[Rule] x, Y \[Rule] y}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Taylor polynomial approximation of degree  n = \>", 
        n\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x,y] \[TildeTilde] \>", 
        S];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S = 
        ReplaceAll[
          S, {\[CapitalDelta]x\  \[Rule] x - x\_0, \[CapitalDelta]y\  \[Rule] 
              y - y\_0}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\n\>", "\<f[x,y] \[TildeTilde] \>", S];\)\ \ \)}], "Input"]
