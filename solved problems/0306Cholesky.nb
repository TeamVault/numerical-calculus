Cell[TextData[StyleBox["Mathematica Subroutine (Doolittle).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(Doolittle[
        n_] := \[IndentingNewLine]Module[{i, j, k, 
          m}, \[IndentingNewLine]L\  = \ 
          Table[0, {n}, {n}]; \[IndentingNewLine]U\  = \ 
          L; \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] 
            n, \(k++\), \[IndentingNewLine]L\_\(\(\[LeftDoubleBracket]\)\(k, \
k\)\(\[RightDoubleBracket]\)\) = \ 1; \[IndentingNewLine]For[j = k, 
            j \[LessEqual] 
              n, \(j++\), \
\[IndentingNewLine]U\_\(\(\[LeftDoubleBracket]\)\(k, j\)\(\
\[RightDoubleBracket]\)\) = 
              A\_\(\(\[LeftDoubleBracket]\)\(k, \
j\)\(\[RightDoubleBracket]\)\) - \[Sum]\+\(m = 1\)\%\(k - 1\)\(L\_\(\(\
\[LeftDoubleBracket]\)\(k, m\)\(\[RightDoubleBracket]\)\)\) 
                    U\_\(\(\[LeftDoubleBracket]\)\(m, j\)\(\
\[RightDoubleBracket]\)\)\ ]; \[IndentingNewLine]For[i = k + 1, 
            i \[LessEqual] 
              n, \(i++\), \
\[IndentingNewLine]L\_\(\(\[LeftDoubleBracket]\)\(i, k\)\(\
\[RightDoubleBracket]\)\) = \((A\_\(\(\[LeftDoubleBracket]\)\(i, k\)\(\
\[RightDoubleBracket]\)\) - \[Sum]\+\(m = 1\)\%\(k - 1\)\(L\_\(\(\
\[LeftDoubleBracket]\)\(i, m\)\(\[RightDoubleBracket]\)\)\) 
                        U\_\(\(\[LeftDoubleBracket]\)\(m, k\)\(\
\[RightDoubleBracket]\)\))\)/
                U\_\(\(\[LeftDoubleBracket]\)\(k, \
k\)\(\[RightDoubleBracket]\)\)]]]\)], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Crout).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(Crout[
        n_] := \[IndentingNewLine]Module[{i, j, k, 
          m}, \[IndentingNewLine]L\  = \ 
          Table[0, {n}, {n}]; \[IndentingNewLine]U\  = \ 
          L; \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] 
            n, \(k++\), \[IndentingNewLine]L\_\(\(\[LeftDoubleBracket]\)\(k, \
k\)\(\[RightDoubleBracket]\)\) = \ 
            A\_\(\(\[LeftDoubleBracket]\)\(k, k\)\(\[RightDoubleBracket]\)\) \
- \[Sum]\+\(m = 1\)\%\(k - 1\)\(L\_\(\(\[LeftDoubleBracket]\)\(k, 
                      m\)\(\[RightDoubleBracket]\)\)\) 
                  U\_\(\(\[LeftDoubleBracket]\)\(m, \
k\)\(\[RightDoubleBracket]\)\); \[IndentingNewLine]For[j = k, 
            j \[LessEqual] 
              n, \(j++\), \
\[IndentingNewLine]U\_\(\(\[LeftDoubleBracket]\)\(k, j\)\(\
\[RightDoubleBracket]\)\) = \((A\_\(\(\[LeftDoubleBracket]\)\(k, j\)\(\
\[RightDoubleBracket]\)\) - \[Sum]\+\(m = 1\)\%\(k - 1\)\(L\_\(\(\
\[LeftDoubleBracket]\)\(k, m\)\(\[RightDoubleBracket]\)\)\) 
                        U\_\(\(\[LeftDoubleBracket]\)\(m, j\)\(\
\[RightDoubleBracket]\)\)\ )\)/
                L\_\(\(\[LeftDoubleBracket]\)\(k, \
k\)\(\[RightDoubleBracket]\)\)]; \[IndentingNewLine]For[i = k + 1, 
            i \[LessEqual] 
              n, \(i++\), \
\[IndentingNewLine]L\_\(\(\[LeftDoubleBracket]\)\(i, k\)\(\
\[RightDoubleBracket]\)\) = \((A\_\(\(\[LeftDoubleBracket]\)\(i, k\)\(\
\[RightDoubleBracket]\)\) - \[Sum]\+\(m = 1\)\%\(k - 1\)\(L\_\(\(\
\[LeftDoubleBracket]\)\(i, m\)\(\[RightDoubleBracket]\)\)\) 
                        U\_\(\(\[LeftDoubleBracket]\)\(m, k\)\(\
\[RightDoubleBracket]\)\))\)/
                U\_\(\(\[LeftDoubleBracket]\)\(k, \
k\)\(\[RightDoubleBracket]\)\)]]]\)], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Forward Elimination).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(ForeSub[
        n_] := \[IndentingNewLine]Module[{i, j}, \[IndentingNewLine]Y = 
          Table[0, {n}]; \
\[IndentingNewLine]Y\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\
\) = B\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\)\/L\_\(\(\
\[LeftDoubleBracket]\)\(n, n\)\(\[RightDoubleBracket]\)\); \
\[IndentingNewLine]For[i = 1, \ 
          i \[LessEqual] 
            n, \ \(i++\), \[IndentingNewLine]Y\_\(\(\[LeftDoubleBracket]\)\(i\
\)\(\[RightDoubleBracket]\)\) = \((B\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\) - \[Sum]\+\(j = \ 1\)\%\(i - 1\)\(L\_\(\(\
\[LeftDoubleBracket]\)\(i, j\)\(\[RightDoubleBracket]\)\)\) 
                      Y\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\))\)/
              L\_\(\(\[LeftDoubleBracket]\)\(i, \
i\)\(\[RightDoubleBracket]\)\)]]\)], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Back Substitution).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(BackSub[
        n_] := \[IndentingNewLine]Module[{i, j}, \[IndentingNewLine]X = 
          Table[0, {n}]; \
\[IndentingNewLine]X\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\
\) = Y\_\(\(\[LeftDoubleBracket]\)\(n\)\(\[RightDoubleBracket]\)\)\/U\_\(\(\
\[LeftDoubleBracket]\)\(n, n\)\(\[RightDoubleBracket]\)\); \
\[IndentingNewLine]For[i = n - 1, \ 
          1 \[LessEqual] 
            i, \ \(i--\), \[IndentingNewLine]X\_\(\(\[LeftDoubleBracket]\)\(i\
\)\(\[RightDoubleBracket]\)\) = \((Y\_\(\(\[LeftDoubleBracket]\)\(i\)\(\
\[RightDoubleBracket]\)\) - \[Sum]\+\(j = \ i + 1\)\%n\( 
                          U\_\(\(\[LeftDoubleBracket]\)\(i, 
                          j\)\(\[RightDoubleBracket]\)\)\) 
                      X\_\(\(\[LeftDoubleBracket]\)\(j\)\(\
\[RightDoubleBracket]\)\))\)/
              U\_\(\(\[LeftDoubleBracket]\)\(i, \
i\)\(\[RightDoubleBracket]\)\)]]\)], "Input"],

