    \(\(Print["\<p = min{m,n} = \>", Min[m, n]];\)\ \ \), "\n", 
    \(\(Print["\<\n\>", "\<P[h]+Q[h] = \>", P[h] + Q[h]];\)\ \ \), "\n", 
