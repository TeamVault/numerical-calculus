Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell["\<\
We need to define this function individually in each \
sub-interval.\
\>", "Text"],

Cell[BoxData[{
    \(\(L = \[Pi];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_] = Abs[x - L];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(gr1 = 
        Plot[f[x], {x, 0, 2  L}, PlotStyle \[Rule] Blue, 
          Ticks \[Rule] {Range[0, 2  L, L\/2], 
              Range[0, L, L\/2]}];\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x], "\<     for \>", 
        0, "\< \[LessEqual] x \[LessEqual] \>", 
        2  L, "\<.\>"];\)\ \ \)}], "Input",
  AspectRatioFixed->False],

Cell[BoxData[{
    \(\(n = 5;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Clear[c, d, j, k, t];\)\ \ \), "\n", 
    \(\(c\_j_\  := \ \(1\/L\) \(\[Integral]\_0\%\(2  L\)f[t] 
              Cos[j\ \[Pi]\/L\ t] \[DifferentialD]t\);\)\ \ \), "\n", 
    \(\(d\_j_\  := \ \(1\/L\) \(\[Integral]\_0\%\(2  L\)f[t] 
              Sin[j  \[Pi]\/L\ t] \[DifferentialD]t\);\)\ \ \), "\n", 
    \(\(Print[\*"\"\<   \!\(c\_k\) = \>\"", 
        c\_k\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<   \!\(c\_k\) = \>\"", 
        Simplify[c\_k, 
          k \[Element] Integers]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<   \!\(d\_k\) = \>\"", 
        d\_k];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<   \!\(d\_k\) = \>\"", 
        Simplify[d\_k, 
          k \[Element] Integers]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S[x_]\  = 
        c\_0\/2 + \[Sum]\+\(k = 1\)\%n\((\(c\_k\) 
                  Cos[k  \[Pi]\/L\ x] + \(d\_k\) 
                  Sin[k\ \(\[Pi]\/L\) x])\);\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<S[x] = \>", S[x]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(s[x_]\  = 
        Chop[N[c\_0]\/2 + \[Sum]\+\(k = 1\)\%n\((N[c\_k] 
                    Cos[k  \[Pi]\/L\ x] + 
                  N[d\_k] 
                    Sin[k  \[Pi]\/L\ x])\)];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<s[x] = \>", s[x]\ ];\)\ \ \)}], "Input"],

