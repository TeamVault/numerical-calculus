Cell[TextData[{
  StyleBox["Theorem 2.  (Limits and Continuous Functions).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      StyleBox[\(f \((x)\)\),
        FontSize->12]]],
