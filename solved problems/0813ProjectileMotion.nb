Cell[TextData[{
  StyleBox["Model for the Ascent",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\nWe can use the above solution for the ascent portion of the curve \
only. "
}], "Text"],

Cell[BoxData[{
    \(\(g = 32;\)\ \), "\[IndentingNewLine]", 
    \(\(k = \(1\/2048\) m;\)\ \), "\n", 
    \(\(V\_0 = 300;\)\ \), "\n", 
    \(\(Y\_0 = 0;\)\ \), "\n", 
    \(\(Clear[v, t];\)\ \), "\n", 
    \(\(deup = m\ \(v'\)[t] \[Equal] \(-k\)\ v[t]\^2 - \ m\ g;\)\ \), "\n", 
    \(\(sol1 = DSolve[\ {deup, v[0] \[Equal] V\_0}, v[t], t];\)\ \), "\n", 
