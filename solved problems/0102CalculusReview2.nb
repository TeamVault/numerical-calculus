    \(\(Qtimeset = Timing[\ Q[V]\ ];\)\ \ \), "\n", 
    \(\(Print["\<Time to evaluate P[x]; \>", m, "\< times is \>", 
        Ptimeset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\ \
];\)\ \ \), "\n", 
    \(\(Print["\<Time to evaluate Q[x]; \>", m, "\< times is \>", 
        Qtimeset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\ \
];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 18.  (Geometric Series).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  If   ",
  Cell[BoxData[
      \(\(\(|\)\(r\)\(|\)\(\(<\)\(1\)\)\)\)]],
  ",  then the geometric series  ",
  Cell[BoxData[
      RowBox[{
        StyleBox[
          UnderoverscriptBox["\[Sum]", 
            StyleBox[\(n = 0\),
              FontSize->10], 
            StyleBox["\[Infinity]",
              FontSize->10]],
          FontSize->18], \(r\^n\), " "}]]],
  "  converges, and     \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox[
            UnderoverscriptBox["\[Sum]", 
              StyleBox[\(n = 0\),
                FontSize->10], 
              StyleBox["\[Infinity]",
                FontSize->10]],
            FontSize->18], \(r\^n\)}], " ", "=", " ", 
        StyleBox[
          FractionBox[
            StyleBox["1",
              FontSize->12], 
            StyleBox[\(1 - r\),
              FontSize->12]],
          FontSize->12]}]]],
  "    \ni.e.  \n\t",
  Cell[BoxData[
      RowBox[{\(\(1\  + \ r\  + \ r\^2\  +  ... \) + \ r\^\(n - 1\)\  + \ 
            r\^n\  +  ... \), " ", "=", " ", 
        StyleBox[
          FractionBox[
            StyleBox["1",
              FontSize->12], 
            StyleBox[\(1 - r\),
              FontSize->12]],
          FontSize->12]}]]],
  ".\n\nIf   ",
  Cell[BoxData[
      \(\(\(|\)\(r\)\(|\)\(\(\[GreaterEqual]\)\(1\)\)\)\)]],
  "  then the geometric series diverges.  "
}], "Text",
  CellTags->"Geometric Series"],

Cell[TextData[{
  StyleBox["Example 30.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Sum the infinite series  ",
  Cell[BoxData[
      \(\(1 + 1\/4 + 1\/4\^2 + 1\/4\^3 + 1\/4\^4 + 1\/4\^5 +  ... \) + 
            1\/4\^n +  ... \  = \ \[Sum]\+\(n = 0\)\%\[Infinity] 1\/4\^n\)]],
  "."
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 30.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[n, S, V];\)\ \ \ \), "\n", 
    \(\(V = {};\)\ \ \ \), "\n", 
    \(\(S = 0;\)\ \ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(For[n = 0, 
        n \[LessEqual] 12, \(n++\), \[IndentingNewLine]S = 
          S + N[1\/4\^n]; \ \ \[IndentingNewLine]V = 
          Append[V, {n, S}]];\)\ \ \), "\n", 
    \(\(dots = 
        ListPlot[V, PlotStyle \[Rule] Magenta, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(line = 
        Graphics[{Green, Line[{{0, 4\/3}, {12, 4\/3}}]}];\)\ \ \), "\n", 
    \(\(Show[line, dots, Axes \[Rule] True, 
        PlotRange \[Rule] {{0, 12}, {0, 1.35}}, 
        Prolog \[Rule] {PointSize[0.02]}, 
        Ticks \[Rule] {Range[0, 15, 1], Range[0, 1.5, 0.25]}, 
        AxesLabel \[Rule] {"\<n\>", "\<S\>"}, 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<Partial sums \!\(\[Sum]\+\(k = 0\)\%n\)\!\(1\/4\^k\) for \
the infinite series \!\(\[Sum]\+\(n = 0\)\%\[Infinity]\)\!\(1\/4\^n\).\>\""];\
\)\ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "  can find the sum of the infinite series.  "
}], "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(Clear[n, val];\)\ \ \), "\n", 
    \(\(val\  = \ \[Sum]\+\(n = 0\)\%\[Infinity] 1\/4\^n;\)\ \ \), "\n", 
