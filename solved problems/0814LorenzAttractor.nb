Cell[BoxData[{
    \(\(Clear[t, x, y, z];\)\), "\[IndentingNewLine]", 
    \(\(LorenzEqns[p_, r_, c_, x0_, y0_, z0_] := 
        Module[{}, \[IndentingNewLine]eqns = {\ \(x'\)[t] \[Equal] 
                p \((\ y[t] - 
                      x[t])\), \[IndentingNewLine]\ \ \ \ \ \ \ \ \ \ \ \ \
\(y'\)[t] \[Equal] \ \ r\ x[t] - \ y[t] - \ 
                  x[t] z[t], \[IndentingNewLine]\ \ \ \ \ \ \ \ \ \ \ \ \ \
\(z'\)[t] \[Equal] \ 
                x[t]\ y[t] - c\ z[t]\ }; \[IndentingNewLine]ics = {x[
                  0] \[Equal] \ x0, y[0] \[Equal] \ y0, \ 
              z[0] \[Equal] \ z0}; \[IndentingNewLine]Return[
            Flatten[{eqns, ics}]]];\)\ \)}], "Input"],

Cell[BoxData[{
    \(\(deic = 
        LorenzEqns[10, 28, 8\/3, 10, \(-10\), 
          20];\)\), "\[IndentingNewLine]", 
    \(solDE\  = \ 
      NDSolve[deic, \ \[IndentingNewLine]\ {x, y, z}, \ {t, 0, 20}, 
        MaxSteps \[Rule] 3000]\ \ \)}], "Input"],

Cell[TextData[{
