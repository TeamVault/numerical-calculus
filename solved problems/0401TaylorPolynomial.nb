Cell[TextData[{
  StyleBox["Corollary 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      RowBox[{"f", "\[Element]", 
        RowBox[{
          StyleBox[\(C\^\(n + 1\)\),
            FontWeight->"Bold"], "[", 
          RowBox[{\(x\_0 - R\), 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], \(x\_0 + R\)}], "]"}]}]]],
  ", and that the Taylor polynomial of degree ",
  Cell[BoxData[
      \(n\)]],
  " for ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  " is ",
  Cell[BoxData[
      RowBox[{\(P \((x)\)\), "=", 
        RowBox[{\(\[Sum]\+\(k = 0\)\%n\), 
          RowBox[{
            FractionBox[
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((k)\),
                    Derivative],
                  
                  MultilineFunction->None], \((x\_0)\)}], \(k!\)], \(\((x - 
                    x\_0)\)\^k\)}]}]}]]],
  ", then \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          SuperscriptBox["P", 
            TagBox[\((k)\),
              Derivative],
            MultilineFunction->None], \((x\_0)\)}], " ", "=", " ", 
        RowBox[{
          SuperscriptBox["f", 
            TagBox[\((k)\),
              Derivative],
            MultilineFunction->None], \((x\_0)\)}]}]]],
  "   for  ",
  Cell[BoxData[
      RowBox[{\(k = 0\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "1", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "2", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "...", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "n"}]]],
  "."
}], "Text"],

