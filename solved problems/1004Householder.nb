Cell[TextData[{
  "Use column 1 to compute the  S  and  R.  \n\t\n\t",
  Cell[BoxData[{
      \(\(\(S\  = \ \@\(\[Sum]\+\(j = 2\)\%4\((a\_\(j, 1\))\)\^2\)\)\(\
\[IndentingNewLine]\)
      \)\), "\[IndentingNewLine]", 
      \(S\  = \@\(2\^2 + 2\^2 + 1\^2\)\[IndentingNewLine]\), 
    "\[IndentingNewLine]", 
      \(S = \ \@9\[IndentingNewLine]\), "\[IndentingNewLine]", 
      \(S\  = \ 3\)}]],
  " \n\t\n\t\n\t",
  Cell[BoxData[{
      \(\(\(R\ \  = \(\@\(2\ \((a\_\(2, 1\) + S)\)\ S\)\  = \ \@\(2\ \((2 + \
S)\)\ S\)\)\)\(\[IndentingNewLine]\)
      \)\), "\[IndentingNewLine]", 
      \(\(\(R\  = \ \@\(2\ \((2 + \@\(2\^2 + 2\^2 + 1\^2\))\)\ \@\(2\^2 + \
2\^2 + 1\^2\)\)\)\(\[IndentingNewLine]\)
      \)\), "\[IndentingNewLine]", 
      \(R\  = \ \@\(2\ \((2 + 3)\)\ 3\)\ \ \[IndentingNewLine]\), 
    "\[IndentingNewLine]", 
      \(R\  = \ \ \@30\[IndentingNewLine]\), "\[IndentingNewLine]", 
      \(\(\(R\)\(\ \)\(=\)\(\ \)\(5.477225575\)\(\ \)\)\)}]],
  "    "
}], "Text"],

Cell[TextData[{
  "The following ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
