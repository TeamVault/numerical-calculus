Notebook[{

Cell[CellGroupData[{
Cell[BoxData[{
    StyleBox[\(f[x_] := x^3;\),
      FontSize->16], "\[IndentingNewLine]", 
    StyleBox[
      RowBox[{\(a := 0; \ b := Pi/2; \ n = 2;\), 
        "\[IndentingNewLine]", \( (*\(X := {\(-0.861136\), \(-0.339981\), 
                0.339981, 0.861136};\)*) \), 
        "\[IndentingNewLine]", \( (*\(A := {0.347854, 0.652145, 0.652145, 
                0.347854};\)*) \)}],
      FontSize->16], "\[IndentingNewLine]", 
    StyleBox[\(X := {\(-1\)/\@3, 1/\@3};\),
      FontSize->16], "\[IndentingNewLine]", 
    StyleBox[\(A := {1, 1};\),
      FontSize->16], "\[IndentingNewLine]", 
    StyleBox[\(G := \((b - a)\)/2*
            Sum[A[\([i]\)]*f[\((X[\([i]\)] \((b - a)\) + a + b)\)/2], {i, 1, 
                n}];\),
      FontSize->16], "\[IndentingNewLine]", 
    StyleBox[\(Print["\<G= \>", G, "\< eroarea= \>", 
        Simplify[\[Integral]\_a\%b f[x] \[DifferentialD]x - G]]\),
      FontSize->16]}], "Input"],

Cell[BoxData[
    InterpretationBox[\("G= "\[InvisibleSpace]\(1\/4\ \[Pi]\ \((1\/8\ \
\((\[Pi]\/2 - \[Pi]\/\(2\ \@3\))\)\^3 + 
              1\/8\ \((\[Pi]\/2 + \[Pi]\/\(2\ \@3\))\)\^3)\)\)\
\[InvisibleSpace]" eroarea= "\[InvisibleSpace]0\),
      SequenceForm[ "G= ", 
        Times[ 
          Rational[ 1, 4], Pi, 
          Plus[ 
            Times[ 
              Rational[ 1, 8], 
              Power[ 
                Plus[ 
                  Times[ 
                    Rational[ 1, 2], Pi], 
                  Times[ 
                    Rational[ -1, 2], 
                    Power[ 3, 
                      Rational[ -1, 2]], Pi]], 3]], 
            Times[ 
              Rational[ 1, 8], 
              Power[ 
                Plus[ 
                  Times[ 
                    Rational[ 1, 2], Pi], 
                  Times[ 
                    Rational[ 1, 2], 
                    Power[ 3, 
                      Rational[ -1, 2]], Pi]], 3]]]], " eroarea= ", 0],
      Editable->False]], "Print"]
}, Open  ]]
},
FrontEndVersion->"5.0 for Microsoft Windows",
ScreenRectangle->{{0, 1024}, {0, 685}},
WindowSize->{846, 629},
WindowMargins->{{0, Automatic}, {Automatic, 0}}
]

