Cell[BoxData[{
    \(\(Clear[h];\)\), "\[IndentingNewLine]", 
    \(\(f\_1[{x_, y_}] = 1 - 4\ x + 2\ x\^2 - 2\ y\^3;\)\ \ \), "\n", 
    \(\(f\_2[{x_, y_}] = \(-4\) + x\^4 + 4\ y + 4\ y\^4;\)\ \ \), "\n", 
    \(\(F\&\[RightVector][{x_, y_}] = {f\_1[{x, y}], 
          f\_2[{x, y}]};\)\ \ \), "\n", 
    \(\(J[{x_, y_}] = 
        Transpose[{\[PartialD]\_x\ F\&\[RightVector][{x, y}], \[PartialD]\_y\ 
              F\&\[RightVector][{x, y}]}];\)\ \ \), "\n", 
    \(\(Print["\<Solve\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<0 = \!\(f\_1\)(x,y) = \>\"", 
        f\_1[{x, y}]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<0 = \!\(f\_2\)(x,y) = \>\"", 
        f\_2[{x, y}]];\)\ \ \), "\n", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(F\&\[RightVector]\)[{x,y}] = \>\"", 
        MatrixForm[
          F\&\[RightVector][{x, y}]]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<  J(x,y) = \>", MatrixForm[J[{x, y}]]];\)\ \ \)}], "Input",
  AspectRatioFixed->False],

Cell[TextData[{
  "Second, graph the curves ",
  Cell[BoxData[
      RowBox[{
