Cell[TextData[{
  StyleBox["Theorem 12.  (First Fundamental Theorem of Calculus).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  If  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is continuous over the interval  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  then there exists a function  ",
  Cell[BoxData[
      \(F \((x)\)\)]],
  ",  called the antiderivative of  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  ",  such that  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox[
            SubsuperscriptBox["\[Integral]", 
              StyleBox["a",
                FontSize->10], 
              StyleBox["b",
                FontSize->10]],
            FontSize->18], \(f \((x)\)\ \[DifferentialD]x\)}], " ", "=", 
        " ", \(F \((b)\) - F \((a)\)\)}]]],
  "  where  ",
  Cell[BoxData[
      \(F' \((x)\) = f \((x)\)\)]],
  ".  "
}], "Text",
  CellTags->"First Fundamental Theorem of Calculus"],

Cell[TextData[{
  StyleBox["Example 23.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Let  ",
  Cell[BoxData[
      \(f \((x)\) = tan \((x)\)\)]],
  ".  Show that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox[
            SubsuperscriptBox["\[Integral]", 
              StyleBox[\(\(-\[Pi]\)/3\),
                FontSize->10], 
              StyleBox[\(\[Pi]/4\),
                FontSize->10]],
            FontSize->18], \(f \((x)\)\ \[DifferentialD]x\)}], "=", 
        RowBox[{
          RowBox[{"F", 
            RowBox[{"(", 
              FractionBox[
                StyleBox["\[Pi]",
                  FontSize->10], 
                StyleBox["4",
                  FontSize->10]], ")"}]}], "-", 
          RowBox[{"F", 
            RowBox[{"(", 
              FractionBox[
                StyleBox[\(-\[Pi]\),
                  FontSize->10], 
                StyleBox["3",
                  FontSize->10]], ")"}]}]}]}]]],
  ",   where  ",
  Cell[BoxData[
      \(F' \((x)\) = f \((x)\)\)]],
  ".  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 23.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, f, F, v, x];\)\ \ \), "\n", 
    \(\(f[x_] = Tan[x];\)\ \ \), "\n", 
    \(\(F[x_] = \[Integral]f[x] \[DifferentialD]x;\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<The indefinite integral is:\>"];\)\ \ \), "\n", 
    \(\(Print["\<F[x] = \[Integral] f[x] \[DifferentialD]x\>"];\)\ \ \), "\n\
", 
    \(\(Print["\<F[x] = \[Integral] Tan[x] \[DifferentialD]x = \>", 
        F[x]];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(a = \(-\(\[Pi]\/3\)\);\)\ \ \), "\n", 
    \(\(b = \[Pi]\/4;\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<F[x] = \>", F[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<The interval is  [\>", a, "\<,\>", 
        b, "\<]\>"];\)\ \ \), "\n", 
    \(\(Print["\<The value of the definite integral is:\>"];\)\ \ \), "\n", 
    \(\(Print["\<F[b] - F[a] = \>", F[b], "\< - \>", F[a], "\<  =  \>", 
        N[F[b] - F[a]]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<F[b] - F[a] = \>", F[b] - \ F[a], "\<  =  \>", 
        N[F[b] - F[a]]\ ];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(\(-\[Pi]\)/3\)\%\(\[Pi]/4\)\)f[x]\
\[DifferentialD]x = \>\"", "\<F[\>", b, "\<] - F[\>", 
        a, "\<]\>"\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(\(-\[Pi]\)/3\)\%\(\[Pi]/4\)\)f[x]\
\[DifferentialD]x = \>\"", "\< - Log[Cos[[\>", b, "\<] + Log[Cos[[\>", 
        a, "\<]\>"\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(\(-\[Pi]\)/3\)\%\(\[Pi]/4\)\)f[x]\
\[DifferentialD]x = \>\"", "\< (\>", \(-\ Log[Cos[b]]\), "\<) + (\>", 
        Log[Cos[a]], "\<)\>"\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(\(-\[Pi]\)/3\)\%\(\[Pi]/4\)\)f[x]\
\[DifferentialD]x = \>\"", F[b]\  - \ F[a]\ , "\< = \>", 
        N[F[b]\  - \ F[a]]\ ];\)\ \ \)}], "Input"],

Cell[TextData[{
  "Or use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s integrate command with limits of integration:"
}], "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(a = \(-\(\[Pi]\/3\)\);\)\), "\n", 
    \(\(b = \[Pi]\/4;\)\), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\), "\n", 
    \(\(v = \[Integral]\_a\%b f[x] \[DifferentialD]x;\)\), "\n", 
    \(\(Print["\<The interval is  [\>", a, "\<,\>", b, "\<]\>"];\)\), "\n", 
    \(\(Print["\<The value of the integral is:\>"];\)\), "\n", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(\(-\[Pi]\)/3\)\%\(\[Pi]/4\)\)f[x]\
