Cell[BoxData[{
    \(\(Clear[f, p, t, x, y, z];\)\ \), "\n", 
    \(f[x_, y___, z_] := 
      Module[{}, Return[\(f[y, z] - f[x, y]\)\/\(z - x\)]]\), "\n", 
    \(\(p[t_] = 
        f[x\_0]\  + \ 
          f[x\_0, x\_1] \((t - x\_0)\)\  + \ \[IndentingNewLine]f[x\_0, x\_1, 
              x\_2] \((t - x\_0)\) \((t - x\_1)\)\  + \ \[IndentingNewLine]f[
              x\_0, x\_1, x\_2, 
              x\_3] \((t - x\_0)\) \((t - x\_1)\) \((t - x\_2)\);\)\), "\n", 
    \(\(Print["\<p[x] = \>", p[x]];\)\ \), "\n", 
    \(\(Print[\*"\"\<p[\!\(x\_0\)] = \>\"", 
        p[x\_0]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<p[\!\(x\_1\)] = \>\"", p[x\_1]];\)\), "\n", 
    \(\(Print[\*"\"\<p[\!\(x\_2\)] = \>\"", 
        p[x\_2]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<p[\!\(x\_2\)] = \>\"", 
        Together[p[x\_2]]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<p[\!\(x\_3\)] = \>\"", 
        p[x\_3]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<p[\!\(x\_3\)] = \>\"", 
        Together[p[x\_3]]\ ];\)\ \ \)}], "Input"],

