Cell[TextData[StyleBox["Mathematica Subroutine (Newton-Horner Iteration).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(NewtonHorner[x0_, 
        max_]\)\(:=\)\(\[IndentingNewLine]\)\(Module[{j = 0, k, 
          n = Length[a] - 1, 
          r0\  = \ N[x0]}, \[IndentingNewLine]c = \(b = 
            Table[0, {i, 
                n + 1}]\); \[IndentingNewLine]Print[\*"\"\<\!\(r\_0\) = \
\>\"", PaddedForm[r0, {16, 16}], \*"\"\<,   P[\!\(r\_0\)] = \>\"", 
          NumberForm[P[r0], 16]\ ]; \ \[IndentingNewLine]r1\  = \ 
          r0; \ \[IndentingNewLine]While[\ 
          j < max, \[IndentingNewLine]r0\  = \ 
            r1; \ \[IndentingNewLine]b\_\(\(\[LeftDoubleBracket]\)\(n + 1\)\(\
\[RightDoubleBracket]\)\) = 
            a\_\(\(\[LeftDoubleBracket]\)\(n + \
1\)\(\[RightDoubleBracket]\)\); \[IndentingNewLine]For[\ k = n, \ 
            1 \[LessEqual] 
              k, \ \(k--\), \ \(b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\
\[RightDoubleBracket]\)\) = 
                a\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\
\  + \ r0\ b\_\(\(\[LeftDoubleBracket]\)\(k + 1\)\(\[RightDoubleBracket]\)\);\
\)\ \ ]; \[IndentingNewLine]c\_\(\(\[LeftDoubleBracket]\)\(n + 1\)\(\
\[RightDoubleBracket]\)\) = 
            b\_\(\(\[LeftDoubleBracket]\)\(n + \
1\)\(\[RightDoubleBracket]\)\); \[IndentingNewLine]For[\ k = n, \ 
            2 \[LessEqual] 
              k, \ \(k--\), \ \(c\_\(\(\[LeftDoubleBracket]\)\(k\)\(\
\[RightDoubleBracket]\)\) = 
                b\_\(\(\[LeftDoubleBracket]\)\(k\)\(\[RightDoubleBracket]\)\)\
\  + \ r0\ c\_\(\(\[LeftDoubleBracket]\)\(k + 1\)\(\[RightDoubleBracket]\)\);\
\)\ \ ]; \[IndentingNewLine]r1\  = \ 
            r0\  - \ 
              b\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\/\
c\_\(\(\[LeftDoubleBracket]\)\(2\)\(\[RightDoubleBracket]\)\); \ \
\[IndentingNewLine]j\  = \ 
            j + 1; \ \ \[IndentingNewLine]Print["\<r\>"\_j, "\< = \>", 
            PaddedForm[r1, {16, 16}], "\<,   P[\>", "\<r\>"\_j, "\<] = \>", 
            NumberForm[P[r1], 
              16]\ ];\ ]; \ \[IndentingNewLine]Print["\<  r  = \>", 
          NumberForm[r1, 
            16]\ ]; \ \[IndentingNewLine]Print["\< \[CapitalDelta]r  = \
\[PlusMinus]\>", Abs[r1 - r0]\ ]; \ \[IndentingNewLine]Print["\<P[r] = \>", 
          NumberForm[P[r1], 16]\ ];\ ]\)\(\ \)\)\)], "Input"],

Cell[TextData[StyleBox["Mathematica Subroutine (Newton-Raphson Iteration).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    \(\(\(NewtonRaphson[x0_, 
        max_]\)\(:=\)\(\[IndentingNewLine]\)\(Module[{k = 0, 
          r0\  = \ N[
              x0]}, \[IndentingNewLine]Print[\*"\"\<\!\(r\_0\) = \>\"", 
          PaddedForm[r0, {16, 16}], \*"\"\<,   P[\!\(r\_0\)] = \>\"", 
          NumberForm[P[r0], 16]\ ]; \ \[IndentingNewLine]r1\  = \ 
          r0; \ \[IndentingNewLine]While[\ 
          k < max, \[IndentingNewLine]r0\  = \ 
            r1; \ \[IndentingNewLine]r1\  = \ 
            r0\  - \ P[r0]\/\(P'\)[r0]; \ \[IndentingNewLine]k\  = \ 
            k + 1; \ \ \[IndentingNewLine]Print["\<r\>"\_k, "\< = \>", 
            PaddedForm[r1, {16, 16}], "\<,   f[\>", "\<r\>"\_k, "\<] = \>", 
            NumberForm[P[r1], 
              16]\ ];\ ]; \ \[IndentingNewLine]Print["\<  r  = \>", 
          NumberForm[r1, 
            16]\ ]; \ \[IndentingNewLine]Print["\< \[CapitalDelta]r  = \
\[PlusMinus]\>", Abs[r1 - r0]\ ]; \ \[IndentingNewLine]Print["\<P[r] = \>", 
          NumberForm[P[r1], 16]\ ];\ ]\)\(\ \)\)\)], "Input"],

Cell[TextData[{
  StyleBox["Example 5.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  StyleBox["  Use the Newton-Horner\[CloseCurlyQuote]s subroutine and the \
