Cell[BoxData[
    \(Graeffe[\[Epsilon]_, v0_]\  := \ 
      Module[{i, j, k, t}, \[IndentingNewLine]v = v0; \[IndentingNewLine]c = 
          CoefficientList[f\_0[x], x]; \ \[IndentingNewLine]n = 
          Length[c] - 
            1; \[IndentingNewLine]A = \(T = \(Z = {}\)\); \
\[IndentingNewLine]A = 
          Append[A, c]; \[IndentingNewLine]t = \(z = 
            Table[0, {i, n}]\); \[IndentingNewLine]tol = 
          1; \[IndentingNewLine]T = \(Z = 
            Append[Z, 
              z]\); \[IndentingNewLine]Print["\<f\>"\_0, "\<[x] = \>", 
          f\_0[x]\ ]; \[IndentingNewLine]k = 1; \[IndentingNewLine]While[\ 
          And[tol > \[Epsilon], 
            k \[LessEqual] v], \[IndentingNewLine]f\_k[
              x_]\  = \ \(\((\(-1\))\)\^n\) 
              ReplaceAll[Expand[f\_\(k - 1\)[x] f\_\(k - 1\)[\(-x\)]], 
                x\^j_\  \[Rule] \ 
                  x\^\(j/2\)]; \[IndentingNewLine]Print["\<f\>"\_k, "\<[x] = \
\>", N[f\_k[x]]\ ]; \[IndentingNewLine]c = 
            N[CoefficientList[f\_k[x], x]]; \ \[IndentingNewLine]A = 
            Append[A, c]; \[IndentingNewLine]For[i = 1, 
            i \[LessEqual] 
              n, \(i++\), \
\[IndentingNewLine]\(z\_\(\(\[LeftDoubleBracket]\)\(i\)\(\[RightDoubleBracket]\
\)\) = Abs[\(-\(c\_\(\(\[LeftDoubleBracket]\)\(n + 1 - i\)\(\
\[RightDoubleBracket]\)\)\/c\_\(\(\[LeftDoubleBracket]\)\(n + 2 - i\)\(\
\[RightDoubleBracket]\)\)\)\)];\)\ ]; \[IndentingNewLine]t = \@z\%\(2\^k\); \
\[IndentingNewLine]Z = Append[Z, z]; \[IndentingNewLine]T = 
            Append[T, 
              t]; \[IndentingNewLine]tol = \@\(\((T\_\(\(\[LeftDoubleBracket]\
\)\(-2\)\(\[RightDoubleBracket]\)\) - T\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\
\[RightDoubleBracket]\)\))\) . \((T\_\(\(\[LeftDoubleBracket]\)\(-2\)\(\
\[RightDoubleBracket]\)\) - T\_\(\(\[LeftDoubleBracket]\)\(-1\)\(\
\[RightDoubleBracket]\)\))\)\); \[IndentingNewLine]\ 
          k = k + 1;\ ]; \[IndentingNewLine]v = 
          k - 1; \[IndentingNewLine]Print["\<\n\>", "\<Using Graeffe's method \
for\>"]; \[IndentingNewLine]Print["\<f\>"\_0, "\<[x] = \>", 
          f\_0[x], "\<\n\>"\ ]; \[IndentingNewLine]Print["\<The magnitudes \
for the roots are\>"]; \[IndentingNewLine]Print[
          NumberForm[t, 17]]; \[IndentingNewLine]\ Return[t];]\)], "Input"],

