Cell[TextData[{
  StyleBox["Definition 5.  (Definite Integral as a Limit of a Riemann Sum).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Let  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "   be continuous over the interval  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  ",  and let  P  be a partition of  ",
  Cell[BoxData[
      RowBox[{"[", 
        RowBox[{"a", 
          StyleBox[",",
            FontFamily->"Times New Roman"], "b"}], "]"}]]],
  "  where  ",
  Cell[BoxData[
      \(a = \(\(x\_0 < x\_1 < x\_2 <  ... \) < x\_n = b\)\)]],
  " then  \n\n\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{
          StyleBox[
            SubsuperscriptBox["\[Integral]", 
              StyleBox["a",
                FontSize->10], 
              StyleBox["b",
                FontSize->10]],
            FontSize->18], \(f \((x)\)\ \[DifferentialD]x\)}], " ", "=", 
        " ", \(\(\(\ \)\(lim\)\(\ \)\)\+\(\[LeftDoubleBracketingBar]P\
\[RightDoubleBracketingBar]\  \[Rule] \ 0\)\ \ \(\[Sum]\+\(k = 1\)\%n f[
                x\_k] \[CapitalDelta]x\_k\)\)}]]],
  ".  "
}], "Text",
  CellTags->"Definite Integral as a Limit of a Riemann Sum"],

