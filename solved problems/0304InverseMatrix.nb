Cell[TextData[{
  StyleBox["Definition (",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[ButtonBox["Hilbert Matrix",
    ButtonData:>{
      URL[ "http://mathworld.wolfram.com/HilbertMatrix.html"], None},
    ButtonStyle->"Hyperlink"],
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  StyleBox[").",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  The elements of the Hilbert matrix  ",
  Cell[BoxData[
      SubscriptBox[
        StyleBox["H",
          FontWeight->"Bold"], "n"]]],
  " of order n are  ",
  Cell[BoxData[
      \(h\_\(i, j\)\  = \ 1\/\(i + j - 1\)\)]],
