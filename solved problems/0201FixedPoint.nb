Cell[TextData[{
  StyleBox["Remark 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  "  It is assumed that ",
  Cell[BoxData[
      \(p\_0 \[NotEqual] P\)],
    AspectRatioFixed->False],
  " in statement (ii). \n\n",
  StyleBox["Remark 2.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  "  Because ",
  Cell[BoxData[
      StyleBox["g",
        FontWeight->"Plain"]], "Input"],
