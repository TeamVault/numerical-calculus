}], "Text"],

Cell[BoxData[
    \(\(\(FixedPointIteration[x0_, 
          max_] := \[IndentingNewLine]Module[{}, \[IndentingNewLine]p\_0\  = \
\ N[x0]; \ \[IndentingNewLine]k\  = \ 
            0; \ \[IndentingNewLine]Print["\<  p\>"\_0, "\< = \>", 
            PaddedForm[p\_0, {15, 15}]]; \ \[IndentingNewLine]While[
            k < max, \[IndentingNewLine]\(Module[{}, \[IndentingNewLine]p\_1\ \
 = \ g[p\_0]; \ \[IndentingNewLine]k\  = \ 
                  k + 1; \ \[IndentingNewLine]Print["\<  p\>"\_k, "\< = \>", 
                  PaddedForm[
                    p\_1, {15, 15}]\ ]; \ \[IndentingNewLine]p\_0\  = \ 
                  p\_1;\ ];\)\ ]; \ \[IndentingNewLine]p\  = \ 
            p\_0; \ \[IndentingNewLine]Print["\< \>"]; \ \
\[IndentingNewLine]Print["\<The function is g[x] = \>", 
            g[x]]; \ \[IndentingNewLine]Print["\<  p  = \>", 
            PaddedForm[
              p, {15, 15}]]; \ \[IndentingNewLine]Print["\<g[p] = \>", 
            PaddedForm[g[p], {15, 15}]];\ \ ];\)\(\ \ \)\)\)], "Input",
  AspectRatioFixed->False],

