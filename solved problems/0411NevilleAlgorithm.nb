Cell[BoxData[{
    RowBox[{\(P = 0; \ Remove[P];\), " "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{"NevillePoly", " ", ":=", " ", 
        RowBox[{"Mod", "[", 
          RowBox[{\({}\), ",", "\[IndentingNewLine]", 
            
            RowBox[{\(Clear[i, j, k, l, m, n, P, t]\), ";", " ", 
              "\[IndentingNewLine]", \(P\_i_[t_] := f[x\_i]\), ";", " ", 
              "\[IndentingNewLine]", 
              RowBox[{\(P\_\(i_, j_\)[t_]\), ":=", 
                RowBox[{\(1\/\(x\_j - x\_i\)\), 
                  RowBox[{"Det", "[", 
                    RowBox[{"(", GridBox[{
                          {\(P\_i[t]\), \(x\_i - t\)},
                          {\(P\_j[t]\), \(x\_j - t\)}
                          }], ")"}], "]"}]}]}], ";", " ", 
              "\[IndentingNewLine]", 
              RowBox[{\(P\_\(i_, j_, k_\)[t_]\), ":=", 
                RowBox[{\(1\/\(x\_k - x\_i\)\), 
                  RowBox[{"Det", "[", 
                    RowBox[{"(", GridBox[{
                          {\(P\_\(i, j\)[t]\), \(x\_i - t\)},
                          {\(P\_\(j, k\)[t]\), \(x\_k - t\)}
                          }], ")"}], "]"}]}]}], ";", " ", 
              "\[IndentingNewLine]", 
              RowBox[{\(P\_\(i_, j_, k_, l_\)[t_]\), ":=", 
                RowBox[{\(1\/\(x\_l - x\_i\)\), 
                  RowBox[{"Det", "[", 
                    RowBox[{"(", GridBox[{
                          {\(P\_\(i, j, k\)[t]\), \(x\_i - t\)},
                          {\(P\_\(j, k, l\)[t]\), \(x\_l - t\)}
                          }], ")"}], "]"}]}]}], ";", " ", 
              "\[IndentingNewLine]", 
              RowBox[{\(P\_\(i_, j_, k_, l_, m_\)[t_]\), ":=", 
                RowBox[{\(1\/\(x\_m - x\_i\)\), 
                  RowBox[{"Det", "[", 
                    RowBox[{"(", GridBox[{
                          {\(P\_\(i, j, k, l\)[t]\), \(x\_i - t\)},
                          {\(P\_\(j, k, l, m\)[t]\), \(x\_m - t\)}
                          }], ")"}], "]"}]}]}], ";", " ", 
              "\[IndentingNewLine]", 
              RowBox[{\(P\_\(i_, j_, k_, l_, m_, n_\)[t_]\), ":=", 
                RowBox[{\(1\/\(x\_n - x\_i\)\), 
                  RowBox[{"Det", "[", 
                    RowBox[{"(", GridBox[{
                          {\(P\_\(i, j, k, l, m\)[t]\), \(x\_i - t\)},
                          {\(P\_\(j, k, l, m, n\)[t]\), \(x\_n - t\)}
                          }], ")"}], "]"}]}]}], ";"}]}], " ", "]"}]}], 
      ";"}]}], "Input"],

