Cell[TextData[{
  StyleBox["Theorem (Shifted-Inverse Power Method)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontFamily->"Times New Roman",
    FontWeight->"Bold"],
  StyleBox["Assume that the ",
    FontFamily->"Times New Roman"],
  StyleBox["n\[Times]n",
    FontFamily->"Courier New"],
  StyleBox[" ",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["matrix  ",
    FontFamily->"Times New Roman"],
  StyleBox["A",
    FontFamily->"Courier New",
    FontWeight->"Bold"],
  StyleBox["  ",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontSlant->"Italic"],
  StyleBox["has distinct eigenvalues  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{\(\[Lambda]\_1\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \(\[Lambda]\_2\), 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], "...", 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
          FontWeight->"Plain",
          FontSlant->"Plain",
          FontColor->GrayLevel[0],
          FontVariations->{"Underline"->False,
          "StrikeThrough"->False}], \(\[Lambda]\_n\)}]]],
  StyleBox["  ",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["and consider the eigenvalue ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\[Lambda]\_j\)]],
  StyleBox[". Then a constant  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\[Alpha]\)]],
  "  ",
  StyleBox["can be chosen so that   ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\[Mu]\_1\  = \ 1\/\(\[Lambda]\_j - \[Alpha]\)\)]],
  StyleBox[" ",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["is the dominant eigenvalue of  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      SuperscriptBox[
        RowBox[{"(", 
          RowBox[{
            StyleBox["A",
              FontWeight->"Bold"], "-", 
            StyleBox[
              RowBox[{"\[Alpha]", 
                StyleBox["I",
                  FontWeight->"Bold"]}]]}], ")"}], \(-1\)]]],
  StyleBox[".  Furthermore, if   ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      SubscriptBox[
        StyleBox["X",
          FontWeight->"Bold"], "0"]]],
  StyleBox["  is chosen appropriately, then the  sequences ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"{", 
        RowBox[{
          SubscriptBox[
            StyleBox["X",
              FontWeight->"Bold"], "k"], " ", "=", " ", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{\(x\_1\%\((k)\)\), 
                StyleBox[",",
                  FontFamily->"Times New Roman",
                  FontSize->12,
                  FontWeight->"Plain",
                  FontSlant->"Plain",
                  FontColor->GrayLevel[0],
                  FontVariations->{"Underline"->False,
                  "StrikeThrough"->False}], \(x\_2\%\((k)\)\), 
                StyleBox[",",
                  FontFamily->"Times New Roman",
                  FontSize->12,
                  FontWeight->"Plain",
                  FontSlant->"Plain",
                  FontColor->GrayLevel[0],
                  FontVariations->{"Underline"->False,
                  "StrikeThrough"->False}], "...", 
                StyleBox[",",
                  FontFamily->"Times New Roman",
                  FontSize->12,
                  FontWeight->"Plain",
                  FontSlant->"Plain",
                  FontColor->GrayLevel[0],
                  FontVariations->{"Underline"->False,
                  "StrikeThrough"->False}], \(x\_n\%\((k)\)\)}], ")"}], 
            "T"]}], "}"}]]],
  "  and  ",
  Cell[BoxData[
      \({c\_k}\)]],
  "  ",
  StyleBox["generated recursively by  \n\n\t\t",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        SubscriptBox[
          StyleBox["Y",
            FontWeight->"Bold"], "k"], " ", "=", " ", 
        RowBox[{
          StyleBox[
            SuperscriptBox[
              RowBox[{"(", 
                RowBox[{
                  StyleBox["A",
                    FontWeight->"Bold"], 
                  StyleBox["-",
                    FontWeight->"Plain"], 
                  StyleBox[
                    RowBox[{
                      StyleBox["\[Alpha]",
                        FontWeight->"Plain"], 
                      StyleBox["I",
                        FontWeight->"Bold"]}]]}], ")"}], \(-1\)],
            FontWeight->"Bold"], " ", 
          SubscriptBox[
            StyleBox["X",
              FontWeight->"Bold"], "k"]}]}]]],
  StyleBox["  \n\tand",
    FontFamily->"Times New Roman"],
  StyleBox["\n\t\t",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  Cell[BoxData[
      RowBox[{
        SubscriptBox[
          StyleBox["X",
            FontWeight->"Bold"], \(k + 1\)], " ", "=", " ", 
        RowBox[{\(1\/c\_\(k + 1\)\), 
          SubscriptBox[
            StyleBox["Y",
              FontWeight->"Bold"], "k"]}]}]]],
  StyleBox["  \n\n",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["where  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(c\_\(k + 1\)\  = \ \ x\_j\%\((k)\)\)]],
  StyleBox["  and  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(x\_j\%\((k)\) = \ 
        max\+\(1 \[LessEqual] i \[LessEqual] n\){\(|\)\(x\_i\%\((k)\)\)\(|\)}\
\)]],
  StyleBox[",  will converge to the dominant eigenpair  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\[Mu]\_1\)]],
  StyleBox[",",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      SubscriptBox[
        StyleBox["V",
          FontWeight->"Bold"], "j"]]],
  StyleBox["  of the matrix  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      SuperscriptBox[
        RowBox[{"(", 
          RowBox[{
            StyleBox["A",
              FontWeight->"Bold"], "-", 
            StyleBox[
              RowBox[{"\[Alpha]", 
                StyleBox["I",
                  FontWeight->"Bold"]}]]}], ")"}], \(-1\)]]],
  StyleBox[". ",
    FontFamily->"Times New Roman"],
  StyleBox["  ",
    FontFamily->"Times New Roman",
    FontSlant->"Italic"],
  StyleBox["Finally, the corresponding eigenvalue for the matrix  ",
    FontFamily->"Times New Roman"],
  StyleBox["A",
    FontFamily->"Courier New",
    FontWeight->"Bold"],
  StyleBox["  ",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontSlant->"Italic"],
  StyleBox["is given by the calculation  \n\n\t\t",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\[Lambda]\_j\  = \ 1\/\[Mu]\_1 + \[Alpha]\)]]
}], "Text"],

