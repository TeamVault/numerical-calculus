Cell[TextData[{
  StyleBox["Example 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Fit the curve  ",
  Cell[BoxData[
      \(y = c\ \[ExponentialE]\^\(a\ x\)\)]],
  "  to the data points  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"(", 
          RowBox[{"0.0", 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], " ", "1.5"}], ")"}], 
        StyleBox[",",
          FontFamily->"Times New Roman",
          FontSize->12,
