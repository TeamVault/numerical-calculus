Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`FilledPlot`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(gr1\  = \ 
        FilledPlot[{f[x]}, \ {x, a, 0}, Fills \[Rule] {{{1, Axis}, Red}}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(gr2\  = \ 
        FilledPlot[{f[x]}, \ {x, \ 0, b}, Fills \[Rule] {{{1, Axis}, Green}}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(grf = 
        Plot[f[x], {x, a, b}, 
          Ticks \[Rule] {Range[\(-1\), 1, 0.5], Range[\(-2\), 1, 0.5]}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(lines = 
        Graphics[{Line[{{a, 0}, {a, f[a]}}], 
