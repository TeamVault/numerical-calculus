Cell[TextData[StyleBox["Mathematica Subroutine (Newton's Method for Systems \
in n-Dimensions).",
  FontWeight->"Bold",
  FontColor->RGBColor[0.500008, 0, 0.996109]]], "Text"],

Cell[BoxData[
    RowBox[{
      RowBox[{
        RowBox[{\(NewtonSystem[P0_, \[Epsilon]_, max_]\), ":=", 
          "\[IndentingNewLine]", 
          RowBox[{"Module", "[", 
            
            RowBox[{\({dist = 1, F0, J0, k = 0, P}\), ",", 
              "\[IndentingNewLine]", 
              RowBox[{
                RowBox[{
                  SubscriptBox[
                    StyleBox[
                      OverscriptBox[
                        StyleBox["P",
                          FontWeight->"Bold"], "\[RightVector]"],
                      FontWeight->"Bold"], "0"], " ", "=", " ", "P0"}], ";", 
                " ", "\[IndentingNewLine]", 
                RowBox[{"Print", "[", 
                  RowBox[{"\"\<\!\(\(\*
StyleBox[
OverscriptBox[
StyleBox[\"P\",\nFontWeight->\"Bold\"], \"\[RightVector]\"],\n\
FontWeight->\"Bold\"]\)\_0\) = \>\"", ",", 
                    SubscriptBox[
                      StyleBox[
                        OverscriptBox[
                          StyleBox["P",
                            FontWeight->"Bold"], "\[RightVector]"],
                        FontWeight->"Bold"], "0"]}], "]"}], ";", " ", 
                "\[IndentingNewLine]", 
                RowBox[{"While", "[", " ", 
                  
                  RowBox[{\(And[\ k < max, dist > \[Epsilon]\ ]\), ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{
                      RowBox[{
                        SubscriptBox[
                          StyleBox[
                            OverscriptBox[
                              StyleBox["P",
                                FontWeight->"Bold"], "\[RightVector]"],
                            FontWeight->"Bold"], "1"], " ", "=", " ", 
                        RowBox[{
                          SubscriptBox[
                            StyleBox[
                              OverscriptBox[
                                StyleBox["P",
                                  FontWeight->"Bold"], "\[RightVector]"],
                              FontWeight->"Bold"], "0"], " ", "-", " ", 
                          RowBox[{
                            RowBox[{"Inverse", "[", 
                              RowBox[{"J", "[", 
                                SubscriptBox[
                                  StyleBox[
                                    OverscriptBox[
                                      StyleBox["P",
                                        FontWeight->"Bold"], 
                                      "\[RightVector]"],
                                    FontWeight->"Bold"], "0"], "]"}], "]"}], 
                            ".", 
                            RowBox[{\(F\&\[RightVector]\), "[", 
                              SubscriptBox[
                                StyleBox[
                                  OverscriptBox[
                                    StyleBox["P",
                                      FontWeight->"Bold"], "\[RightVector]"],
                                  FontWeight->"Bold"], "0"], "]"}]}]}]}], ";",
                       " ", "\[IndentingNewLine]", 
                      RowBox[{"Print", "[", 
                        RowBox[{
                          SubscriptBox["\"\<\!\(\*
StyleBox[
OverscriptBox[
StyleBox[\"P\",\nFontWeight->\"Bold\"], \"\[RightVector]\"],\n\
FontWeight->\"Bold\"]\)\>\"", \(k + 1\)], ",", "\"\< = \>\"", ",", 
                          SubscriptBox[
                            StyleBox[
                              OverscriptBox[
                                StyleBox["P",
                                  FontWeight->"Bold"], "\[RightVector]"],
                              FontWeight->"Bold"], "1"]}], "]"}], ";", "  ", 
                      "\[IndentingNewLine]", 
                      RowBox[{"dist", " ", "=", " ", 
                        SqrtBox[
                          RowBox[{
                            RowBox[{"(", 
                              RowBox[{
                                SubscriptBox[
                                  StyleBox[
                                    OverscriptBox[
                                      StyleBox["P",
                                        FontWeight->"Bold"], 
                                      "\[RightVector]"],
                                    FontWeight->"Bold"], "1"], "-", 
                                SubscriptBox[
                                  StyleBox[
                                    OverscriptBox[
                                      StyleBox["P",
                                        FontWeight->"Bold"], 
                                      "\[RightVector]"],
                                    FontWeight->"Bold"], "0"]}], ")"}], ".", 
                            RowBox[{"(", 
                              RowBox[{
                                SubscriptBox[
                                  StyleBox[
                                    OverscriptBox[
                                      StyleBox["P",
                                        FontWeight->"Bold"], 
                                      "\[RightVector]"],
                                    FontWeight->"Bold"], "1"], "-", 
                                SubscriptBox[
                                  StyleBox[
                                    OverscriptBox[
                                      StyleBox["P",
                                        FontWeight->"Bold"], 
                                      "\[RightVector]"],
                                    FontWeight->"Bold"], "0"]}], ")"}]}]]}], 
                      ";", " ", "\[IndentingNewLine]", 
                      RowBox[{
                        SubscriptBox[
                          StyleBox[
                            OverscriptBox[
                              StyleBox["P",
                                FontWeight->"Bold"], "\[RightVector]"],
                            FontWeight->"Bold"], "0"], " ", "=", " ", 
                        SubscriptBox[
                          StyleBox[
                            OverscriptBox[
                              StyleBox["P",
                                FontWeight->"Bold"], "\[RightVector]"],
                            FontWeight->"Bold"], "1"]}], ";", " ", 
                      "\[IndentingNewLine]", \(k\  = \ k + 1\), ";"}]}], "  ",
                   "]"}], ";", " ", "\[IndentingNewLine]", 
                RowBox[{"Return", "[", 
                  SubscriptBox[
                    StyleBox[
                      OverscriptBox[
                        StyleBox["P",
                          FontWeight->"Bold"], "\[RightVector]"],
                      FontWeight->"Bold"], "1"], "]"}], ";"}]}], " ", "]"}]}],
         ";"}], " "}]], "Input",
  AspectRatioFixed->False],

Cell[TextData[{
