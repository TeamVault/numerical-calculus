Cell[TextData[{
  StyleBox["Example 1.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find the Pad\[EAcute] approximation ",
  Cell[BoxData[
      \(\(R\_\(2, 2\)\) \((x)\)\)]],
  "for ",
  Cell[BoxData[
      \(f[x] = \[ExponentialE]\^x\)],
    AspectRatioFixed->True],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "First, set up the equation   ",
  Cell[BoxData[
      \(f \((x)\) \(Q\_m\) \((x)\)\  - \ \(P\_n\) \((x)\)\  = \ \ 0\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[a, f, k, m, n, p, q, R, Q, P, 
        x];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_] = \[ExponentialE]\^x;\)\ \ \), "\n", 
    \(\(F[x_, k_]\  := \ Series[f[x], {x, 0, k}];\)\ \ \ \ \), "\n", 
    \(\(P[x_, k_]\  = \ \[Sum]\+\(i = 0\)\%k p\_i\ x\^i;\)\ \ \), "\n", 
    \(\(Q[x_, k_]\  = \ 
        1\  + \ \[Sum]\+\(i = 1\)\%k q\_i\ x\^i;\)\ \ \), "\n", 
    \(\(n = 2;\)\ \ \), "\n", 
    \(\(m = 2;\)\ \ \), "\n", 
    \(\(vars = 
        Union[Table[p\_i, {i, 0, n}], 
          Table[q\_i, {i, 1, m}]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(P\_n\)(x) = \>\"", P[x, n]\ ];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(Q\_m\)(x) = \>\"", Q[x, m]\ ];\)\ \ \ \), "\n", 
    \(\(Print["\< f(x) = \>", 
        F[x, m + n]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<Form  f(x)\!\(Q\_m\)(x) - \!\(P\_n\)(x) = 0\>\""];\)\ \ \
\), "\n", 
    \(\(Print[
        Expand[F[x, n + m] Q[x, m] - P[x, n]] \[Equal] 0];\)\ \ \)}], "Input",\

  AspectRatioFixed->False],

Cell[TextData[{
  "Second, solve the equation   ",
  Cell[BoxData[
      \(f \((x)\) \(Q\_m\) \((x)\)\  - \ \(P\_n\) \((x)\)\  = \ \ 0\)]],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(L = Expand[F[x, n + m] Q[x, m] - P[x, n]];\)\ \ \), "\n", 
    \(\(coeffs = CoefficientList[L, x];\)\ \ \), "\n", 
    \(\(Eqns = Distribute[coeffs \[Equal] 0, \ List];\)\ \ \), "\n", 
    \(\(Print[TableForm[Eqns]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(solset = Solve[Eqns, vars];\)\ \ \ \), "\[IndentingNewLine]", 
    \(\(R[x_] = 
        Simplify[
          ReplaceAll[P[x, n]\/Q[x, n], 
            solset\_\(\(\[LeftDoubleBracket]\)\(1\)\(\[RightDoubleBracket]\)\)\
]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[
        TableForm[Transpose[solset]]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<R[x] = \>", R[x]\ ];\)\ \)}], "Input",
  AspectRatioFixed->False],

Cell[TextData[{
  "Compare our work with ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s Pade subroutine."
}], "Text"],

Cell[BoxData[{
    \(\(Needs["\<Calculus`Pade`\>"];\)\ \ \), "\n", 
    \(\(Clear[r];\)\), "\[IndentingNewLine]", 
    \(\(r[x_] = 
        ExpandDenominator[
          ExpandNumerator[
            Together[
              Pade[f[x], {x, 0, 2, 
                  2}]\ ]\ ]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<r[x] = \>", r[x]\ ];\)\ \)}], "Input"],

Cell["\<\
Plot graphs of the function and its Pade approximation over the \
interval  [-1,1].  \
\>", "Text"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\), "\[IndentingNewLine]", 
    \(\(Plot[{f[x], r[x]}, {x, \ \(-1\), \ 1}, 
        PlotStyle \[Rule] \ {Red, Blue}];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<r[x] = \>", r[x]\ ];\)\ \)}], "Input"],

Cell["Find the error  over the interval  [-1,1].  ", "Text"],

Cell[BoxData[{
    \(\(Plot[f[x] - r[x], {x, \(-1\), 1}, PlotRange \[Rule] All, 
        PlotStyle \[Rule] \ Green];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x]-r[x] = \>", 
        f[x] - r[x]\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<The maximum error is\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<|f[x]-r[x]| \[LessEqual] \>", 
        Abs[f[1.0] - r[1.0]]\ ];\)\ \)}], "Input"],

