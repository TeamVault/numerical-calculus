Cell[CellGroupData[{

Cell[TextData[StyleBox["Exploration 1.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, c];\)\), "\[IndentingNewLine]", 
    \(\(F[x_, c_] = c\ Cosh[x\/c] - c;\)\ \ \), "\[IndentingNewLine]", 
    \(\(a = 1;\)\ \ \), "\[IndentingNewLine]", 
    \(\(b = 1;\)\ \ \), "\[IndentingNewLine]", 
    \(\(d = 0.618759;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(dot0 = 
        Graphics[{{Blue, PointSize[0.03], 
              Point[{\(-\(a\/2\)\), F[\(-\(a\/2\)\), d\/2]}]}, {Blue, 
              PointSize[0.03], Point[{a\/2, F[a\/2, d\/2]}]}}, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\[IndentingNewLine]", 
    \(\(gr0 = 
        Plot[{F[x, d\/2]}, {x, \(-\(a\/2\)\), a\/2}, PlotStyle \[Rule] Blue, 
          Ticks \[Rule] {Range[\(-\(a\/2\)\), a\/2, 1\/2], 
              Range[0, b\/2, 1\/2]}, 
          DisplayFunction \[Rule] Identity];\)\ \), "\[IndentingNewLine]", 
    \(\(Show[gr0, dot0, 
        DisplayFunction \[Rule] $DisplayFunction];\)\), \
"\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, d\/2]];\)\ \), "\[IndentingNewLine]", 
    \(\(dot1 = 
        Graphics[{{Orange, PointSize[0.03], 
              Point[{\(-a\), F[\(-a\), d]}]}, {Orange, PointSize[0.03], 
              Point[{a, F[a, d]}]}}, 
          DisplayFunction \[Rule] Identity];\)\ \), "\[IndentingNewLine]", 
    \(\(gr1 = 
        Plot[{F[x, d]}, {x, \(-a\), a}, PlotStyle \[Rule] Orange, 
          Ticks \[Rule] {Range[\(-a\), a, 1], Range[0, b, 1]}, 
          DisplayFunction \[Rule] Identity];\)\), "\[IndentingNewLine]", 
    \(\(Show[gr1, dot1, 
        DisplayFunction \[Rule] $DisplayFunction];\)\), \
"\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, d]];\)\ \), "\[IndentingNewLine]", 
    \(\(dot2 = 
        Graphics[{{Green, PointSize[0.03], 
              Point[{\(-2\) a, F[\(-2\) a, 2  d]}]}, {Green, PointSize[0.03], 
              Point[{2  a, F[2  a, 2  d]}]}}, 
          DisplayFunction \[Rule] Identity];\)\), "\[IndentingNewLine]", 
    \(\(gr2 = 
        Plot[{F[x, 2  d]}, {x, \(-2\) a, 2  a}, PlotStyle \[Rule] Green, 
          Ticks \[Rule] {Range[\(-2\) a, 2  a, 1], Range[0, 2  b, 1]}, 
          DisplayFunction \[Rule] Identity];\)\), "\[IndentingNewLine]", 
    \(\(Show[gr2, dot2, 
        DisplayFunction \[Rule] $DisplayFunction];\)\), \
"\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, 2  d]];\)\ \), "\[IndentingNewLine]", 
    \(\(dot3 = 
        Graphics[{{Magenta, PointSize[0.03], 
              Point[{\(-3\) a, F[\(-3\) a, 3  d]}]}, {Magenta, 
              PointSize[0.03], Point[{3  a, F[3  a, 3  d]}]}}, 
          DisplayFunction \[Rule] Identity];\)\), "\[IndentingNewLine]", 
    \(\(gr3 = 
        Plot[{F[x, 3  d]}, {x, \(-3\) a, 3  a}, PlotStyle \[Rule] Magenta, 
          Ticks \[Rule] {Range[\(-3\) a, 3  a, 1], Range[0, 3  b, 1]}, 
          DisplayFunction \[Rule] Identity];\)\), "\[IndentingNewLine]", 
    \(\(Show[gr3, dot3, 
        DisplayFunction \[Rule] $DisplayFunction];\)\), \
"\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, 3  d]];\)\ \)}], "Input"],

Cell["\<\
Do the above four catenaries look similar ?  Are they the same ? 
Plot them on the same graph to see what is happening.  \
\>", "Text"],

Cell[BoxData[{
    \(\(Show[gr0, gr1, gr2, gr3, dot0, dot1, dot2, dot3, 
        PlotRange \[Rule] {{\(-3\), 3}, {0, 3.1}}, 
        Ticks \[Rule] {Range[\(-3\), 3, 1], Range[0, 3, 1]}, 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, d\/2]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, d]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, 2  d]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<y = \>", F[x, 3  d]\ ];\)\ \ \)}], "Input"],

Cell[TextData[{
  "\tHence we see that the \"shape\" of the curve ",
  Cell[BoxData[
      \(y = f \((x)\)\)]],
  " is preserved when it is changed to ",
  Cell[BoxData[
      \(y = c\ f \((x\/c)\)\)]],
  ", indeed the curve is merely magnified by the scale factor ",
  Cell[BoxData[
      \(c\)]],
  " when ",
  Cell[BoxData[
      \(c > 0\)]],
  " or reduced by the scale factor ",
  Cell[BoxData[
      \(c\)]],
  " when  ",
  Cell[BoxData[
      \(c < 0\)]],
  ".  Therefore, in order to \"preserve the shape\",  use the formula  \n\n\t\
",
  Cell[BoxData[
      RowBox[{"y", " ", "=", " ", 
        RowBox[{
          RowBox[{\(c\ cosh \((x\/c)\)\), " ", "-", " ", 
            RowBox[{"c", "  ", 
              StyleBox["where",
                FontFamily->"Times New Roman",
                FontSize->12,
                FontWeight->"Plain",
                FontSlant->"Plain",
                FontColor->GrayLevel[0],
                FontVariations->{"Underline"->False,
                "StrikeThrough"->False}], "  ", "c"}]}], ">", "0"}]}]],
    AspectRatioFixed->False,
    ImageRegion->{{0, 1}, {0, 1}}],
  ",\n\nfor a \"catenary\" which passes through the origin.  "
}], "Text",
  AspectRatioFixed->False,
  ImageRegion->{{0, 1}, {0, 1}}]
}, Closed]],

Cell[TextData[{
  StyleBox["Approximated by a parabola",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\n\tNotice that  ",
  Cell[BoxData[
      \(f \((x)\)\  = \ \ c\ cosh \((x\/c)\)\)],
    AspectRatioFixed->False,
    ImageRegion->{{0, 1}, {0, 1}}],
  "  is an even function. The following computation shows that the first term \
in the Maclaurin series is ",
  Cell[BoxData[
      \(x\^2\/\(2\ c\)\)]],
  ".  For this reason it is often claimed that the shape of a hanging cable \
is \"approximated by a parabola.\"  \n\n\t",
  Cell[BoxData[
      RowBox[{
        InterpretationBox[\("f[x] = "\[InvisibleSpace]\(\(-c\) + 
              c\ Cosh[x\/c]\)\),
          SequenceForm[ "f[x] = ", 
            Plus[ 
              Times[ -1, c], 
              Times[ c, 
                Cosh[ 
                  Times[ 
                    Power[ c, -1], x]]]]],
          Editable->False], " ", "=", 
        " ", \(\[Sum]\+\(n = 1\)\%\[Infinity]\( 
                1\/\(\(\((2  n)\)!\) c\^\(2  n - 1\)\)\) x\^\(2  n\)\)}]]],
  ".\n\n\t",
  Cell[BoxData[
      \(f[
          x]\  = \ \(\(1\/\(\(2!\)\ c\)\) x\^2 + \(1\/\(\(4!\)\ c\^3\)\) 
                  x\^4 + \(1\/\(\(6!\)\ c\^5\)\) 
                  x\^6 + \(1\/\(\(8!\)\ c\^7\)\) 
                  x\^8 + \(1\/\(\(10!\)\ c\^9\)\) 
                  x\^10 +  ... \) + \(1\/\(\(\((2  n)\)!\) 
                    c\^\(2  n - 1\)\)\) x\^\(2  n\)\  + \  ... \)]],
  ".\n\n\t",
  Cell[BoxData[
      \(f[x]\  \[TildeTilde] \ \(1\/\(\(2!\)\ c\)\) x\^2\)]]
}], "Text",
  AspectRatioFixed->False,
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Exploration 2.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[TextData[{
  "Find the Maclaurin series for  ",
  Cell[BoxData[
      \(f \((x)\)\  = \ \ c\ cosh \((x\/c)\)\)],
    AspectRatioFixed->False,
    ImageRegion->{{0, 1}, {0, 1}}],
  ". "
}], "Text"],

Cell[BoxData[{
    \(\(Clear[c, f, x];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_] = c\ Cosh[x\/c] - c;\)\ \), "\[IndentingNewLine]", 
    \(\(s[x_] = Series[f[x], {x, 0, 12}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<s[x] = \>", s[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<The first term in the series is:\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<f[x] \[TildeTilde] \>", 
        Take[Normal[s[x]], 1]];\)\)}], "Input",
  AspectRatioFixed->False,
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[TextData[{
  "The general term in the series is  ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 1\)\%\[Infinity]\( 
              1\/\(\(\((2  n)\)!\) c\^\(2  n - 1\)\)\) x\^\(2  n\)\)]],
  ".  Summing the first six terms in the series, we get   "
}], "Text"],

Cell[BoxData[{
    \(\(s12[
          x_]\  = \ \[Sum]\+\(n = 1\)\%6\( 
                1\/\(\(\((2  n)\)!\) c\^\(2  n - 1\)\)\) 
            x\^\(2  n\);\)\), "\[IndentingNewLine]", 
    \(\(Print["\< f[x] = \>", f[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\< s[x] = \>", s[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(s\_12\)[x] = \>\"", s12[x]];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " is able to sum the infinite series.  "
}], "Text"],

Cell[BoxData[{
    \(\(S[
          x_] = \[Sum]\+\(n = 1\)\%\[Infinity]\( 
                1\/\(\(\((2  n)\)!\) c\^\(2  n - 1\)\)\) 
            x\^\(2  n\);\)\), "\[IndentingNewLine]", 
    \(\(Print["\< f[x] = \>", f[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\< S[x] = \!\(\[Sum]\+\(n = \
1\)\%\[Infinity]\)\!\(1\/\(\(\((2  n)\)!\) c\^\(2  n - 1\)\)\)\!\(x\^\(2  n\)\
\)\>\""];\)\), "\[IndentingNewLine]", 
    \(\(Print["\< S[x] = \>", S[x]];\)\), "\[IndentingNewLine]", 
    \(\(S[x_] = FullSimplify[S[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\< S[x] = \>", S[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\< S[x] = \>", Expand[S[x]]];\)\)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Arc Length",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "\n\n\tThe arc length of the curve  ",
  Cell[BoxData[
      \(y = \(f[x] = c\ Cosh[x\/c] - c\)\)],
    AspectRatioFixed->False,
    ImageRegion->{{0, 1}, {0, 1}}],
  "  is found by using the integrand  ",
  Cell[BoxData[
      \(g[
          x]\  = \ \(\@\(1 + \((\(f'\)[x])\)\^2\)\  = \@\(1 + Sinh[x\/c]\^2\)\
\)\)]],
  ".  The length of the catenary over the interval  [0,a] is given by the \
calculation  \n\n\t",
  Cell[BoxData[
      \(Arc\ Length\  = \ \(\[Integral]\_0\%a\((\@\(1 + \((\(f'\)[x])\)\^2\))\
\) \[DifferentialD]x\  = \ c\ Sinh[a\/c]\)\)]],
  "."
}], "Text",
  AspectRatioFixed->False,
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Exploration 3.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[c, f, x];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_] = c\ Cosh[x\/c] - c;\)\ \), "\[IndentingNewLine]", 
    \(\(g[x_] = \@\(1 + \((\(f'\)[x])\)\^2\);\)\ \), "\[IndentingNewLine]", 
    \(\(G[
          x_]\  = \ \[Integral]\(\@\(1 + \((\(f'\)[x])\)\^2\)\) \
\[DifferentialD]x;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<         f[x] = \>", f[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\@\(1 + \((\(f'\)[x])\)\^2\)\) = \>\"", \@\(1 + \
\((\(f'\)[x])\)\^2\)];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<         g[x] = \>", g[x]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<G[x] = \>", "\<\[Integral] \>", \*"\"\<\!\(\@\(1 + \
\((\(f'\)[x])\)\^2\)\)\>\"", "\< \[DifferentialD]x\>"];\)\), "\
\[IndentingNewLine]", 
    \(\(Print["\<G[x] = \>", "\<\[Integral] \>", \@\(1 + \
\((\(f'\)[x])\)\^2\), "\< \[DifferentialD]x\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<G[x] = \>", G[x]];\)\), "\[IndentingNewLine]", 
    \(\(G[x_]\  = \ Simplify[G[x]];\)\), "\[IndentingNewLine]", 
