Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 37 (b).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[
    \(\(\(\[Sum]\+\(n = 
            0\)\%\[Infinity]\)\(2\^n\/\(n!\)\)\(\ \ \)\)\)], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 37 (c).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

