    FontSlant->"Italic"],
  " and then find the Jacobian matrix",
  StyleBox[" ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        StyleBox["J",
          FontWeight->"Bold"], 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman",
              FontSize->12,
              FontWeight->"Plain",
              FontSlant->"Plain",
              FontColor->GrayLevel[0],
              FontVariations->{"Underline"->False,
              "StrikeThrough"->False}], "y"}], ")"}]}]]],
  ".  "
}], "Text"],

Cell[BoxData[{
    \(\(f\_1[{x_, y_}] = 1 - 4\ x + 2\ x\^2 - 2\ y\^3;\)\ \ \), "\n", 
    \(\(f\_2[{x_, y_}] = \(-4\) + x\^4 + 4\ y + 4\ y\^4;\)\ \ \), "\n", 
    \(\(F\&\[RightVector][{x_, y_}] = {f\_1[{x, y}], 
          f\_2[{x, y}]};\)\ \ \), "\n", 
    \(\(J[{x_, y_}] = 
        Transpose[{\[PartialD]\_x\ F\&\[RightVector][{x, y}], \[PartialD]\_y\ 
              F\&\[RightVector][{x, y}]}];\)\ \ \), "\n", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<0 = \!\(f\_1\)(x,y) = \>\"", 
        f\_1[{x, y}]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<0 = \!\(f\_2\)(x,y) = \>\"", 
        f\_2[{x, y}]];\)\ \ \), "\n", 
    \(\(Print["\<\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<    J(x,y) = \>", MatrixForm[J[{x, y}]]];\)\ \ \)}], "Input",\

  AspectRatioFixed->False],

Cell[TextData[{
