    \(\(dotx = 
        ListPlot[x00, PlotStyle \[Rule] Green, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(doty = 
        ListPlot[y00, PlotStyle \[Rule] Green, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(grf = 
        Plot[f[x], {x, a, b}, PlotStyle \[Rule] Red, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(Show[grf, dot0, dotx, doty, PlotRange \[Rule] {{0, 0.5}, {0, 1.0}}, 
        Prolog \[Rule] {PointSize[0.02]}, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] "\<y = Sin[x+1]\>", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<y = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<  \!\(x\_n\)               \!\(y\_n\)\>\""];\)\ \ \), "\n\
", 
    \(\(Print[TableForm[xy0]\ ];\)\ \ \), "\n", 
    \(\(Print["\<The limiting values are:\>"];\)\ \ \), "\n", 
    \(\(Print[x0, "\<\t \t \t\>", y0];\)\ \ \), "\n", 
    \(\(Print["\<The function f[x] = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<is continuous at x = \>", a];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 8.  (Rolle's Theorem).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
  Cell[BoxData[
      \(f \((x)\)\)]],
  "  is continuous on the (closed and finite) interval  ",
  Cell[BoxData[
      \(\([a, b]\)\)]],
  ",  and that  ",
  Cell[BoxData[
      \(f' \((x)\)\)]],
  "  exists for all  ",
  Cell[BoxData[
      \(a < x < b\)]],
  ".   If  ",
  Cell[BoxData[
      \(f \((a)\) = 0\)]],
  "  and  ",
  Cell[BoxData[
      \(f \((b)\) = 0\)]],
  ",  then there exists a value  ",
  Cell[BoxData[
      \(c\)]],
  ",  with  ",
  Cell[BoxData[
      \(a < c < b\)]],
  ",  such that  ",
  Cell[BoxData[
      \(f' \((c)\) = 0\)]],
  ".  "
}], "Text",
  CellTags->"Rolle's Theorem"],

Cell[TextData[{
  StyleBox["Example16.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  The function  ",
  Cell[BoxData[
      \(f \((x)\) = sin \((x)\) - 1\/2\)]],
  "  has two zeros in  ",
  Cell[BoxData[
      \(\([0, 3]\)\)]],
  ".  According to Rolle's theorem, there exists a value  ",
  Cell[BoxData[
      \(c\)]],
  "  with  ",
  Cell[BoxData[
      \(0 < c < 3\)]],
  "  such that  ",
  Cell[BoxData[
      \(f' \((c)\) = 0\)]],
  ".  Find  ",
  Cell[BoxData[
      \(c\)]],
  ".  "
}], "Text",
  Evaluatable->False],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 16.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell["Find the two zeros of  f(x).", "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(Clear[f, x];\)\ \ \), "\n", 
    \(\(f[x_] = Sin[x] - 1\/2;\)\ \ \), "\n", 
    \(\(solset\  = \ Solve[f[x] \[Equal] 0, x];\)\ \ \), "\n", 
    \(\(Print["\<f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<Solve  f[x] = 0 .\>"];\)\ \ \), "\n", 
    \(\(Print[solset];\)\ \ \), "\n", 
    \(\(Print["\<A  solution is   \>", 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\)];\)\ \ \), "\n", 
    \(\(Print["\<Another one is  \>", \[Pi] - 
          solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\)];\)\ \ \)}], "Input"],

Cell["Or, the two roots can obtained using the inverse function.", "Text",
  Evaluatable->False],

Cell[BoxData[{
    \(\(r1 = ArcSin[1\/2];\)\ \ \), "\n", 
    \(\(r2 = r1 + 2\ \((\[Pi]\/2 - r1)\);\)\ \ \), "\n", 
    \(\(Print[\*"\"\<   \!\(r\_1\) = \>\"", r1];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<f[\!\(r\_1\)] = \>\"", f[r1]];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<   \!\(r\_2\) = \>\"", r2];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<f[\!\(r\_2\)] = \>\"", f[r2]];\)\ \ \)}], "Input"],

Cell["Find the function  f '(x)  and its zeros.", "Text",
  Evaluatable->False],

Cell[BoxData[{
    RowBox[{
      RowBox[{
        RowBox[{"solset", "=", 
          RowBox[{"Solve", "[", 
            RowBox[{
              RowBox[{
                RowBox[{
                  SuperscriptBox["f", "\[Prime]",
                    MultilineFunction->None], "[", "x", "]"}], "\[Equal]", 
                "0"}], ",", "x"}], "]"}]}], ";"}], "  "}], "\n", 
    RowBox[{\(c = 
          solset\_\(\(\[LeftDoubleBracket]\)\(2, 1, \
2\)\(\[RightDoubleBracket]\)\);\), "  "}], "\n", 
    RowBox[{\(yc = f[c];\), "  "}], "\n", 
    RowBox[{\(Print["\<f[x] = \>", f[x]\ ];\), "  "}], "\n", 
    RowBox[{\(Print["\< f'[x] = \>", \(f'\)[x]\ ];\), "  "}], "\n", 
    RowBox[{\(Print["\<Solve f'[c] = 0.\>"];\), "  "}], "\n", 
    RowBox[{\(Print["\<   c  = \>", c];\), "  "}], "\n", 
    RowBox[{\(Print["\< f[c] = \>", yc];\), "  "}], "\n", 
    RowBox[{\(Print["\<f'[c] = \>", \(f'\)[c]\ ];\), "  "}]}], "Input"],

Cell[BoxData[{
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(grf = 
        Plot[f[x], {x, 0, \[Pi]}, PlotStyle \[Rule] Red, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(dot\  = \ 
        Graphics[{Green, PointSize[0.025], 
            Point[{c, f[c]}]}];\)\ \ \), "\n", 
    \(\(lmax = 
        Graphics[{Green, 
            Line[{{c + 1, f[c]}, {c - 1, f[c]}}]}];\)\ \ \), "\n", 
    \(\(Show[grf, lmax, dot, PlotRange \[Rule] {{0, \[Pi]}, {\(-0.5\), 0.5}}, 
        Prolog \[Rule] {PointSize[0.02]}, 
        Ticks \[Rule] {Range[0, \[Pi], \[Pi]\/4], Range[\(-1\), 1, 0.25]}, 
        AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] \*"\"\<y = Sin[x] - \!\(1\/2\)\>\"", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<The value c for Rolle's theorem for the function\>"];\)\ \ \
\), "\n", 
    \(\(Print["\<f(x) = \>", f[x]\ ];\)\ \ \), "\n", 
    \(\(Print["\<c = \>", c];\)\ \ \), "\n", 
    \(\(Print["\<f(c) = f(\>", c, \*"\"\<) = \!\(y\_c\) = \>\"", 
        f[c]\ ];\)\ \ \), "\n", 
    \(\(Print["\<f'(c) = \>", \(f'\)[c]\ ];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Theorem 9.  (Mean Value Theorem for Derivatives).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  Assume that  ",
