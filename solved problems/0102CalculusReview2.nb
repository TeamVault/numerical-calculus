Cell[TextData[{
  StyleBox["Example 35.",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find the Taylor polynomial approximation of degree  ",
  Cell[BoxData[
      \(n = 1\)]],
  "  for  ",
  Cell[BoxData[
      \(f \((x, y)\) = 5  x\^2 + x\^4 - 9\ x\^2\ y + 3\ y\^2 + 2\ y\^4\)]],
  "  expanded about  ",
  Cell[BoxData[
      \(\((x\_0, y\_0)\) = \((1, 1)\)\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 35.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[f, x, y];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_, y_] = 
        5  x\^2 + x\^4 - 9\ x\^2\ y + 3\ y\^2 + 
          2\ y\^4;\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] = \>", f[x, y]];\)\ \ \)}], "Input"],

Cell["Use the formula in Theorem 25. ", "Text"],

Cell[BoxData[{
    RowBox[{\(x\_0 = 1;\), " "}], "\[IndentingNewLine]", 
    RowBox[{\(y\_0 = 1;\), "  "}], "\[IndentingNewLine]", 
    RowBox[{
      RowBox[{
        RowBox[{"S", "=", 
          RowBox[{\(f[x\_0, y\_0]\), "+", 
            RowBox[{\((y - y\_0)\), " ", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((0, 1)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x\_0, y\_0\), "]"}]}], "+",
             
            RowBox[{\(1\/2\), " ", \(\((y - y\_0)\)\^2\), " ", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((0, 2)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x\_0, y\_0\), "]"}]}], "+",
             
            RowBox[{\((x - x\_0)\), " ", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((1, 0)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x\_0, y\_0\), "]"}]}], "+",
             
            RowBox[{\((x - x\_0)\), " ", \((y - y\_0)\), " ", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((1, 1)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x\_0, y\_0\), "]"}]}], "+",
             
            RowBox[{\(1\/2\), " ", \(\((x - x\_0)\)\^2\), " ", 
              RowBox[{
                SuperscriptBox["f", 
                  TagBox[\((2, 0)\),
                    Derivative],
                  MultilineFunction->None], "[", \(x\_0, y\_0\), "]"}]}]}]}], 
        ";"}], "  "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<f[x,y] = \>", f[x, y]\ ];\), 
      "  "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<Taylor polynomial approximation of degree  n = \
2\>"];\), "  "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<f[x,y] \[TildeTilde] \>", S];\), 
      "  "}], "\[IndentingNewLine]", 
    RowBox[{\(Print["\<f[x,y] \[TildeTilde] \>", Expand[S]\ ];\), 
      "  "}]}], "Input"],

Cell[TextData[{
  "Or use ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s procedure ",
  StyleBox["Series",
    FontWeight->"Bold"],
  " with caution.  "
}], "Text"],

Cell[BoxData[{
    \(\(n = 2;\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = 
        Series[f[1 + \[CapitalDelta]x\ \ t, 1 + \[CapitalDelta]y\ t], {t, 0, 
            n}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = 
        ReplaceAll[Normal[S2], t \[Rule] 1];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = Expand[S2];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Clear[s];\)\ \ \), "\[IndentingNewLine]", 
    \(\(s[x_, y_] = 
        ReplaceAll[
          S2, {\[CapitalDelta]x\  \[Rule] x - 1, \[CapitalDelta]y\  \[Rule] 
              y - 1}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] = \>", f[x, y]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Taylor polynomial approximation of degree  n = \>", 
        n\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] \[TildeTilde] \>", 
        s[x, y]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] \[TildeTilde] \>", 
        Expand[s[x, y]]\ ];\)\ \ \)}], "Input"],

Cell[TextData[{
  StyleBox["Observation.",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 1, 0]],
  "  When the given function  ",
  Cell[BoxData[
      \(f[x, y]\)]],
  ", (which is a polynomial of two variables of order n = 4) is expanded in a \
Taylor polynomial of degree n = 4, the result is an equivalent expression  ",
  Cell[BoxData[
      \(f[x, y]\)]],
  "."
}], "Text"],

Cell[BoxData[{
    \(\(n = 4;\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = 
        Series[f[1 + \[CapitalDelta]x\ \ t, 1 + \[CapitalDelta]y\ t], {t, 0, 
            n}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = 
        ReplaceAll[Normal[S2], t \[Rule] 1];\)\ \ \), "\[IndentingNewLine]", 
    \(\(S2 = Expand[S2];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Clear[s];\)\ \ \), "\[IndentingNewLine]", 
    \(\(s[x_, y_] = 
        ReplaceAll[
          S2, {\[CapitalDelta]x\  \[Rule] x - 1, \[CapitalDelta]y\  \[Rule] 
              y - 1}];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] = \>", f[x, y]];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<Taylor polynomial approximation of degree  n = \>", 
        n\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] \[TildeTilde] \>", 
        s[x, y]\ ];\)\ \ \), "\[IndentingNewLine]", 
    \(\(Print["\<f[x,y] \[TildeTilde] \>", 
        Expand[s[x, y]]\ ];\)\ \ \)}], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Definition 6.  (Local Extrema for a Function of Two Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  ",
  StyleBox["Suppose that  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "y"}], ")"}]}]]],
  StyleBox[" is defined in some disk  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(D\)]],
  StyleBox["  with center  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  ".  ",
  StyleBox["  \n\nThe function ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "y"}], ")"}]}]]],
  StyleBox[" has a ",
    FontFamily->"Times New Roman"],
  StyleBox["local maximum",
    FontFamily->"Times New Roman",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" at ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  " provided that \n\n\t\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}], 
        "\[LessEqual]", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]}]]],
  " for all ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], "\[Element]",
         "D"}]]],
  ". \n\n",
  StyleBox["The function ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(f \((x, y)\)\)]],
  StyleBox[" has a ",
    FontFamily->"Times New Roman"],
  StyleBox["local minimum at",
    FontFamily->"Times New Roman",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox[" ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(\((x\_0, y\_0)\)\)]],
  " provided that \n\n\t\t",
  Cell[BoxData[
      RowBox[{
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}], 
        "\[GreaterEqual]", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]}]]],
  " for all ",
  Cell[BoxData[
      RowBox[{
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], ")"}], "\[Element]",
         "D"}]]],
  "."
}], "Text",
  CellTags->"Local Extrema for a Function of Two Variables"],

Cell[TextData[{
  StyleBox["Theorem 26.  (Critical Points for a Function of Two Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  If the function  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{"x", 
            StyleBox[",",
              FontFamily->"Times New Roman"], "y"}], ")"}]}]]],
  StyleBox["  has a either a local minimum or local maximum at  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  and the first-order partial derivatives exist there, then both  \n\n\t\t\
",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  ". \n\nA point  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  such that  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  ",  or one of the partial derivatives does not exist is called a ",
  StyleBox["critical point",
    FontColor->RGBColor[0, 0, 1]],
  ".  "
}], "Text",
  CellTags->"Critical Points for a Function of Two Variables"],

Cell[TextData[{
  StyleBox["Theorem 27.  (Second Derivative Test for a Function of Two \
Variables).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  Suppose  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"f", 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f\_x\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f\_y\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f\_xx\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], \(f\_xy\), 
        StyleBox[",",
          FontFamily->"Times New Roman"], 
        StyleBox[" ",
          FontFamily->"Times New Roman"], 
        RowBox[{
          StyleBox["and",
            FontFamily->"Times New Roman"], 
          StyleBox["  ",
            FontFamily->"Times New Roman"], \(f\_yy\)}]}]]],
  "  are all continuous on a disk ",
  StyleBox[" ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      \(D\)]],
  StyleBox["  with center ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  ".  Assume that  ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  "  is a critical point, i.e.  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  "  and  ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_x\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "=", 
        "0"}]]],
  ".  \n\n",
  StyleBox["(i)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["   If  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        RowBox[{
          RowBox[{\(f\_xx\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \(f\_yy\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "-", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{\(f\_xy\), 
                RowBox[{"(", 
                  RowBox[{\(x\_0\), 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], 
              ")"}], "2"]}], ">", "0"}]]],
  " and ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_xx\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], ">", 
        "0"}]]],
  " then ",
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]]],
  " is a local minimum.  \n\n",
  StyleBox["(ii)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["   If  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        RowBox[{
          RowBox[{\(f\_xx\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \(f\_yy\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "-", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{\(f\_xy\), 
                RowBox[{"(", 
                  RowBox[{\(x\_0\), 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], 
              ")"}], "2"]}], ">", "0"}]]],
  " and ",
  Cell[BoxData[
      RowBox[{
        RowBox[{\(f\_xx\), 
          RowBox[{"(", 
            RowBox[{\(x\_0\), 
              StyleBox[",",
                FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "<", 
        "0"}]]],
  " then ",
  Cell[BoxData[
      RowBox[{"f", 
        RowBox[{"(", 
          RowBox[{\(x\_0\), 
            StyleBox[",",
              FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}]]],
  " is a local maximum.  \n\n",
  StyleBox["(iii)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["   If  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        RowBox[{
          RowBox[{\(f\_xx\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \(f\_yy\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "-", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{\(f\_xy\), 
                RowBox[{"(", 
                  RowBox[{\(x\_0\), 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], 
              ")"}], "2"]}], "<", "0"}]]],
  " then ",
  Cell[BoxData[
      RowBox[{"z", "=", 
        RowBox[{"f", 
          RowBox[{"(", 
            RowBox[{"x", 
              StyleBox[",",
                FontFamily->"Times New Roman"], "y"}], ")"}]}]}]]],
  "has a saddle point at ",
  Cell[BoxData[
      RowBox[{"(", 
        RowBox[{\(x\_0\), 
          StyleBox[",",
            FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]]],
  ".  \n\n",
  StyleBox["(iv)",
    FontFamily->"Times New Roman",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["   If  ",
    FontFamily->"Times New Roman"],
  Cell[BoxData[
      RowBox[{
        RowBox[{
          RowBox[{\(f\_xx\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], 
              ")"}], \(f\_yy\), 
            RowBox[{"(", 
              RowBox[{\(x\_0\), 
                StyleBox[",",
                  FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], "-", 
          SuperscriptBox[
            RowBox[{"(", 
              RowBox[{\(f\_xy\), 
                RowBox[{"(", 
                  RowBox[{\(x\_0\), 
                    StyleBox[",",
                      FontFamily->"Times New Roman"], \(y\_0\)}], ")"}]}], 
              ")"}], "2"]}], "=", "0"}]]],
  " then a mere computation of the second partials is not helpful in \
determining the nature of the critical point. Theory could be developed for \
investigating this situations and it would involve the fourth order partial \
derivatives.  It is awkward to use and for this reason is not used. "
}], "Text",
  CellTags->"Second Derivative Test for a Function of Two Variables"],

Cell[TextData[{
  StyleBox["Definition 7.  (Limit of a Sequence).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  ",
  Cell[BoxData[
      \(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\ x\_n = x\)]],
  "  means that for any real number  ",
  Cell[BoxData[
      \(\[Epsilon] > 0\)]],
  "  there corresponds a positive integer  ",
  Cell[BoxData[
      \(N\_\[Epsilon]\)]],
  "  (which depends on  ",
  Cell[BoxData[
      \(\[Epsilon]\)]],
  ")  such that  \n\n\t",
  Cell[BoxData[
      \(\(\(|\)\(x - x\_n\)\(|\)\(\(<\)\(\[Element]\)\)\)\)]],
  "  whenever  ",
  Cell[BoxData[
      \(n > N\_\[Epsilon]\)]],
  ".  \n\nInformally, the sequence  ",
  Cell[BoxData[
      \({x\_n}\_1\%\[Infinity]\)]],
  "  has  ",
  Cell[BoxData[
      \(x\)]],
  "  as its limit as  ",
  Cell[BoxData[
      \(n\)]],
  "  approaches infinity, provided the terms  ",
  Cell[BoxData[
      \(x\_n\)]],
  "  can be made as close as we please to  ",
  Cell[BoxData[
      \(x\)]],
  "  by making  ",
  Cell[BoxData[
      \(n\)]],
  "  large enough.  When this happens, we write \n\n\t",
  Cell[BoxData[
      \(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\ x\_n = x\)]],
  "  \nor\n\t",
  Cell[BoxData[
      \(\(\(\ \)\(x\_n \[Rule] \(x\ \ \ as\ \ n\  \[Rule] \
\[Infinity]\)\)\)\)]],
  "  \n\nIf  ",
  Cell[BoxData[
      \(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\ x\_n = x\)]],
  ",  we say that the sequence  ",
  Cell[BoxData[
      \({x\_n}\_1\%\[Infinity]\)]],
  "  ",
  StyleBox["converges",
    FontColor->RGBColor[0, 0, 1]],
  " to  x.  "
}], "Text",
  CellTags->"Limit of a Sequence"],

Cell[TextData[{
  StyleBox["Example 36 (a).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\) \(2  n\  + \
\ 3\)\/\(n\^2\  + \ 1\)\)]],
  ",  ",
  StyleBox["(b).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\) \(1\  + \ \
2  n\  + \ 3  n\^3\)\/\(4  n\^3\  + \ 5  n\^2\  + \ 6\)\)]],
  ",  ",
  StyleBox["(c).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\) \(n\^3\  + \
\ 1\)\/\(6  n\^2\  + \ 7  n\^2\  + \ 8\)\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 36 (a).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[n];\)\ \), "\[IndentingNewLine]", 
    \(X\_n_ = \(2  n + 3\)\/\(\(n\^2 + 1\)\(\ \ \)\)\)}], "Input"],

Cell[BoxData[
    \(\(\(Limit[\ X\_n, \ n \[Rule] \[Infinity]]\)\(\ \ \)\)\)], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 36 (b).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[n];\)\ \), "\[IndentingNewLine]", 
    \(X\_n_ = \(1 + 2  n + 3  n\^3\)\/\(\(4  n\^3 + 5  n\^2 + 6\)\(\ \ \
\)\)\)}], "Input"],

Cell[BoxData[
    \(\(\(Limit[\ X\_n, \ n \[Rule] \[Infinity]]\)\(\ \ \)\)\)], "Input"]
}, Closed]],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 36 (c).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[{
    \(\(Clear[n];\)\ \), "\[IndentingNewLine]", 
    \(X\_n_ = \(n\^3 + 1\)\/\(\(6  n\^2 + 7  n\^2 + 8\)\(\ \ \)\)\)}], "Input"],

Cell[BoxData[
    \(Limit[\ X\_n, \ n \[Rule] \[Infinity]]\)], "Input"]
}, Closed]],

Cell[TextData[{
  StyleBox["Definition 8.  (Limit of a Series).",
    FontWeight->"Bold",
    FontColor->RGBColor[0, 0, 1]],
  "  The ",
  StyleBox["infinite series",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontColor->RGBColor[1, 0, 1]],
  Cell[BoxData[
      \(\[Sum]\+\(k = 1\)\%\[Infinity] x\_k\)]],
  StyleBox["  ",
    FontColor->RGBColor[1, 0, 1]],
  StyleBox["converges",
    FontColor->RGBColor[0, 0, 1]],
  " to  S if for any real number  ",
  Cell[BoxData[
      \(\[Epsilon] > 0\)]],
  "  there corresponds a positive integer  ",
  Cell[BoxData[
      \(N\_\[Epsilon]\)]],
  "  (which depends on  ",
  Cell[BoxData[
      \(\[Epsilon]\)]],
  ")  such that  \n\n\t",
  Cell[BoxData[
      \(\(\(|\)\(S - \[Sum]\+\(k = 1\)\%n \
x\_k\)\(|\)\(\(<\)\(\[Element]\)\)\)\)]],
  "  whenever  ",
  Cell[BoxData[
      \(n > N\_\[Epsilon]\)]],
  ".  \n\nInformally, the ",
  StyleBox["infinite series",
    FontColor->RGBColor[0, 0, 1]],
  StyleBox["  ",
    FontColor->RGBColor[1, 0, 1]],
  Cell[BoxData[
      \(S = \[Sum]\+\(k = 1\)\%\[Infinity] x\_k\)]],
  StyleBox["  ",
    FontColor->RGBColor[1, 0, 1]],
  StyleBox["converges",
    FontColor->RGBColor[0, 0, 1]],
  " if the sequence  ",
  Cell[BoxData[
      \({S\_n}\_1\%\[Infinity]\)]],
  "  of partial sums,  ",
  Cell[BoxData[
      \(S\_n = \[Sum]\+\(k = 1\)\%n x\_k\)]],
  ",  has  ",
  Cell[BoxData[
      \(S\)]],
  "  as",
  " its limit.",
  "\n\t\n\tIf  ",
  Cell[BoxData[
      \(S\_n = \[Sum]\+\(k = 1\)\%n x\_k\)]],
  ",  then  ",
  Cell[BoxData[
      \(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\ S\_n\  = \ 
        S\)]],
  ", \nor  \n\t",
  Cell[BoxData[
      \(\(\(\(\ \)\(lim\)\(\ \)\)\+\(n\  \[Rule] \ \[Infinity]\)\) \(\[Sum]\+\
\(k = 1\)\%n x\_k\) = \ \[Sum]\+\(k = 1\)\%\[Infinity] x\_k\)]]
}], "Text",
  CellTags->"Limit of a Series"],

Cell[TextData[{
  StyleBox["Example 37 (a).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 1\)\%\[Infinity]\((\(-1\))\)\^\(n + 1\)\/n\)]],
  ",  ",
  StyleBox["(b).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 0\)\%\[Infinity] 2\^n\/\(n!\)\)]],
  ",  ",
  StyleBox["(c).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Find  ",
  Cell[BoxData[
      \(\[Sum]\+\(n = 0\)\%\[Infinity]\(\((\(-1\))\)\^n\ \ 2\^\(2  n + \
1\)\)\/\(\((2  n + 1)\)!\)\)]],
  ".  "
}], "Text"],

Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 37 (a).",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell[BoxData[
    \(\(\(\[Sum]\+\(n = 
            1\)\%\[Infinity]\)\(\((\(-1\))\)\^\(n + 1\)\/n\)\(\ \ \)\)\)], \
"Input"]
