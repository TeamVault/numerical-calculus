                6}]]; \ \[IndentingNewLine]Print["\<{f[a],f[b],f[c],f[d]} = \
\>", PaddedForm[{Ya, Yc, Yd, Yb}, {13, 12}]]; \ \[IndentingNewLine]While[\ 
            k \[LessEqual] max, \[IndentingNewLine]If[\ 
              Yc < Yd, \[IndentingNewLine]Module[{}, \[IndentingNewLine]b\  = \
\ d; \ \[IndentingNewLine]Yb\  = \ Yd; \ \[IndentingNewLine]d\  = \ 
                  c; \ \[IndentingNewLine]Yd\  = \ 
                  Yc; \ \[IndentingNewLine]c\  = \ 
                  a + \((1 - F\_\(n - 1 - k\)\/F\_\(n - k\))\) \((b - 
                          a)\); \ \[IndentingNewLine]Yc\  = \ 
                  f[c];\ ], \[IndentingNewLine]\(Module[{}, \
\[IndentingNewLine]a\  = \ c; \ \[IndentingNewLine]Ya\  = \ 
                    Yc; \ \[IndentingNewLine]c\  = \ 
                    d; \ \[IndentingNewLine]Yc\  = \ 
                    Yd; \ \[IndentingNewLine]d\  = \ 
                    a\  + \ \(F\_\(n - 1 - k\)\/F\_\(n - k\)\) \((b - 
                            a)\); \ \[IndentingNewLine]Yd\  = \ 
                    f[d];\ \ ];\)\ ]; \ \[IndentingNewLine]\ 
            pointsab = {{Red, PointSize[0.02], Point[{a, f[a]}], 
                  Point[{b, f[b]}]}, {Blue, PointSize[0.02], 
                  Point[{c, f[c]}], 
                  Point[{d, f[d]}]}}; \ \ \[IndentingNewLine]dotsab = 
              Graphics[pointsab]; \ \ \[IndentingNewLine]gr0 = 
              Plot[f[x], {x, a0, b0}, 
                PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 0.75}}, 
                PlotStyle \[Rule] Magenta, 
                DisplayFunction \[Rule] Identity]; \ \[IndentingNewLine]gr1 = 
              FilledPlot[f[x], {x, a, b}, 
                PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 0.72}}, 
                PlotStyle \[Rule] Red, 
                Fills \[Rule] {RGBColor[1, 0.8, 0.85]}, Curves \[Rule] Front, 
                DisplayFunction \[Rule] Identity]; \[IndentingNewLine]If[
              b - a > 0.1, \[IndentingNewLine]Show[gr0, gr1, dotsab, 
                PlotRange \[Rule] {{a0 - 0.05, b0 + 0.05}, {0, 0.72}}, 
                Ticks \[Rule] {Range[0, 2, 0.1], Range[0, 2, 0.1]}, 
                DisplayFunction \[Rule] $DisplayFunction], \
\[IndentingNewLine]\(Show[gr0, gr1, dotsab, 
