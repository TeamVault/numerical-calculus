Cell[TextData[StyleBox["Solution 4.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell["Now investigate the \"recursive path\" the subroutine takes.", "Text"],

Cell[BoxData[{
    \(\(f[
          x_] = \((14  x - 
              11  x\^2)\) \[ExponentialE]\^\(\(-2\) x\);\)\ \), "\
\[IndentingNewLine]", 
    \(\(a = 0.0;\)\ \), "\[IndentingNewLine]", 
    \(\(b = 2.0;\)\ \), "\[IndentingNewLine]", 
    \(\(X = {a, b};\)\ \), "\n", 
    \(\(val\  = Adapt[a, b, 0.002];\)\), "\[IndentingNewLine]", 
    \(\(X = Sort[X];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\[Integral]\>"\_a\^b, f[x], "\<\[DifferentialD]x = \>", 
        val];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<The abscissas {\!\(x\_k\)} for the intervals [\!\(x\_\(k \
- 1\)\),\!\(x\_k\)] where Simpson Rule\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[\*"\"\<\!\(\[Integral]\_\(x\_\(k - 1\)\)\%\(x\_k\)\)f[x]\
\[DifferentialD]x\>\"", "\< \[TildeTilde] \>", \*"\"\<\!\(\(x\_k - x\_\(k - 1\
\)\)\/6\)(f[\!\(x\_\(k - 1\)\)]+4f[\!\(\(x\_\(k - 1\) + \
x\_k\)\/2\)]+f[\!\(x\_k\)])\>\""];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<was applied were\>"];\)\ \), "\[IndentingNewLine]", 
