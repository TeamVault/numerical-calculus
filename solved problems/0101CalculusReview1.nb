    \(\(Print[\*"\"\<\!\(lim\+\(x \[Rule] X\_0\)\) \>\"", \*"\"\<\!\(\(f[x] - \
f[X\_0]\)\/\(x - X\_0\)\) = \>\"", lim];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<f'[\!\(X\_0\)] = \>\"", lim];\)\ \ \)}], "Input"],

Cell["Using the second definition we obtain:  ", "Text"],

Cell[BoxData[{
    \(\(Clear[\[CapitalDelta]x, f, x];\)\ \ \), "\[IndentingNewLine]", 
    \(\(f[x_] = \ x\^3 - x\^2 - x + 2\ ;\)\ \ \), "\n", 
    \(\(q = \(f[x + \[CapitalDelta]x] - f[x]\)\/\[CapitalDelta]x\ ;\)\ \ \), \
"\n", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\), "\n", 
    \(\(Print[\*"\"\<\!\(\(f[x + \[CapitalDelta]x] - f[x]\)\/\(x + \
\[CapitalDelta]x - x\)\) = \>\"", q];\)\ \ \), "\n", 
    \(\(q = Expand[q];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(\(f[x + \[CapitalDelta]x] - f[x]\)\/\[CapitalDelta]x\
\) = \>\"", q];\)\ \ \), "\n", 
    \(\(lim = Limit[q, \[CapitalDelta]x \[Rule] 0];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<\!\(lim\+\(\[CapitalDelta]x \[Rule] 0\)\) \>\"", \*"\"\<\
\!\(\(f[x + \[CapitalDelta]x] - f[x]\)\/\[CapitalDelta]x\) = \>\"", 
        lim];\)\ \ \), "\n", 
    \(\(Print["\<f'[x] = \>", lim];\)\ \ \)}], "Input"],

Cell[BoxData[{
    \(\(df[x_] = \(f'\)[x];\)\ \ \), "\n", 
    \(\(a = 0;\)\ \ \), "\n", 
    \(\(c = 1.2;\)\ \ \), "\n", 
    \(\(b = 2;\)\ \ \), "\n", 
    \(\(ln0 = {};\)\ \ \), "\n", 
    \(\(pt0 = {};\)\ \ \), "\n", 
    \(\(Needs["\<Graphics`Colors`\>"];\)\ \ \), "\[IndentingNewLine]", 
    \(\(For[j = 1, 
        j \[LessEqual] 6, \(j++\), \[IndentingNewLine]x0 = 
          N[c]; \ \ \[IndentingNewLine]y0 = 
          N[f[x0]]; \ \ \[IndentingNewLine]x1 = 
          N[c + 1\/4\^j]; \ \ \[IndentingNewLine]y1 = 
          N[f[x1]]; \ \ \[IndentingNewLine]dx = 
          x1 - x0; \ \ \[IndentingNewLine]dy = 
          y1 - y0; \ \ \[IndentingNewLine]m = 
          dy\/dx; \ \ \[IndentingNewLine]ln0 = 
          Append[ln0, 
            Graphics[{Hue[1 - \(j - 1\)\/7], 
                Line[{{x0, y0}, {b, 
                      y0 + \((b - c)\)\ m}}]}]]; \ \ \[IndentingNewLine]pt0 = 
          Append[pt0, {dx, dy, dy\/dx}];\ ];\)\ \ \), "\n", 
    \(\(grf = 
        Plot[f[x], {x, a, b}, PlotStyle \[Rule] Green, 
          DisplayFunction \[Rule] Identity];\)\ \ \), "\n", 
    \(\(Show[grf, ln0, PlotRange \[Rule] {{a, b}, {0, 5}}, 
        AspectRatio \[Rule] 1, AxesLabel \[Rule] {x, y}, \n\t
        PlotLabel \[Rule] "\<y = f[x]\>", 
        DisplayFunction \[Rule] $DisplayFunction];\)\ \ \), "\n", 
    \(\(Print["\<Table of values for the slope.\>"];\)\ \ \), "\n", 
    \(\(Print[\*"\"\<  \[CapitalDelta]x               \[CapitalDelta]y        \
       \!\(\[CapitalDelta]y\/\[CapitalDelta]x\)\>\""];\)\ \ \), "\n", 
    \(\(Print[TableForm[pt0]];\)\ \ \), "\n", 
    \(\(Print["\<The  function     f[x] = \>", f[x]];\)\ \ \), "\n", 
    \(\(Print["\<The derivative   f'[x] = \>", \(f'\)[x]];\)\ \ \), "\n", 
    \(\(Print["\<The derivative f'[1.2] = \>", \(f'\)[
          1.2]];\)\ \ \)}], "Input"]
