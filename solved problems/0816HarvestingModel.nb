Cell[CellGroupData[{

Cell[TextData[StyleBox["Solution 7.",
  FontWeight->"Bold",
  FontColor->RGBColor[1, 0, 1]]], "Text"],

Cell["\<\
First, verify that the formula for the general solution is correct.\
\>", "Text"],

Cell[BoxData[{
    \(\(Clear[a, b, t, x];\)\), "\[IndentingNewLine]", 
    \(\(de = \(x'\)[t] \[Equal] 
          2\ x[t] - \ x[t]\^2 - 1;\)\), "\[IndentingNewLine]", 
    \(solset\  = MapAll[Simplify, DSolve[de, x[t], t]]\)}], "Input"],

Cell[BoxData[{
    \(sol = 
      Together[ReplaceAll[
          a\/\(2\ b\) + \(1\/b\) 1\/\(t - 4\ b\ c\), {a \[Rule] 2, 
            b \[Rule] 1, 
            k \[Rule] a\^2/\((4\ b)\)}]]\), "\[IndentingNewLine]", 
    \(sol = ReplaceAll[sol, c \[Rule] c/4]\)}], "Input"],

Cell[BoxData[{
    \(\(Print["\<The general solution to\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<is\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<x[t] = \>", 
        solset\_\(\(\[LeftDoubleBracket]\)\(1, 1, \
2\)\(\[RightDoubleBracket]\)\)];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<x[t] = \>", sol];\)\)}], "Input"],

Cell[TextData[{
  "First, verify that the formula for the solution with the initial condition \
 ",
  Cell[BoxData[
      \(x \((0)\) = x\_0\)]],
  "  is correct. "
}], "Text"],

Cell[BoxData[{
    \(\(ic = x[0] \[Equal] 2;\)\), "\[IndentingNewLine]", 
    \(solset = DSolve[{de, ic}, x[t], t]\)}], "Input"],

Cell[BoxData[
    \(sol = 
      Together[ReplaceAll[
          a\/\(2\ b\) + \(1\/b\) 
              1\/\(t - 2/\((a - 2\ b\ x\_0)\)\), {a \[Rule] 2, b \[Rule] 1, 
            k \[Rule] a\^2/\((4\ b)\), x\_0 \[Rule] 2}]]\)], "Input"],

Cell[BoxData[{
    \(\(Print["\<The solution to\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[de];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<with the initial condition\>"];\)\), "\[IndentingNewLine]", 
    \(\(Print[ic];\)\), "\[IndentingNewLine]", 
