              RowBox[{\(Clear[h, i, j, n, f, s, x]\), ";", "  ", 
                "\[IndentingNewLine]", \(s[h_] = 
                  Normal[Series[f[x\_0 + h], {h, 0, k + 1}]]\), ";", "  ", 
                "\[IndentingNewLine]", \(eqns = 
                  Table[s[j\ h] \[Equal] f[x\_0 + \ j\ h], {j, 0, k}]\), ";", 
                "    ", "\[IndentingNewLine]", 
                RowBox[{"vars", " ", "=", " ", 
                  RowBox[{"Table", "[", 
                    RowBox[{
                      RowBox[{
                        SuperscriptBox["f", 
                          TagBox[\((k)\),
                            Derivative],
                          MultilineFunction->None], "[", \(x\_0\), "]"}], 
                      ",", \({k, 1, k}\)}], "]"}]}], ";", "  ", 
                "\[IndentingNewLine]", \(pts\  = \ 
                  Table[{x\_0, f[x\_0 + \ j\ h]}, {j, 0, k}]\), ";", "  ", 
                "\[IndentingNewLine]", \(solset = Solve[eqns, vars]\), ";", 
