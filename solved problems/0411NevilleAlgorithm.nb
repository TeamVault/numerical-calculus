Cell[TextData[{
  StyleBox["6 (a).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Compute the values for Neville's tableau. "
}], "Text"],

Cell[BoxData[{
    \(\(NevilleInterpolation[X, x\&^\ ];\)\), "\[IndentingNewLine]", 
    \(\(Needs["\<LinearAlgebra`MatrixManipulation`\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Ntable = 
        Table["\<\>", {i, 1, 6}, {j, 1, 6}];\)\), "\[IndentingNewLine]", 
    \(\(For[i = 1, 
        i \[LessEqual] 
          6, \(i++\), \[IndentingNewLine]\(For[j = 1, 
            j \[LessEqual] 
              i, \(j++\), \ \(Ntable\_\(\(\[LeftDoubleBracket]\)\(i, j\)\(\
\[RightDoubleBracket]\)\)\  = \ 
                p\_\(i - 1, j - 1\);\)\ ];\)\ ];\)\), "\[IndentingNewLine]", 
    \(\(sub = Table[{i}, {i, 1, 6}];\)\ \), "\[IndentingNewLine]", 
    \(\(Ntable = 
        AppendRows[sub, 
          AppendRows[Partition[X, 1], Ntable]];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<Neville's tableau is\>"];\)\ \), "\[IndentingNewLine]", 
    \(\(Print[
        TableForm[Ntable, 
          TableHeadings \[Rule] {None, {"\<i\>", \*"\"\< \!\(x\_i\)\>\"", \*"\
\"\<\!\(p\_\(i, 0\)\)=f[\!\(x\_i\)]\>\"", \*"\"\<\!\(p\_\(i, 1\)\)\>\"", \
\*"\"\<\!\(p\_\(i, 2\)\)\>\"", \*"\"\<\!\(p\_\(i, 3\)\)\>\"", \
\*"\"\<\!\(p\_\(i, 4\)\)\>\"", \*"\"\<\!\(p\_\(i, 5\)\)\>\""}}]];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<f[x] = \>", f[x]\ ];\)\), "\[IndentingNewLine]", 
    \(\(Print["\<We have the Neville approximation\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<f[\>", 
        x\&^, "\<] \[TildeTilde] \>", "\<p\>"\_\(5, 5\), "\< = \>", 
        p\_\(5, 5\)\ ];\)\ \), "\[IndentingNewLine]", 
    \(\(Print["\<It is close to the true value\>"];\)\ \), "\
\[IndentingNewLine]", 
    \(\(Print["\<f[\>", x\&^, "\<] = \>", f[x\&^]\ ];\)\)}], "Input"],

Cell[TextData[{
  StyleBox["6 (b).",
    FontWeight->"Bold",
    FontColor->RGBColor[1, 0, 0]],
  "  Compute the values for Aitken's tableau. "
}], "Text"],

Cell[BoxData[{
    \(\(AitkenInterpolation[X, x\&^\ ];\)\), "\n", 
